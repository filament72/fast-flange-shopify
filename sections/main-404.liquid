
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
<link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
  assign sid = section.id
  assign stt_layout = section.settings.layout
  assign stt_image_bg = section.settings.image_bg

  if stt_layout == 't4s-se-container' 
    assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
  elsif stt_layout == 't4s-container-wrap'
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif
  
  assign general_block = false
  
-%}
{% render 'section_style', sid: sid %}
<div class="t4s-section-inner t4s_se_{{ sid }} {{ stt_layout }} {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%}  t4s-has-imgbg lazyloadt4s {%- endif -%}"  {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {% endif %}>
  {{- html_layout[0] -}}
    {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner {% if stt_image_bg != blank %} t4s-has-imgbg lazyloadt4s {% endif %} "  {% if stt_image_bg != blank %}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {% endif %} > {%- endif -%} 
    <div class="error-404-wrap t4s-text-lg-{{ section.settings.content_align }} t4s-text-md-{{ section.settings.content_align_tb }} t4s-text-{{ section.settings.content_align_mb }} t4s-empty__page">
    	<div class="t4s-container">
        {% for block in section.blocks %}
          {%- case block.type -%}
            {%- when 'custom_text' -%}
              {% if block.settings.text != blank %}
                {%- assign general_block = true -%}
                <div data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-text-bl t4s-fnt-fm-{{ block.settings.fontf }} t4s-font-italic-{{ block.settings.font_italic }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-hidden-br-mb-{{ block.settings.remove_br_tag }} t4s-text-shadow-{{ block.settings.text_shadow }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_text', block: block -%}>{{ block.settings.text }}</div>
              {% endif %}
            {%- when 'space_html' -%}
              {%- assign general_block = true -%}
              <div class="t4s-space-html t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" {%- render 'bk_cus_style', type: 'space_html', block: block -%}></div>
            {%- when 'html' -%}
              {% if block.settings.html_content != blank %}
                {%- assign general_block = true -%}
                  <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-raw-html t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-rte--list" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'html', block: block -%}>{{ block.settings.html_content }}</div>
                {% endif %}
            {%- when 'search_form' -%}
              {%- liquid 
                assign search_terms = search.terms
                assign key_terms = search_terms
                if search_terms contains 'product_type:'
                  assign arr_terms = search_terms | remove: 'product_type:' | replace_first: ' AND', ' AND ' | replace_first: '  ', ' ' | split: ' AND '
                  assign product_type = arr_terms[0] | strip
                  assign search_terms = arr_terms[1] | strip
                  if search_terms == blank
                    assign key_terms = key_terms | remove: ' AND '
                    assign search_terms = product_type
                  endif
                endif 
              -%}
              <div class="t4s-404-search t4s-404-search-style{{ block.settings.style }}" style="--input-cl: {{ block.settings.input_cl }};--btn-cl: {{ block.settings.btn_cl }};--btn-bg-cl: {{ block.settings.btn_bg_cl }};--btn-hover-cl: {{ block.settings.btn_hover_cl }};--btn-bg-hover-cl: {{ block.settings.btn_bg_hover_cl }};--border-color: {{ block.settings.border_cl }};">
                <form action="{{ routes.search_url }}" method="get" role="search" class="t4s-search-form t4s-search-page-form js_frm_search_none">
                  <div class="t4s-search-form-inner">
                    <div class="input-group__field input-group__field--connected t4s-search-form__input-wrapper row no-gutters tl">
                         <input type="hidden" name="type" value="product">
                         <input type="hidden" name="options[unavailable_products]" value="{{ settings.unavailable_prs }}">
                         <input type="hidden" name="options[prefix]" value="last">
                         <div class="t4s-pr t4s-input-group"> 
                           <input type="text" name="q" value="{{ search_terms }}" placeholder="{{ 'search.general.placeholder' | t }}" class="t4s-search-form__input t4s-input-type js_iput_search col" />
                           <div class="t4s-line t4s-pa"></div>
                         </div>
                        <button type="reset" class="t4s-search-form__clear-action"><i class="las la-times"></i></button>
                    </div>
                    <button type="submit" class="t4s-search-form-submit js_btn_search">
                      <span class="t4s-search-submit-text">{{ 'search.general.submit' | t }}</span>
                        <svg class="t4s-icon t4s-icon--search" version="1.0" xmlns="http://www.w3.org/2000/svg"
                         width="300.000000pt" height="300.000000pt" viewBox="0 0 300.000000 300.000000"
                         preserveAspectRatio="xMidYMid meet">
                        <g transform="translate(0.000000,300.000000) scale(0.050000,-0.050000)" stroke="none">
                        <path d="M5680 5953 c-50 -26 -328 -171 -620 -323 -3536 -1841 -4978 -2600
                        -5015 -2642 -52 -58 -57 -126 -16 -207 36 -68 -59 -30 882 -351 l781 -266 249
                        213 c2014 1725 3003 2566 3008 2560 3 -4 -416 -515 -933 -1137 -1595 -1921
                        -1564 -1882 -1536 -1900 42 -26 2612 -900 2646 -900 53 0 125 40 153 86 24 37
                        720 4629 721 4751 0 135 -167 196 -320 116z"/><path d="M2184 968 l6 -612 56 -48 c63 -53 184 -65 238 -23 37 29 721 957 712
                        965 -14 14 -963 330 -989 330 -24 0 -28 -89 -23 -612z"/></g></svg>
                    </button>
                  </div>
                </form>
              </div>
            {%- when 'image' -%}
              {%- liquid 
                assign image = block.settings.image_child
                if block.settings.img_link != blank
                  assign image_parent = 'a,' | split: ','
                else
                  assign image_parent = 'div,data-' | split: ','
                endif 
              -%}
              {%- if image != blank -%}
                {%- assign general_block = true -%}
                <{{ image_parent[0] }} {{ image_parent[1] }}href="{{ block.settings.img_link }}" class="t4s-bl-item t4s-img-child t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', block: block -%}>
                    <img class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                </{{- image_parent[0] -}}>
              {%- else -%}
                {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
              {%- endif -%}
            {%- when 'custom_button' -%}
              {%- if block.settings.button_link != blank and block.settings.button_text != blank -%}
                {%- assign custom_button = true -%}
                {%- assign  button_style = block.settings.button_style -%}
                <a href="{{ block.settings.button_link }}" target="{{ block.settings.target_link }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-btn t4s-btn-custom  t4s-pe-auto t4s-fnt-fm-{{ block.settings.fontf }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' %}t4s-btn-effect-{{ block.settings.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_button', block: block -%}>{{ block.settings.button_text }}
                {%- if block.settings.button_icon_w > 0 -%}
                  {%- if block.settings.icon_style == "1" -%}
                    {% comment %} <svg class="t4s-btn-icon"  width="16" height="11" viewBox="0 0 16 11" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                      <rect y="4" width="12" height="3" />
                      <path d="M15.0434 5.50985L11.3245 9.44864L11.2626 1.63039L15.0434 5.50985Z"/>
                    </svg> {% endcomment %}
                    <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="18" height="18"><path d="M 21.1875 9.28125 L 19.78125 10.71875 L 24.0625 15 L 4 15 L 4 17 L 24.0625 17 L 19.78125 21.28125 L 21.1875 22.71875 L 27.90625 16 Z"/></svg>
                  {%- elsif block.settings.icon_style == "2" -%}
                      <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>   
                  {% else %}
                    <svg class="t4s-btn-icon" version="1.1" x="0px" y="0px" width="22px" height="22px" viewBox="0 0 22 22" style="enable-background:new 0 0 22 22;" xml:space="preserve"><polygon points="6,20 6,2 17,11 "></polygon></svg>  
                  {%- endif -%}
              {%- endif -%}
               </a>
              {%- endif -%}
          {%- endcase -%}
        {% endfor %}
    	</div>
    </div>
  {{- html_layout[1] -}}
</div>
{%- if general_block or custom_button -%}
  {{ 't4s-general-block.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- if custom_button -%}
  {{ 't4s-button-style.css' | asset_url | stylesheet_tag }}
{%- endif -%}
<style>
  .error-404-wrap.t4s-empty__page {
    margin: 0;
    min-height: 200px;
  }
  .error-404-wrap p a {
    pointer-events: auto;
    color: var(--link-text-cl);
  }
  .error-404-wrap p a:hover {
    text-decoration: underline;
  }
  .error-404-wrap a strong {
    font-weight: 500;
  }
  .error-404-wrap.t4s-text-lg-center .t4s-404-search .t4s-search-form .t4s-search-form-inner {
    justify-content: center;
    margin: 0 auto;
  }
  .t4s-404-search {
    pointer-events: auto;
  }
  .t4s-404-search .t4s-search-form .t4s-search-form-inner {
    display: flex;
    width: 100%;
    max-width: 580px;
    background-color: #f9f9f9;
  }
  .t4s-404-search .t4s-search-form .t4s-search-form-inner .input-group__field {
    width: 100%;
    position: relative;
  }
  .t4s-404-search .t4s-search-form .t4s-mini-search__categories {
    display: none;
  }
  .t4s-404-search .t4s-search-form input {
    font-size: 16px;
    font-weight: 300;
    padding: 0 30px;
    width: 100%;
    height: 60px;
    line-height: 58px;
    border: solid 1px var(--border-color);
    border-inline-end: none;
    background-color: #f9f9f9;
  }
  .t4s-404-search .t4s-search-form .t4s-search-form__clear-action {
    position: absolute;
    z-index: 1;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto 0;
    background-color: transparent;
    color: currentcolor;
    opacity: 0;
    visibility: hidden;
  }
  .t4s-404-search .t4s-search-form .t4s-search-form-submit {
    width: 160px;
    height: 60px;
    text-align: center;
    padding: 0;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    text-transform: uppercase;
    font-weight: 500;
    font-size: 14px;
    letter-spacing: 2.38px;
    color: var(--btn-cl);
    background-color: var(--btn-bg-cl);
  }
  .t4s-404-search .t4s-search-form .t4s-search-form-submit:hover {
    color: var(--btn-hover-cl);
    background-color: var(--btn-bg-hover-cl);
  }
  .t4s-404-search .t4s-search-form .t4s-search-form-submit  svg {
    color: currentcolor;
    display: none;
    width: 22px;
    height: 22px;
    fill: currentcolor;
  }
  .t4s-404-search.t4s-404-search-style2 .t4s-search-form .t4s-search-form-inner,
  .t4s-404-search.t4s-404-search-style3 .t4s-search-form .t4s-search-form-inner {
    background-color: #fff;
    border: solid 1px var(--border-color);
  }
  .t4s-404-search.t4s-404-search-style2 .t4s-search-form input,
  .t4s-404-search.t4s-404-search-style3 .t4s-search-form input {
    border: none;
    background-color: transparent;
    color: var(--input-cl);
  }
  .t4s-404-search.t4s-404-search-style2 .t4s-search-form .t4s-search-form-submit,
  .t4s-404-search.t4s-404-search-style3 .t4s-search-form .t4s-search-form-submit {
    background-color: transparent;
    width: 80px;
    height: 58px;
  }
  .t4s-404-search.t4s-404-search-style2 .t4s-search-form input,
  .t4s-404-search.t4s-404-search-style3 .t4s-search-form input,
  .t4s-404-search.t4s-404-search-style2 .t4s-search-form .t4s-search-form-submit,
  .t4s-404-search.t4s-404-search-style3 .t4s-search-form .t4s-search-form-submit { 
    height: 58px;
  }
  .t4s-404-search.t4s-404-search-style2 .t4s-search-form .t4s-search-form-submit .t4s-search-submit-text,
  .t4s-404-search.t4s-404-search-style3 .t4s-search-form .t4s-search-form-submit .t4s-search-submit-text {
    display: none;
  }
  .t4s-404-search.t4s-404-search-style2 .t4s-search-form .t4s-search-form-submit svg,
  .t4s-404-search.t4s-404-search-style3 .t4s-search-form .t4s-search-form-submit svg {
    display: inline-flex;
  }
  .t4s-404-search.t4s-404-search-style3 .t4s-search-form .t4s-search-form-inner {
    border-radius: 30px;
    overflow: hidden;
  }
  @media(max-width: 1024px) and (min-width: 768px) {
    .error-404-wrap.t4s-text-md-center .t4s-404-search .t4s-search-form .t4s-search-form-inner {
      justify-content: center;
      margin: 0 auto;
    }
  }
  @media(max-width: 767px) {
    .error-404-wrap.t4s-text-center .t4s-404-search .t4s-search-form .t4s-search-form-inner {
      justify-content: center;
      margin: 0 auto;
    }
  }
</style>
{% schema %}
{
  "name": "404",
  "tag": "section",
  "class": "t4s-section t4s-section-main error-404 not-found t4s-pr t4s-oh",
  "settings": [
    {
      "type": "header",
      "content": "1. General options"
    },
    {
      "type": "select",
      "id": "content_align",
      "label": "Content align",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "select",
      "id": "content_align_tb",
      "label": "Content align (Tablet)",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "select",
      "id": "content_align_mb",
      "label": "Content align (Mobile)",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "header",
      "content": "2. Design options"
    },
    {
      "type": "select",
      "id": "layout",
      "default": "t4s-container-wrap",
      "label": "Layout",
      "options": [
        {
          "value": "t4s-se-container",
          "label": "Container"
        },
        {
          "value": "t4s-container-wrap",
          "label": "Wrapped container"
        },
        {
          "value": "t4s-container-fluid",
          "label": "Full width"
        }
      ]
    },
    {
      "type": "color",
      "id": "cl_bg",
      "label": "Background"
    },
    {
      "type": "color_background",
      "id": "cl_bg_gradient",
      "label": "Background gradient"
    },
    {
      "type": "image_picker",
      "id": "image_bg",
      "label": "Background image"
    },
    {
      "type": "text",
      "id": "mg",
      "label": "Margin",
      "info": "Margin top, margin right, margin bottom, margin left. If you do not use it please blank.",
      "default": ",,50px,",
      "placeholder": ",,50px,"
    },
    {
      "type": "text",
      "id": "pd",
      "label": "Padding",
      "info": "Padding top, padding right, padding bottom, padding left. If you do not use it please blank.",
      "placeholder": "50px,,50px,"
    },
    {
      "type": "header",
      "content": "+ Design Tablet Options"
    },
    {
      "type": "text",
      "id": "mg_tb",
      "label": "Margin",
      "default": ",,50px,",
      "placeholder": ",,50px,"
    },
    {
      "type": "text",
      "id": "pd_tb",
      "label": "Padding",
      "placeholder": ",,50px,"
    },
    {
      "type": "header",
      "content": "+ Design mobile options"
    },
    {
      "type": "text",
      "id": "mg_mb",
      "label": "Margin",
      "default": ",,30px,",
      "placeholder": ",,50px,"
    },
    {
      "type": "text",
      "id": "pd_mb",
      "label": "Padding",
      "placeholder": ",,50px,"
    }
  ],
  "blocks": [
    {
      "type": "custom_text",
      "name": "Text",
      "settings": [
        {
          "type": "richtext",
          "id": "text",
          "label": "Input text",
          "info": "If you want to line break, please add a <br> tag in the text"
        },
        {
          "type": "select",
          "id": "fontf",
          "default": "inherit",
          "label": "Font family",
          "options": [
            {
              "label": "Inherit",
              "value": "inherit"
            },
            {
              "label": "Font family #1",
              "value": "1"
            },
            {
              "label": "Font family #2",
              "value": "2"
            },
            {
              "label": "Font family #3",
              "value": "3"
            },
            {
              "label": "Font family #4 (Custom)",
              "value": "4"
            },
            {
              "label": "Font family #5 (Custom)",
              "value": "5"
            }
          ]
        },
        {
          "type": "color",
          "id": "text_cl",
          "label": "Color text",
          "default": "#fff"
        },
        {
          "type": "color",
          "id": "link_cl",
          "label": "Color Link",
          "default": "#C91F28"
        },
        {
          "type": "range",
          "id": "text_fs",
          "label": "Font size",
          "max": 100,
          "min": 10,
          "step": 1,
          "unit": "px",
          "default": 16
        },
        {
          "type": "range",
          "id": "text_lh",
          "label": "Line height",
          "max": 100,
          "min": 0,
          "step": 1,
          "default": 0,
          "unit": "px",
          "info": "Set '0' to use default"
        },
        {
          "type": "range",
          "id": "text_fw",
          "label": "Font weight",
          "min": 100,
          "max": 900,
          "step": 100,
          "default": 400
        },
        {
          "type": "range",
          "id": "text_ls",
          "label": "Letter spacing",
          "max": 5,
          "min": -5,
          "default": 0,
          "step": 0.1,
          "unit": "px"
        },
        {
          "type": "checkbox",
          "id": "font_italic",
          "label": "Enable font italic style",
          "default": false
        },
        {
          "type": "checkbox",
          "id": "text_shadow",
          "label": "Enable text shadow",
          "default": false
        },
        {
          "type": "number",
          "id": "mgb",
          "label": "Margin bottom",
          "default": 15
        },
        {
          "type": "header",
          "content": "+ Option on tablet"
        },
        {
          "type": "range",
          "id": "text_fs_tb",
          "label": "Font size (Tablet)",
          "max": 60,
          "min": 10,
          "step": 1,
          "unit": "px",
          "default": 16
        },
        {
          "type": "range",
          "id": "text_lh_tb",
          "label": "Line height (Tablet)",
          "max": 70,
          "min": 0,
          "step": 1,
          "default": 0,
          "unit": "px",
          "info": "Set '0' to use default"
        },
        {
          "type": "range",
          "id": "text_ls_tb",
          "label": "Letter spacing (Tablet)",
          "min": -5,
          "max": 5,
          "step": 0.1,
          "unit": "px",
          "default": 0
        },
        {
          "type": "number",
          "id": "mgb_tb",
          "label": "Margin bottom (Tablet)",
          "default": 10
        },
        {
          "type": "header",
          "content": "+ Option on mobile"
        },
        {
          "type": "checkbox",
          "id": "hidden_mobile",
          "label": "Hidden on mobile ",
          "default": false
