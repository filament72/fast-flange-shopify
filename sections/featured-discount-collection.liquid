                  {% if slider_dots %} t4s-dots-style-{{ section.settings.dot_owl }} t4s-slider-dots-{{ show_dots }} t4s-base-dots-{{ section.settings.dots_pos }} t4s-dots-cl-{{ section.settings.dots_cl }} t4s-dots-round-{{ section.settings.dots_round }} {% endif %} t4s-flickity-slider flickityt4s" data-flickityt4s-js='{"customIcon":{{ icon_slider }}, "viewBox":"{{ view_box }}", "setPrevNextButtons": true,"arrowIcon": "{{ arrow_icon }}","imagesLoaded": 0,"adaptiveHeight": 1, "contain": 1, "groupCells": "100%", "dragThreshold" : 5, "cellAlign": "left","wrapAround": {{ section.settings.loop }},"prevNextButtons": {{ slider_btns }},"percentPosition": 1,"pageDots": {{ slider_dots }}, "autoPlay" : {{ section.settings.au_time | times: 1000 }}, "pauseAutoPlayOnHover" : {{ section.settings.au_hover }}, "t4sid": "{{ sid }}","navUI": 1,"cellSelector": ".t4s-product"}' style="--btn-distance: {{ section.settings.btn_distance }}px;--space-dots: {{ section.settings.dots_space }}px;--dots-bottom-pos: {{ section.settings.dots_bottom_pos }}px;">
                {%- endif -%}
                    {%- if product_list != blank -%}
                      {%- for product in product_list -%}
                        {%- render 'pr-discount-item1', product: product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: show_vendor, use_link_vendor: use_link_vendor, enable_rating: enable_rating, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, imgatt: imgatt, placeholder_img: placeholder_img, pr_countdown: "" -%}
                      {%- endfor -%}
                    {%- else -%}
                      {%- for i in (1..18) limit: limit -%}
                        <div class="t4s-col-item t4s-product t4s-pr-style{{ product_des }}">
                          <div class="t4s-product-inner t4s-pr">
                            <div class="t4s-product-img t4s-pr t4s-oh" data-cacl-slide >
                              <a class="t4s-d-block" data-cacl-slide href="/admin/products">{%- capture current -%}{%- cycle 1, 2, 3, 4, 5, 6 -%}{%- endcapture -%} 
                              {{ 'product-' | append: current | placeholder_svg_tag: 't4s-placeholder-svg' }}</a>
                            </div>
                            <div class="t4s-product-info">
                              <div class="t4s-product-info__inner">
                                <h3 class="t4s-product-title"><a href="/admin/products">{{ 'onboarding.product_title' | t }}</a></h3>
                                <span class="t4s-product-price"><ins>$39.00</ins><del>$59.00</del></span>
                              </div>
                            </div>
                          </div>
                        </div>
                      {%- endfor -%}
                    {%- endif -%}
                  </div>
              </div>
            </div>
          {%- when 'box_layout2' -%}
            {%- capture pr_countdown_html -%}
              <div class="t4s-top-head-countdown" style="--cd-primary-cl: {{ block.settings.cd_primary_color }};--cd-secondary-cl: {{ block.settings.cd_secondary_color }};" timeline hdt-reveal="slide-in">
                {% if block.settings.cd_title != blank %}
                  <h4 class="t4s-countdown-title">{{ block.settings.cd_title }}</h4>
                {% endif %}
                <div class="t4s-countdown-time" data-countdown-t4s data-date='{{ block.settings.date }}'>
                  <span class="countdown-days">
                    <span class="cd_timet4 t4s-cd-number">%-D</span>
                  </span>
                  <span class="t4s-line-separate">:</span>
                  <span class="countdown-hours">
                    <span class="cd_timet4 t4s-cd-number">%H</span>
                  </span>
                  <span class="t4s-line-separate">:</span>
                  <span class="countdown-min">
                    <span class="cd_timet4 t4s-cd-number">%M</span> 
                  </span>
                  <span class="t4s-line-separate">:</span>
                  <span class="countdown-sec">
                    <span class="cd_timet4 t4s-cd-number">%S</span> 
                  </span>
                </div>
              </div>
            {%- endcapture -%}
            {%- liquid
              assign date_countdown = block.settings.date
              if date_countdown != blank
                assign pr_countdown = pr_countdown_html | replace_first: 'date_data_t4s', date_countdown
              endif -%}

            <div class="t4s-discount-collection-layout2" style="--st-primary-cl: {{ block.settings.cl_stock_1 }};--st-secondary-cl: {{ block.settings.cl_stock_2 }};--st-text-cl: {{ block.settings.cl_stock_3 }};">
              <div class="t4s-top-heading">
                {%- if section.settings.top_heading != blank -%}
                  <h3 class="t4s-section-heading t4s-title"><span >{{ section.settings.top_heading | replace: "[", "<span class='t4s-heading-highlight'>" | replace: "]", "</span>" }}</span></h3>
                {%- endif -%}
                {{ pr_countdown }}
                {%- if block.settings.head_btn_url != blank and block.settings.head_btn_label != blank -%}
                  {{ 't4s-button-style.css' | asset_url | stylesheet_tag }}
                  <div class="t4s-head-btn t4s-text-end">
                    <a class="t4s-btn t4s-btn-base t4s-btn-style-{{ block.settings.head_button_style }} t4s-btn-size-{{ block.settings.head_btns_size }} t4s-btn-icon-{{ block.settings.head_btn_icon }} t4s-btn-color-{{ block.settings.head_btns_cl }} t4s-btn-rounded-{{ block.settings.btn_rounded }} {% if block.settings.head_button_style == 'default' or block.settings.head_button_style == 'outline' %}t4s-btn-effect-{{ block.settings.head_button_effect }}{% endif %}" href="{{ block.settings.head_btn_url }}">{{ block.settings.head_btn_label }}
                      {% if block.settings.head_btn_icon %}
                        <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512" width="22" height="22"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>
                      {% endif %}
                    </a>
                  </div>
                {%- endif -%}
              </div>
              {%- if layout_des == "1" -%} 
