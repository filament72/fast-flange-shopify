                            </div>
                          </div>
                        </div>
                      </div>
                    {%- endfor -%}
                  {%- endif -%}
                </div>
            </div>
          {%- when 'box_layout3' -%}
            {%- render 'section_tophead', get_height: true -%}
            <div class="t4s-row t4s-discount-collection-layout3 t4s-gx-0 t4s-gy-0" style="--st-primary-cl: {{ block.settings.cl_stock_1 }};--st-secondary-cl: {{ block.settings.cl_stock_2 }};--st-text-cl: {{ block.settings.cl_stock_3 }};">
              {% if block.settings.date != blank %}
                <div class="t4s-col-item t4s-col-lg-20 t4s-col-md-4 t4s-col-12 t4s-col-countdown">
                  <div class="t4s-top-head-countdown" style="--cd-primary-cl: {{ block.settings.cd_primary_color }};--cd-secondary-cl: {{ block.settings.cd_secondary_color }};" timeline hdt-reveal="slide-in">
