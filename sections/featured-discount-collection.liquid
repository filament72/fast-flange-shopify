<!-- sections/featured-discount-collection.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-collection-products.css' | asset_url | stylesheet_tag }}
{{ 't4s-products-discount.css' | asset_url | stylesheet_tag }}
{{ 't4s-slider-settings.css' | asset_url | stylesheet_tag }}
{{ 'pre_flickityt4s.min.css' | asset_url | stylesheet_tag }}
{%- liquid
  assign sid = section.id
  assign stt_layout = section.settings.layout
  assign stt_image_bg = section.settings.image_bg -%}

{%- liquid
  if stt_layout == 't4s-se-container' 
   assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
  elsif stt_layout == 't4s-container-wrap'
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif

  assign layout_des = section.settings.layout_des
  assign image_ratio = section.settings.image_ratio
  if image_ratio == "ratioadapt"
    assign imgatt = ''
   else 
    assign imgatt = 'data-'
  endif
  assign collection = collections[section.settings.collection]
  assign use_pagination = section.settings.use_pagination 
  assign show_vendor = section.settings.show_vendor
  assign use_link_vendor = settings.use_link_vendor
  assign enable_rating = settings.enable_rating
  assign limit = section.settings.limit
  assign product_des = section.settings.product_des

  if section.settings.btn_owl == "simple"
    assign arrow_icon = 1
  else
    assign arrow_icon = 2
  endif
  assign show_btn = section.settings.show_btn
  if show_btn != "hidden"
    assign slider_btns = true
  else
    assign slider_btns = false
  endif
  assign show_dots = section.settings.show_dots
  if show_dots != "hidden"
    assign slider_dots = true
  else
    assign slider_dots = false
  endif

  assign show_img = settings.show_img
  assign isGrowaveWishlist = false
  if settings.wishlist_mode == "3" and shop.customer_accounts_enabled
    assign isGrowaveWishlist = true
  endif
  assign placeholder_img = settings.placeholder_img
  assign enable_pr_size = settings.enable_pr_size
  assign show_size_type = settings.show_size_type
  assign size_ck = settings.size_ck | append: ',size,sizes,Größe' 
  assign get_size = size_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

  assign enable_pr_color = settings.enable_pr_color
  assign show_cl_type = settings.show_color_type
  assign color_ck = settings.color_ck | append: ',color,colors,couleur,colour'
  assign get_color = color_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

  assign price_varies_style = settings.price_varies_style
  assign app_review = settings.app_review
  
  if layout_des != "3"
    if use_pagination == "load-more"
      assign typeAjax = 'LmDefault'
    else
      assign typeAjax = 'AjaxDefault'
    endif
  else
     if use_pagination == "load-more"
      assign typeAjax = 'LmIsotope'
    else
      assign typeAjax = 'AjaxIsotope'
    endif
  endif
  assign product_list = section.settings.product_list

  -%}
                  
{% render 'section_style', sid: sid %}
<div data-not-main data-ntajax-options='{"id": "{{ sid }}", "type": "{{ typeAjax }}", "isProduct": true, "view": ""}' class="t4s-section-inner t4s_se_{{ sid }} {{ stt_layout }} {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%} t4s-has-imgbg lazyloadt4s {%- endif -%}"  {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%}  >
  {{- html_layout[0] -}}
  {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner {%- if stt_image_bg != blank -%} t4s-has-imgbg lazyloadt4s {%- endif -%} "  {%- if stt_image_bg != blank -%}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%} > {%- endif -%}
    {%- if section.blocks.size > 0 -%}
      {%- for block in section.blocks -%}
        {%- liquid
          assign block_type = block.type 
          assign featured_product = block.settings.featured_product
        -%}
        {%- case block_type -%}
          {%- when 'box_layout1' -%}
            {%- capture pr_countdown_html -%}
              <div class="t4s-pr-countdown" style="--cd-primary-cl: {{ block.settings.cd_primary_color }};--cd-secondary-cl: {{ block.settings.cd_secondary_color }};" hdt-reveal="slide-in">
                <div class="t4s-countdown-time" data-countdown-t4s data-date='date_data_t4s'>
                  <span class="countdown-days">
                    <span class="cd_timet4 t4s-cd-number">%-D</span>
                    <span class="cd_txtt4 t4s-cd-text">{{ "products.product_card.countdown_text.day_plural" | t }} </span>
                  </span>
                  <span class="t4s-line-separate">:</span>
                  <span class="countdown-hours">
                    <span class="cd_timet4 t4s-cd-number">%H</span>
                    <span class="cd_txtt4 t4s-cd-text">{{ "products.product_card.countdown_text.hr_plural" | t }} </span>
                  </span>
                  <span class="t4s-line-separate">:</span>
                  <span class="countdown-min">
                    <span class="cd_timet4 t4s-cd-number">%M</span> 
                    <span class="cd_txtt4 t4s-cd-text">{{ "products.product_card.countdown_text.min_plural" | t }} </span>
                  </span>
                  <span class="t4s-line-separate">:</span>
                  <span class="countdown-sec">
                    <span class="cd_timet4 t4s-cd-number">%S</span> 
                    <span class="cd_txtt4 t4s-cd-text">{{ "products.product_card.countdown_text.sec_plural" | t }} </span>
                  </span>
                </div>
              </div>
            {%- endcapture -%}
            {%- liquid
              assign date_countdown = block.settings.date | default: featured_product.metafields.theme.countdown
              if date_countdown != blank
                assign pr_countdown = pr_countdown_html | replace_first: 'date_data_t4s', date_countdown
              else
                assign pr_countdown = ''
              endif
            -%}
            {%- render 'section_tophead', get_height: true -%}
            <div class="t4s-row t4s-discount-collection-layout1 t4s-gx-0 t4s-gy-0">
              {% if featured_product != blank %}
                <div class="t4s-col-item t4s-col-lg-3 t4s-col-md-4 t4s-col-12 t4s-col-featured-products t4s_{{ block.settings.image_ratio }} t4s_position_{{ block.settings.image_position }} t4s_{{ block.settings.image_size }}">
                  {%- render 'pr-discount-item1', product: featured_product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: show_vendor, use_link_vendor: use_link_vendor, enable_rating: enable_rating, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, imgatt: imgatt, placeholder_img: placeholder_img, pr_countdown: pr_countdown -%}
                </div>
              {% endif %}
              <div class="t4s-col-item {% if featured_product != blank %} t4s-col-lg-9 t4s-col-md-8 {% else %} t4s-col-12 {% endif %}">
