<!-- sections/custom-section.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
{{ 't4s-hero.css' | asset_url | stylesheet_tag }}
{% comment %} {{ 't4s-custom-video.css' | asset_url | stylesheet_tag }} {% endcomment %}
<link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
  assign sid = section.id
  assign stt_layout = section.settings.layout
  assign stt_image_bg = section.settings.image_bg
  if stt_layout == 't4s-se-container' 
    assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
  elsif stt_layout == 't4s-container-wrap'
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif
  assign index = 0 
  assign index2 = 1
  assign arr_item = section.blocks | where: "type", 'bl_col'
  assign root_url = routes.root_url
  if root_url != '/'
    assign root_url = root_url | append: '/'
  endif
  assign countdown = false
  assign custom_button = false
  assign general_block = false
-%}
{%- capture append_style -%}
  {% if stt_image_bg.presentation.focal_point != '50.0% 50.0%' %}--background-position: {{ stt_image_bg.presentation.focal_point }};{% endif %}
{%- endcapture -%}
{% render 'section_style', sid: sid, append_style: append_style %}
<div class="t4s-section-inner t4s-pr t4s_se_{{ sid }} {{ stt_layout }} {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%}  t4s-has-imgbg lazyloadt4s {%- endif -%}"  {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto" data-optimumx="1.5" {% endif %}>
    {{- html_layout[0] -}}
    {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner {% if stt_image_bg != blank %} t4s-has-imgbg lazyloadt4s {% endif %} "  {% if stt_image_bg != blank %}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {% endif %} > {%- endif -%} 
      <div class="t4s-row t4s-gx-lg-{{ section.settings.space_h_item }} t4s-gy-lg-{{ section.settings.space_v_item }} t4s-gx-md-{{ section.settings.space_h_item_tb }} t4s-gy-md-{{ section.settings.space_v_item_tb }} t4s-gx-{{ section.settings.space_h_item_mb }} t4s-gy-{{ section.settings.space_v_item_mb }}"> 
      {%- for block in arr_item offset: index -%}
        {%- liquid 
          assign index = index | plus: 1
          assign pd_item = block.settings.padding_inner | remove: ' ' | split: ','
          assign pd_item_tb = block.settings.padding_inner_tb | remove: ' ' | split: ','
          assign pd_item_mb = block.settings.padding_inner_mb | remove: ' ' | split: ','
          assign ani_delay = 0
          assign percent_delay = block.settings.animation_delay | divided_by: 100.0
          assign time_ani_delay = block.settings.time_animation | times: percent_delay 
        -%}
            <div class="t4s-custom-col t4s-col-item t4s-col-lg-{{ block.settings.col_dk }} t4s-col-md-{{ block.settings.col_tb }} t4s-col-{{ block.settings.col_mb }} bl-{{ block.id }}" {% if block.settings.col_dk == 'custom' or block.settings.col_tb == 'custom' %} style="--t4s-cus-col-lg:{{ block.settings.col_dk_w | remove: ' ' }}%;--t4s-cus-col-md:{{ block.settings.col_dk_w_tb | remove: ' ' }}%;"{% endif %}>
              <div class="t4s-col-inner-wrap">
                <div class="t4s-col-inner t4s-ver-center-{{ block.settings.align_vertical }} t4s-bordered-cl-{{ block.settings.border_col }} t4s-text-lg-{{ block.settings.content_align }} t4s-text-md-{{ block.settings.content_align_tablet }} t4s-text-{{ block.settings.content_align_mobile }} {% if block.settings.bg_image != blank -%}  lazyloadt4s t4s-has-imgbg {%- endif -%}"  {%- if block.settings.bg_image != blank -%}  data-bgset="{{ block.settings.bg_image | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%}  style="--pd-top: {{ pd_item[0] | default: 0 }};--pd-right: {{ pd_item[1] | default: 0 }};--pd-bottom: {{ pd_item[2] | default: 0 }};--pd-left: {{ pd_item[3] | default: 0 }};--pd-tb-top: {{ pd_item_tb[0] | default: 0 }};--pd-tb-right: {{ pd_item_tb[1] | default: 0 }};--pd-tb-bottom: {{ pd_item_tb[2] | default: 0 }};--pd-tb-left: {{ pd_item_tb[3] | default: 0 }};--pd-mb-top: {{ pd_item_mb[0] | default: 0 }};--pd-mb-right: {{ pd_item_mb[1] | default: 0 }};--pd-mb-bottom: {{ pd_item_mb[2] | default: 0 }};--pd-mb-left: {{ pd_item_mb[3] | default: 0 }}; --bg-cl:  {{ block.settings.bg_cl }}">
                  <div class="t4s-w-100" timeline hdt-reveal="slide-in"> 
                    {%- for block in section.blocks offset: index2 -%}
                      {%- liquid
                        assign index2 = index2 | plus: 1
                        if block.type != "bl_col" 
                        else 
                          break  
                        endif -%}  
                        {%- case block.type -%}
                          {%- when 'video' -%}
                            {%- capture append_bg_content_style -%}--content-pd: {{ block.settings.content_pd_tb }}px {{ block.settings.content_pd_lr }}px;--content-pd-mb: {{ block.settings.content_pd_tb_mb }}px {{ block.settings.content_pd_lr_mb }}px;{%- endcapture -%}
                            {%-liquid
                              assign image_mb = block.settings.image_mb
                              assign image = block.settings.image | default: image_mb
                              assign mb_image = image_mb | default: image 
                              assign url = block.settings.link_img1 
                              assign open_link = block.settings.open_link
                              assign bg_opacity = block.settings.bg_opacity | divided_by: 100.0 
                              assign bg_overlay = block.settings.bg_overlay | color_modify: 'alpha', bg_opacity 
                              assign bg_opacity_play = block.settings.bg_opacity_bt_pl | divided_by: 100.0 
                              assign bg_overlay_play = block.settings.button_bg | color_modify: 'alpha', bg_opacity_play 
                            -%}
                            <div class="t4s-video-popup-inline t4s-bg-video t4s-pr t4s-video t4s_ratio_mix {{ block.settings.se_height }} t4s_ratio_cuspx_mb_{{ block.settings.custom_mb }} t4s_ratio_cuspx_tb_{{ block.settings.custom_tb }} t4s_ratio_cuspx_{{ block.settings.custom_dk }}" data-video-poster>
                              <div data-video-insert class="t4s-hero-inner t4s-pr t4s-oh t4s_cover t4s_ratio t4s_position_{{ block.settings.image_position1 }}" style="--bg_overlay_play: {{ bg_overlay_play }}; --ratioapt: {{ image.aspect_ratio | default: 1.7777 }};--ratioaptmb: {{ mb_image.aspect_ratio | default: 1.7777 }}; --height_dk: {{ block.settings.height_dk }}px; --height_tb: {{ block.settings.height_tb }}px; --height_mb: {{ block.settings.height_mb }}px; "> 
                                  {%- if image != blank -%}    
                                  {%- liquid
                                      assign img_url_w     = image | image_url: width: 1904 | split: '1904'
                                      assign img_url_w0    = img_url_w[0]
                                      assign img_url_w1    = img_url_w[1]
                                      assign img_url_w_mb  = mb_image | image_url: width: 1904 | split: '1904'
                                      assign img_url_w_mb0 = img_url_w_mb[0]
                                      assign img_url_w_mb1 = img_url_w_mb[1]  
                                  -%}
                                      <img class="t4s-img-as-bg t4s-d-md-none t4s-parallax-img"
                                      srcset="{{ img_url_w_mb0 }}375{{ img_url_w_mb1 }} 375w, {{ img_url_w_mb0 }}550{{ img_url_w_mb1 }} 550w, {{ img_url_w_mb0 }}750{{ img_url_w_mb1 }} 750w, {{ img_url_w_mb0 }}1100{{ img_url_w_mb1 }} 1100w, {{ img_url_w_mb0 }}1500{{ img_url_w_mb1 }} 1500w, {{ img_url_w_mb0 }}1780{{ img_url_w_mb1 }} 1780w, {{ img_url_w_mb0 }}2000{{ img_url_w_mb1 }} 2000w, {{ img_url_w_mb0 }}3000{{ img_url_w_mb1 }} 3000w,{{ img_url_w_mb0 }}3840{{ img_url_w_mb1 }} 3840w, {{ mb_image | image_url }} {{ mb_image.width }}w"
                                      sizes="100vw" src="{{ img_url_w_mb0 }}1500{{ img_url_w_mb1 }}"
                                      loading="lazy" alt="{{ mb_image.alt | escape }}" width="{{ mb_image.width }}" height="{{ mb_image.height }}">
                                      <img class="t4s-img-as-bg t4s-d-none t4s-d-md-block t4s-parallax-img"
                                      srcset="{{ img_url_w0 }}375{{ img_url_w1 }} 375w, {{ img_url_w0 }}550{{ img_url_w1 }} 550w, {{ img_url_w0 }}750{{ img_url_w1 }} 750w, {{ img_url_w0 }}1100{{ img_url_w1 }} 1100w, {{ img_url_w0 }}1500{{ img_url_w1 }} 1500w, {{ img_url_w0 }}1780{{ img_url_w1 }} 1780w, {{ img_url_w0 }}2000{{ img_url_w1 }} 2000w, {{ img_url_w0 }}3000{{ img_url_w1 }} 3000w,{{ img_url_w0 }}3840{{ img_url_w1 }} 3840w, {{ image | image_url }} {{ image.width }}w"
                                      sizes="100vw" src="{{ img_url_w0 }}1500{{ img_url_w1 }}"
                                      loading="lazy" alt="{{ image.alt | escape }}" width="{{ image.width }}" height="{{ image.height }}">
                                  {%- else -%}
                                      {{ 'lifestyle-1' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1' }} 
                                  {%- endif -%}
                                  <div class="t4s-content-wrap t4s-bg-cl-wrap t4s-pe-none t4s-full-width-link t4s-z-100">
                                      <div class=" t4s-pa t4s-button-video t4s-text-lg-{{ block.settings.text_align }} t4s-text-md-{{ block.settings.text_align_tb }} t4s-text-{{ block.settings.text_align_mb }} t4s-bg-content-true " style="--time-animation: {{ section.settings.time_animation }}s;{%- render 'content_position', ch_pos: section.settings.ch_pos, cv_pos: section.settings.cv_pos, ch_pos_tb: section.settings.ch_pos_tb, cv_pos_tb: section.settings.cv_pos_tb, ch_pos_mb: section.settings.ch_pos_mb, cv_pos_mb: section.settings.cv_pos_mb, append_bg_content_style: append_bg_content_style -%}">
                                          {%- assign enable_btn_close = block.settings.btn_close -%}
                                          {%- capture data_options -%}
                                              {"type": "{%- if block.settings.source == "1" -%}{{ block.settings.video_url.type }}{%-else -%}html5{%- endif -%}", "vid": "{{ block.settings.video_url.id }}", "autoplay": {{ block.settings.au_video }}, "loop": {{ block.settings.loop_video }}, "accent_color": "{{ settings.accent_color | remove_first: '#' }}","srcDefault": "https://cdn.shopify.com/s/files/1/0610/5209/2628/files/Share_your_brand_story_by_adding_a_video_to_your_store.mp4?v=1641630446", "id":"#tmp-video-{{ sid }}" }
                                          {%- endcapture -%}
                                          <div class="t4s-btn-video__wrapper t4s-d-inline-flex t4s-align-items-center t4s-bl-item t4s-animation-none" style="--btn-video-w: {{ block.settings.button_width }}px;--icon-video-w: {{ block.settings.icon_width }}px;--btn-video-fs: {{ block.settings.button_fs }}px;--btn-video-fw: {{ block.settings.button_fw }};--btn-video-w-tb: {{ block.settings.button_width_tb }}px;--icon-video-w-tb: {{ block.settings.icon_width_tb }}px;--btn-video-fs-tb: {{ block.settings.button_fs_tb }}px;--btn-video-w-mb: {{ block.settings.button_width_mb }}px;--icon-video-w-mb: {{ block.settings.icon_width_mb }}px;--btn-video-fs-mb: {{ block.settings.button_fs_mb }}px;--mgb: {{ block.settings.mgb }}px;--mgb-tb: {{ block.settings.mgb_tb }}px;--mgb-mb: {{ block.settings.mgb_mb }}px;--btn-bg: {{ block.settings.button_bg }};--btn-br-color: {{ block.settings.button_br_color }};--icon-color: {{ block.settings.icon_color }};--label-color: {{ block.settings.label_color }};--animation: {{ block.settings.animation }};--delay-animation: {{ ani_delay }}s;">
                                              <div class="t4s-btn-video__box t4s-btn-video__border-{{ block.settings.use_border }} t4s-pe-auto" {{ block.settings.click_action }} data-options='{{ data_options }}'>
                                                  <svg xmlns="http://www.w3.org/2000/svg" version="1.0" viewBox="0 0 44.000000 48.000000">
                                                      <g transform="translate(0.000000,48.000000) scale(0.100000,-0.100000)" stroke="none">
                                                          <path d="M20 246 c0 -130 2 -236 6 -236 3 0 49 25 102 56 53 31 111 64 127 74 117 68 155 92 155 100 0 8 -38 32 -155 100 -162 94 -212 123 -222 132 -10 8 -13 -38 -13 -226z"></path>
                                                      </g>
                                                  </svg>
                                              </div> 
                                              {%- if block.settings.btn_label != blank -%}<button class="t4s-btn-video__link t4s-pe-auto" {{ block.settings.click_action }} data-options='{{ data_options }}'>{{ block.settings.btn_label }}</button>{%- endif -%}
                                          </div>
                                          {%- if block.settings.source == "2" and block.settings.video != blank -%}
                                          <template id="tmp-video-{{ sid }}" class="t4s-d-none">{{ block.settings.video | video_tag: image_size: '1x1', autoplay: block.settings.au_video, loop: block.settings.loop_video, muted: false, controls: true, controlsList: 'nodownload', preload: 'auto', playsinline: '' }}</template>
                                          {%- endif -%}
                                                
                                          {%- if block.settings.animation != 'none' %}{% assign ani_delay = ani_delay | plus: time_ani_delay %}{% endif -%}
                                      </div>
                                  </div>
                                <a href="{{ url }}" target="{{ open_link }}" class="t4s-full-width-link{% if url == blank %} t4s-pe-none {% else %} t4s-pe-auto{% endif %}" style="--bg-overlay: {{ bg_overlay }};"></a>
                              </div>
                              {%- if enable_btn_close -%}
                                  <button class="t4s-btn-cl-vi t4s-pa t4s-t-0" data-video-poster-close title="{{ 'general.popup.close' | t }}"><svg class="t4s-iconsvg-close" role="presentation" viewBox="0 0 16 14"><path d="M15 0L1 14m14 0L1 0" stroke="currentColor" fill="none" fill-rule="evenodd"></path></svg></button>
                              {%- endif -%}
                            </div>
                          {%- when 'gallery' -%}
                            {%- liquid
                              assign open_link = block.settings.open_link
                              assign ARRhtml1 = 'a,,' | split: ','
                              assign ARRhtml2 = 'div,data-' | split: ',' 
                            -%}
                            <div id="t4s-sidebar-{{ block.id }}" class=" t4s-col-item t4s-widget t4s-widget-gallery t4s-tab-wrapper t4s-active" data-t4s-tab-wrapper data-no-auto-close {{ block.shopify_attributes }}>
                                {%- if block.settings.heading != blank -%}
                                  <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                                {%- endif -%}   
                                <div id="b_{{ block.id }}" class="t4s_position_8 t4s_cover t4s-tab-content" data-t4s-tab-content style="display: block;">
                                    <div class="t4s-row t4s_ratio1_1 t4s-row-cols-{{ block.settings.col_dk }} t4s-row-cols-md-{{ block.settings.col_dk }} t4s-row-cols-lg-{{ block.settings.col_dk }} t4s-gx-{{ block.settings.space_item }} t4s-gy-{{ block.settings.space_item}}">
                                        {%- for i in (1..9) -%}
                                            {%- liquid
                                                assign image_list = 'img' | append: i 
                                                assign image_url_list = 'link' | append: i 
                                                assign url = block.settings[image_url_list]
                                                assign image = block.settings[image_list] 
                                                if url == blank 
                                                    assign ARRhtml = ARRhtml2
                                                else 
                                                    assign ARRhtml = ARRhtml1  
                                                endif  
                                            -%}                                               
                                            {%- if image != blank -%}
                                                <div class="t4s-col-item t4s-gallery-item">
                                                    <{{ ARRhtml[0] }} {{ ARRhtml[1] }}href="{{ url }}" {{ ARRhtml[2] }}target="{{ open_link }}" class="t4s-effect t4s-pr t4s-oh t4s-d-block">
                                                        <div class="t4s_ratio" style="background: url({{ image | image_url: width: 1 }});">
                                                            <img class="lazyloadt4s" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                                                        </div>
                                                    </{{ ARRhtml[0] }}>
                                                </div>  
                                            {%- endif -%}                                            
                                        {%- endfor -%}
                                    </div>
                                </div> 
                                {%- style -%}
                                    #t4s-sidebar-{{ block.id }} .t4s-gallery-item .t4s_ratio{border-radius: {{ block.settings.round }}%} 
                                {%- endstyle -%}
                            </div>
                          {%- when 'image_gallery' -%}
                            {%- liquid 
                              assign general_block = true
                              assign image = block.settings.image_child
                              if block.settings.img_link != blank
                                assign ARRhtml = 'a,,' | split: ',' 
                              else
                                assign ARRhtml = 'div,data-,data-' | split: ',' 
                              endif 
                          -%}
                            {% if block.settings.use_hover_gallery %}{{ 't4s-img-gallery-hover.css' | asset_url | stylesheet_tag }}{%- endif -%} 
                          
                              <div class="t4s-gallery-child t4s-img-child t4s-circle-txt-{{ block.settings.box_txt_align }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-pr t4s-text-{{ block.settings.img_align }} t4s-hover-gallery-{{ block.settings.use_hover_gallery }} t4s-shadow-gallery-{{ block.settings.use_shadow_gallery }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'image_gallery', block: block -%}>
                                <{{ ARRhtml[0] }} {{ ARRhtml[1] }}href="{{ block.settings.img_link }}" {{ ARRhtml[2] }}target="{{ block.settings.open_link }}" class="t4s-pr t4s-img-inner" {% if block.settings.use_txt %}{%- render 'bk_cus_style', type: 'custom_text_circle', block: block -%}{% endif %}> 
                                  <div class="t4sw-wrap-img t4s-pr t4s-w-100">
                                    {%- if image != blank -%}{%- assign ratio = image.aspect_ratio -%}
                                      <div class="t4s-img-w t4s-pr t4s-d-inline-block t4s_position_8 t4s_ratioadapt t4s_cover">
                                        <div class="t4s-img-i t4s_ratio t4s-bg-11" style="--aspect-ratioapt: {{ ratio | default: 1.7777 }};background: url({{ image | image_url: width: 1 }});">
                                          <img class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff t4s-img-as-bg pin__image" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                                        </div>
                                      </div>
                                    {%- else -%}
                                      {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                                    {%- endif -%}
                                    {%- for i in (1..5) -%}
                                      {%- liquid
                                          assign img   = 'img_child_' | append: i
                                          assign img_w = 'img_w_' | append: i
                                          assign img_pos_t     = 'img_t_' | append: i
                                          assign img_pos_l     = 'img_l_' | append: i
                                          assign img_id   = block.settings[img]
                                          assign img_w_id = block.settings[img_w]
                                          assign img_pos_t_id = block.settings[img_pos_t]
                                          assign img_pos_l_id  = block.settings[img_pos_l]
                                      -%}
                                      {%- if img_id != blank -%}
                                        <div class="t4s-img-layer t4s-pa t4s-{{ img }}" style="--layer-w: {{ img_w_id }}%;--ps-top: {{ img_pos_t_id }}%;--ps-left: {{ img_pos_l_id }}%;"> 
                                          <div class="t4s-pa t4s-img-layer-innner">
                                            <img class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff" data-src="{{ img_id | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ img_id.width }}" height="{{ img_id.height }}" alt="{{ img_id.alt | escape }}" loading="lazy">
                                            <span class="lazyloadt4s-loader is-bg-img" style="background: url({{ img_id | image_url: width: 1 }});"></span>
                                          </div>
                                        </div>
                                      {%- endif -%} 
                                    {%- endfor -%}
                                  </div>   
                                  {% if block.settings.text != blank and block.settings.use_txt %}
                                    {{ 't4s-text-circle.css' | asset_url | stylesheet_tag }}
                                    <style>@media (max-width:calc({{ block.settings.box_text_width }}px + {{ settings.cus_w_bd }}px)){.t4s-img-child .t4s-img-inner {margin: 0 !important; } }</style> 
                                    <div class="t4s-box-text-circle-wrap t4s-text-md-{{ block.settings.box_align_tb }} t4s-text-{{ block.settings.box_align_mb }} t4s-hidden-mobile-{{ block.settings.hidden_mobile_txt }}">
                                      <div class="t4s-box-text-circle t4s-pr t4s-oh t4s-d-inline-block">
                                        <svg viewbox="0 0 100 100" class="t4s-text-circle">
                                          <defs><path id="t4s-text-circle" d="M 50, 50 m -37, 0 a 37,37 0 1,1 74,0 a 37,37 0 1,1 -74,0"></path></defs>
                                          <text font-size="6px">
                                            <textpath xlink:href="#t4s-text-circle">{{ block.settings.text | strip_html }}</textpath>
                                          </text>
                                        </svg>
                                      </div>
                                    </div>
                                  {% endif %}
                                </{{ ARRhtml[0] }}>
                              </div> 
                          {%- when 'skillbar' -%}
                            {%- if block.settings.text != blank -%}
                              <div class="t4s-fnt-fm-{{ block.settings.fontf }} t4s-skill-bar" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_text', block: block, ani_delay: ani_delay -%}>
                                <div class="t4s-bar-wrap" style="--skill-range: {{ block.settings.skill_bar }}%;--bg-bar: {{ block.settings.bg_cl }};--bg-skill: {{ block.settings.bg_bar_cl }};">
                                  <div class="t4s-title-block t4s-d-flex t4s-justify-content-between t4s-align-items-center" style="margin-bottom:10px;">
                                    <span data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" class="t4s-text t4s-text-bl" style="margin-bottom:0;">{{ block.settings.text }}</span>
                                    <span data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" class="t4s-percent-skill t4s-text-bl" style="margin-bottom:0;">{{ block.settings.skill_bar }}%</span>
                                  </div>
