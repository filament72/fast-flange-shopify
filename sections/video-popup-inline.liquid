<!-- sections/video-popup-inline.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
{{ 't4s-hero.css' | asset_url | stylesheet_tag }}
{%-liquid
  assign stt_layout = section.settings.layout
  assign image_mb = section.settings.image_mb
  assign image = section.settings.image | default: image_mb
  assign mb_image = image_mb | default: image 
  assign se_height = section.settings.se_height
  assign url = section.settings.link_img1 
  assign open_link = section.settings.open_link
  assign bg_content_op = section.settings.bg_content_op | divided_by: 100.0 
  assign bg_content = section.settings.bg_content_cl | color_modify: 'alpha', bg_content_op 
  assign bg_opacity = section.settings.bg_opacity | divided_by: 100.0 
  assign bg_overlay = section.settings.bg_overlay | color_modify: 'alpha', bg_opacity 
  assign br_opacity = section.settings.br_opacity | divided_by: 100.0
  assign br_bg = section.settings.br_bg | color_modify: 'alpha', br_opacity 
  assign ani_delay = 0   
  assign percent_delay = section.settings.animation_delay | divided_by: 100.0
  assign time_ani_delay = section.settings.time_animation | times: percent_delay
  if stt_layout == 't4s-container-wrap' 
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif
  assign countdown = false
  assign use_button = false
  assign use_animation = false
  assign parallax = section.settings.parallax
  assign general_block = false
 -%}
{%- capture append_style -%}
   
{%- endcapture -%}
{%- capture append_bg_content_style -%}--bg-content: {{ bg_content }};--content-pd: {{ section.settings.content_pd_tb }}px {{ section.settings.content_pd_lr }}px;--content-pd-mb: {{ section.settings.content_pd_tb_mb }}px {{ section.settings.content_pd_lr_mb }}px;{%- endcapture -%}
{%- if section.settings.border_bl -%}
  {%- capture append_bg_border_style -%}--br-color: {{ section.settings.br_color }};--br-style: {{ section.settings.br_style }};--br-pd: {{ section.settings.br_pd }}px;--br-pd-mb: {{ section.settings.br_pd_mb }}px;--border-bg: {{ br_bg }};{%- endcapture -%}
{%- endif -%}
{% render 'section_style', append_style: append_style, sid: section.id -%}
<div class="t4s-section-inner t4s-pr t4s_se_{{ section.id }} {{ stt_layout }}"  data-t4s-animate>
  {{- html_layout[0] -}} 
  <div class="t4s-pr t4s-oh" style=" --aspect-ratio-cus: {{ section.settings.height_dk }}px;--aspect-ratio-custb : {{ section.settings.height_tb }}px;--aspect-ratio-cusmb : {{ section.settings.height_mb }}px;">
    <div class="t4s-video-popup-inline t4s-bg-video t4s-pr t4s-row t4s-row-cols-1 t4s-gx-0 t4s-video t4s_ratio_mix {{ se_height }} t4s_ratio_cuspx_mb_{{ section.settings.custom_mb }} t4s_ratio_cuspx_tb_{{ section.settings.custom_tb }} t4s_ratio_cuspx_{{ section.settings.custom_dk }}" data-video-poster>
      <div class="t4s-col-item">
        <div data-video-insert class="t4s-hero-inner t4s-pr t4s-oh t4s_cover t4s_ratio {% if parallax == true %}t4s-parallax t4s-parallax-bg{% endif %} lazyloadt4s" {% if parallax == true %}data-parallax-t4strue{% endif %} style="--ratioapt: {{ image.aspect_ratio | default: 1.7777 }};--ratioaptmb: {{ mb_image.aspect_ratio | default: 1.7777 }};"> 
          {%- if image != blank -%}    
            {%- liquid
              assign img_url_w     = image | image_url: width: 1904 | split: '1904'
              assign img_url_w0    = img_url_w[0]
              assign img_url_w1    = img_url_w[1]
              assign img_url_w_mb  = mb_image | image_url: width: 1904 | split: '1904'
              assign img_url_w_mb0 = img_url_w_mb[0]
              assign img_url_w_mb1 = img_url_w_mb[1]  
            -%}
            
            <img {% if image_mb.presentation.focal_point != blank %} style="object-position: {{image_mb.presentation.focal_point}}"{% endif %} class="t4s-img-as-bg t4s-d-md-none t4s-parallax-img" srcset="{{ img_url_w_mb0 }}375{{ img_url_w_mb1 }} 375w, {{ img_url_w_mb0 }}550{{ img_url_w_mb1 }} 550w, {{ img_url_w_mb0 }}750{{ img_url_w_mb1 }} 750w, {{ img_url_w_mb0 }}1100{{ img_url_w_mb1 }} 1100w" loading="lazy" alt="{{ mb_image.alt | escape }}" width="{{ mb_image.width }}" height="{{ mb_image.height }}">
            
            <img {% if image.presentation.focal_point != blank %} style="object-position: {{image.presentation.focal_point}}"{% endif %} class="t4s-img-as-bg t4s-d-none t4s-d-md-block t4s-parallax-img" srcset="{{ img_url_w0 }}375{{ img_url_w1 }} 375w, {{ img_url_w0 }}550{{ img_url_w1 }} 550w, {{ img_url_w0 }}750{{ img_url_w1 }} 750w, {{ img_url_w0 }}1100{{ img_url_w1 }} 1100w, {{ img_url_w0 }}1500{{ img_url_w1 }} 1500w, {{ img_url_w0 }}1780{{ img_url_w1 }} 1780w, {{ img_url_w0 }}2000{{ img_url_w1 }} 2000w, {{ img_url_w0 }}3000{{ img_url_w1 }} 3000w,{{ img_url_w0 }}3840{{ img_url_w1 }} 3840w, {{ image | image_url }} {{ image.width }}w" sizes="100vw" src="{{ img_url_w0 }}1500{{ img_url_w1 }}" loading="lazy" alt="{{ image.alt | escape }}" width="{{ image.width }}" height="{{ image.height }}">
            
          {%- else -%}
            {{ 'lifestyle-1' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1' }} 
          {%- endif -%}
          <div class="t4s-content-wrap t4s-bg-cl-wrap t4s-pe-none t4s-full-width-link t4s-z-100">
            <div class="t4s-content-position t4s-{{ section.settings.content_width }} t4s-pa t4s-text-lg-{{ section.settings.text_align }} t4s-text-md-{{ section.settings.text_align_tb }} t4s-text-{{ section.settings.text_align_mb }} t4s-bg-content-true t4s-br-content-{{ section.settings.border_bl }}" style="--time-animation: {{ section.settings.time_animation }}s;{%- render 'content_position', ch_pos: section.settings.ch_pos, cv_pos: section.settings.cv_pos, ch_pos_tb: section.settings.ch_pos_tb, cv_pos_tb: section.settings.cv_pos_tb, ch_pos_mb: section.settings.ch_pos_mb, cv_pos_mb: section.settings.cv_pos_mb, append_bg_content_style: append_bg_content_style, append_bg_border_style: append_bg_border_style -%}">
              {%- for block in section.blocks -%} 
                
                {%- case block.type -%}
                  {%- when 'custom_text' -%}
                    {%- assign general_block = true -%}
                    {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                    <{{ block.settings.tag }} data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-text-bl t4s-fnt-fm-{{ block.settings.fontf }} t4s-font-italic-{{ block.settings.font_italic }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-hidden-br-mb-{{ block.settings.remove_br_tag }} t4s-text-shadow-{{ block.settings.text_shadow }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_text', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</{{ block.settings.tag }}>
                  {%- when 'space_html' -%}
                    {%- assign general_block = true -%}
                    {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                    <div class="t4s-bl-item t4s-space-html t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" {%- render 'bk_cus_style', type: 'space_html', block: block, ani_delay: ani_delay -%}></div>
                  {%- when 'html' -%}
                    {%- assign general_block = true -%}
                    {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                    <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-raw-html t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-rte--list" {%- render 'bk_cus_style', type: 'html', block: block, ani_delay: ani_delay -%}>{{ block.settings.html_content }}</div>
                  {%- when 'image' -%}
                    {%- assign image = block.settings.image_child -%}
                    {%- if image -%}
                      {%- assign general_block = true -%}
                      {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                      <div class="t4s-bl-item t4s-img-child t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" {%- render 'bk_cus_style', type: 'image', block: block, ani_delay: ani_delay -%}>
                        <img class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                      </div>
                    {%- endif -%}
                  {%- when 'countdown' -%}
                    {%- if block.settings.date != blank -%}
                  {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {% endif %}
                      {%- assign countdown = true -%}
                        <div class="t4s-bl-item t4s-countdown sepr_coun_dt_wrap t4s-countdown-des-{{ block.settings.cdt_des }} t4s-countdown-size-{{ block.settings.cdt_size }} t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'countdown', block: block, ani_delay: ani_delay -%}>
                          <div class="t4s-time" data-countdown-t4s data-date='{{ block.settings.date }}' data-keyid='#countdown-{{ section.id }}'></div>
                        </div>
                    {% endif %}
                  {%- when 'custom_button' -%}
                    {%- if block.settings.button_link and block.settings.button_text != blank -%}
                      {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                      {%- assign use_button = true -%}
                      {%- assign button_style = block.settings.button_style -%}
                      <a href="{{ block.settings.button_link }}" target="{{ block.settings.target_link }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-btn t4s-btn-custom t4s-pe-auto t4s-fnt-fm-{{ block.settings.fontf }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' %}t4s-btn-effect-{{ block.settings.button_effect }}{% endif %}" {%- render 'bk_cus_style', type: 'custom_button', block: block, ani_delay: ani_delay -%}>{{ block.settings.button_text }}
                      {%- if block.settings.button_icon_w > 0 -%}
                        {%- if block.settings.icon_style == "1" -%}
                          {% comment %} <svg class="t4s-btn-icon"  width="16" height="11" viewBox="0 0 16 11" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <rect y="4" width="12" height="3" />
                            <path d="M15.0434 5.50985L11.3245 9.44864L11.2626 1.63039L15.0434 5.50985Z"/>
                          </svg> {% endcomment %}
                          <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="18" height="18"><path d="M 21.1875 9.28125 L 19.78125 10.71875 L 24.0625 15 L 4 15 L 4 17 L 24.0625 17 L 19.78125 21.28125 L 21.1875 22.71875 L 27.90625 16 Z"/></svg>
                        {%- elsif block.settings.icon_style == "2" -%}
                            <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512" width="18" height="18"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>   
                        {% else %}
                          <svg class="t4s-btn-icon" version="1.1" x="0px" y="0px" width="22" height="22" viewBox="0 0 22 22" style="enable-background:new 0 0 22 22;" xml:space="preserve"><polygon points="6,20 6,2 17,11 "></polygon></svg>  
                        {%- endif -%}
                    {%- endif -%}
                      </a>
                    {%- endif -%}
                  {%- when 'button_video' -%}
                    {%- assign enable_btn_close = block.settings.btn_close -%}
                    {%- capture data_options -%}
                      {"type": "{%- if block.settings.source == "1" -%}{{ block.settings.video_url.type }}{%-else -%}html5{%- endif -%}", "vid": "{{ block.settings.video_url.id }}", "autoplay": {{ block.settings.au_video }}, "loop": {{ block.settings.loop_video }}, "accent_color": "{{ settings.accent_color | remove_first: '#' }}","srcDefault": "https://cdn.shopify.com/s/files/1/0610/5209/2628/files/Share_your_brand_story_by_adding_a_video_to_your_store.mp4?v=1641630446", "id":"#tmp-video-{{ section.id }}" }
                    {%- endcapture -%}
                    <div class="t4s-btn-video__wrapper t4s-d-inline-flex t4s-align-items-center t4s-bl-item t4s-animation-{{ block.settings.animation }}" style="--btn-video-w: {{ block.settings.button_width }}px;--icon-video-w: {{ block.settings.icon_width }}px;--btn-video-fs: {{ block.settings.button_fs }}px;--btn-video-fw: {{ block.settings.button_fw }};--btn-video-w-tb: {{ block.settings.button_width_tb }}px;--icon-video-w-tb: {{ block.settings.icon_width_tb }}px;--btn-video-fs-tb: {{ block.settings.button_fs_tb }}px;--btn-video-w-mb: {{ block.settings.button_width_mb }}px;--icon-video-w-mb: {{ block.settings.icon_width_mb }}px;--btn-video-fs-mb: {{ block.settings.button_fs_mb }}px;--mgb: {{ block.settings.mgb }}px;--mgb-tb: {{ block.settings.mgb_tb }}px;--mgb-mb: {{ block.settings.mgb_mb }}px;--btn-bg: {{ block.settings.button_bg }};--button_bg_hr:{{ block.settings.button_bg_hr }};--btn-br-color: {{ block.settings.button_br_color }};--icon-color: {{ block.settings.icon_color }};--label-color: {{ block.settings.label_color }};--animation: {{ block.settings.animation }};--delay-animation: {{ ani_delay }}s;">
                      <div class="t4s-btn-video__box t4s-btn-video__border-{{ block.settings.use_border }} t4s-pe-auto" {{ block.settings.click_action }} data-options='{{ data_options }}'>
                        {% if block.settings.source_btn == "icon" %}
                          <svg xmlns="http://www.w3.org/2000/svg" version="1.0" viewBox="0 0 44.000000 48.000000">
                            <g transform="translate(0.000000,48.000000) scale(0.100000,-0.100000)" stroke="none">
                              <path d="M20 246 c0 -130 2 -236 6 -236 3 0 49 25 102 56 53 31 111 64 127 74 117 68 155 92 155 100 0 8 -38 32 -155 100 -162 94 -212 123 -222 132 -10 8 -13 -38 -13 -226z"></path>
                            </g>
                          </svg>
                        {% else %}
                      {%- if block.settings.btn_label != blank -%}<button class="t4s-btn-video__link t4s-pe-auto" {{ block.settings.click_action }} data-options='{{ data_options }}'>{{ block.settings.btn_label }}</button>{%- endif -%}
                        {% endif %}
                      </div> 
                    </div>
                    {%- if block.settings.source == "2" and block.settings.video != blank -%}
                    <template id="tmp-video-{{ section.id }}" class="t4s-d-none">{{ block.settings.video | video_tag: image_size: '1x1', autoplay: block.settings.au_video, loop: block.settings.loop_video, muted: false, controls: true, controlsList: 'nodownload', preload: 'auto', playsinline: '' }}</template>
                    {%- endif -%}
                {%- endcase -%}
                {%- if block.settings.animation != 'none' %}{% assign ani_delay = ani_delay | plus: time_ani_delay %}{% endif -%}
              {%- endfor -%} 
            </div>
          </div>
          <a href="{{ url }}" target="{{ open_link }}" class="t4s-full-width-link{% if url == blank %} t4s-pe-none {% else %} t4s-pe-auto{% endif %}" style="--bg-overlay: {{ bg_overlay }};"></a>
        </div>
        {%- if enable_btn_close -%}
          <button class="t4s-btn-cl-vi t4s-pa t4s-t-0" data-video-poster-close title="{{ 'general.popup.close' | t }}"><svg class="t4s-iconsvg-close" role="presentation" viewBox="0 0 16 14"><path d="M15 0L1 14m14 0L1 0" stroke="currentColor" fill="none" fill-rule="evenodd"></path></svg></button>
        {%- endif -%}
      </div>
    </div>
  </div>
  {{- html_layout[1] -}}
  {%- if section.settings.bottom_arrow %}{{ 't4s-bottom_arrow.css' | asset_url | stylesheet_tag }}<button data-go-id="#section-{{ section.id }}-anchor" data-offset="{% if section.settings.bottom_arrow_pos == '2' %}-28{% else %}-.1{% endif %}" class="t4s-scroll-bottom_arrow is--style_{{ section.settings.bottom_arrow_style }} is--cl_{{ section.settings.bottom_arrow_color }} is--pos_{{ section.settings.bottom_arrow_pos }}" aria-label="{{ 'general.aria.scroll_for_more' | t | escape }}"><svg viewBox="0 0 21 11" role="presentation" class="t4s-icon--arrow-bottom"><polyline fill="none" stroke="currentColor" points="0.5 0.5 10.5 10.5 20.5 0.5" stroke-width="1.25"></polyline></svg></button><span id="section-{{ section.id }}-anchor" class="t4s-anchor"></span>{% endif -%}
</div>
{%- if general_block -%}
  {{ 't4s-general-block.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- if use_button -%}
  {{ 't4s-button-style.css' | asset_url | stylesheet_tag }}
  <link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- endif -%}
{%- if use_animation -%}
  <link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- endif -%}
{%- if countdown -%} 
  {{ 't4s-countdown.css' | asset_url | stylesheet_tag }}
  <template id="countdown-{{ section.id }}">
    <span class="countdown-days">
      <span class="cd_timet4s cd-number">%-D</span>
      <span class="cd_txtt4s cd-text">%!D:{{ "sections.countdown_text.day" | t }},{{ "sections.countdown_text.day_plural" | t }};</span>
    </span>
    <span class="countdown-hours">
      <span class="cd_timet4s cd-number">%H</span> 
      <span class="cd_txtt4s cd-text">%!H:{{ "sections.countdown_text.hr" | t }},{{ "sections.countdown_text.hr_plural" | t }};</span>
    </span>
    <span class="countdown-min">
      <span class="cd_timet4s cd-number">%M</span> 
      <span class="cd_txtt4s cd-text">%!M:{{ "sections.countdown_text.min" | t }},{{ "sections.countdown_text.min_plural" | t }};</span>
    </span>
    <span class="countdown-sec">
      <span class="cd_timet4s cd-number">%S</span> 
      <span class="cd_txtt4s cd-text">%!S:{{ "sections.countdown_text.sec" | t }},{{ "sections.countdown_text.sec_plural" | t }};</span>
    </span>
  </template>
{%- endif -%}
{%- if parallax -%}
  {{ 't4s-parallax.min.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{% schema %}
{
  "name": "Video popup & inline",
  "tag": "section",
  "class": "t4s-section t4s-section-all t4s_tp_parallax t4s_tp_cd t4s_tp_mfps t4s_tp_video",
  "disabled_on": {
    "groups": [
      "*"
    ]
  },
  "settings": [
    {
      "type": "header",
      "content": "1. General options"
    },
    {
      "type": "select",
      "id": "se_height",
      "label": "Section height",
      "default": "t4s_ratioadapt_mix",
      "options": [
        {
          "value": "t4s_ratio_fh",
          "label": "Full screen height"
        },
        {
          "value": "t4s_ratioadapt_mix",
          "label": "Adapt to image"
        },
        {
          "value": "t4s_ratioadapt_mix16_9",
          "label": "16:9"
        },
        {
          "value": "t4s_ratioadapt_mix4_3",
          "label": "4:3"
        }
      ]
    },
    {
      "type": "checkbox",
      "id": "custom_dk",
      "label": "Use custom height (Desktop)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_dk",
      "label": "Section height (Desktop)",
      "default": 600
    },
    {
      "type": "checkbox",
      "id": "custom_tb",
      "label": "Use custom height (Tablet)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_tb",
      "label": "Section height (Tablet)",
      "default": 400
    },
    {
      "type": "checkbox",
      "id": "custom_mb",
      "label": "Use custom height (Mobile)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_mb",
      "label": "Section height (Mobile)",
      "default": 250
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Choose Image",
      "info": "1800 x 600px .jpg recommended"
    },
    {
      "type": "image_picker",
      "id": "image_mb",
      "label": "Mobile image (optional)",
      "info": "750 x 1100px .jpg recommended. If none is set, desktop image will be used."
    },
    {
      "type": "checkbox",
      "id": "parallax",
      "label": "Enable parallax scroll",
      "default": false
    },
    {
      "type": "url",
      "id": "link_img1",
      "label": "Link image",
      "info": "The whole image becomes clickable."
    },
    {
      "type": "select",
      "id": "open_link",
      "label": "Open link in",
      "default": "_blank",
      "options": [
        {
          "value": "_self",
          "label": "Current window"
        },
        {
          "value": "_blank",
          "label": "New window"
        }
      ]
    },

    {
      "type": "header",
      "content": "+ Bottom arrow"
    },
    {
      "type": "checkbox",
      "id": "bottom_arrow",
      "label": "Enable bottom arrow",
      "info": "When click will scroll the page to the section below.",
      "default": false
    },
    {
      "type": "select",
      "id": "bottom_arrow_style",
      "default": "button",
      "label": "Style",
      "options": [
        {
          "value": "simple",
          "label": "Simple"
        },
        {
          "value": "button",
          "label": "Button"
        }
      ]
    },
    {
      "type": "select",
      "id": "bottom_arrow_color",
      "default": "light",
      "label": "Color",
      "options": [
        {
          "value": "light",
          "label": "Light"
        },
        {
          "value": "dark",
          "label": "Dark"
        }
      ]
    },
