<!-- blog-post-3.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
{{ 't4s-blog.css' | asset_url | stylesheet_tag }}
{{ 't4s-general-block.css' | asset_url | stylesheet_tag }}  
<link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
<link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
  assign sid = section.id
  assign stt_layout = section.settings.layout
  assign stt_image_bg = section.settings.image_bg

  if stt_layout == 't4s-se-container' 
   assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
  elsif stt_layout == 't4s-container-wrap'
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif
  if section.settings.btn_owl == "simple"
    assign arrow_icon = 1
  else
    assign arrow_icon = 2
  endif
  assign show_btn = section.settings.show_btn
  if show_btn != "hidden"
    assign slider_btns = true
  else
    assign slider_btns = false
  endif
  assign show_dots = section.settings.show_dots
  if show_dots != "hidden"
    assign slider_dots = true
  else
    assign slider_dots = false
  endif
  assign blog = blogs[section.settings.blog]
  assign blog_url = blog.url
  assign limit = section.settings.limit
  assign article_des = section.settings.article_des
  assign by_txt = 'blogs.article.by' | t
  assign readmore_txt = 'blogs.article.read_more' | t
  assign b_effect = section.settings.b_effect
  assign img_effect = section.settings.img_effect
  assign show_tags = section.settings.show_tags
  assign show_content = section.settings.show_content
  assign show_author = section.settings.show_author
  assign show_comment = section.settings.show_comment
  assign show_date = section.settings.show_date
  assign date_format = section.settings.date
  assign show_readmore = section.settings.show_readmore
  assign show_ic_readmore = section.settings.show_ic_readmore
  assign item-cellSelector = ".t4s-article-item9 "
  assign index = 0
  assign index2 = 1
  assign arr_item = section.blocks | where: "type", 'bl_col'  
 -%}
{% render 'section_style', sid: sid %}
<div class="t4s-section-inner t4s_se_{{ sid }} {{ stt_layout }} {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%} t4s-has-imgbg lazyloadt4s {%- endif -%}" {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5"{%- endif -%}>
  {{- html_layout[0] -}}
  {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner {%- if stt_image_bg != blank -%} t4s-has-imgbg lazyloadt4s {%- endif -%}" {%- if stt_image_bg != blank -%} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%}>{%- endif -%}
    <div data-collection-url="{{ collection.url }}" class="t4s-show-border-{{ section.settings.show_bd_se }}  t4s-blog-{{ section.settings.blog_pos }} t4s-row t4s-gx-lg-30 t4s-gy-lg-30  t4s-gx-md-30 t4s-gy-md-30 t4s-gx-30 t4s-gy-30">
      {%- for block in arr_item offset: index -%}
        {%- liquid 
          assign index = index | plus: 1
          assign pd_item = block.settings.padding_inner | remove: ' ' | split: ','
          assign pd_item_tb = block.settings.padding_inner_tb | remove: ' ' | split: ','
          assign pd_item_mb = block.settings.padding_inner_mb | remove: ' ' | split: ','
          assign ani_delay = 0
        -%}
        <div class="t4s-custom-col t4s-col-item t4s-col-lg-6 t4s-col-md-12 t4s-col-12 bl-{{ block.id }}">
          <div class="t4s-col-inner t4s-ver-center-{{ block.settings.align_vertical }}  t4s-text-lg-{{ block.settings.content_align }} t4s-text-md-{{ block.settings.content_align_tablet }} t4s-text-{{ block.settings.content_align_mobile }} {% if block.settings.bg_image != blank -%}  lazyloadt4s t4s-has-imgbg {%- endif -%}"  {%- if block.settings.bg_image != blank -%}  data-bgset="{{ block.settings.bg_image | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%}  style="--pd-top: {{ pd_item[0] | default: 0 }};--pd-right: {{ pd_item[1] | default: 0 }};--pd-bottom: {{ pd_item[2] | default: 0 }};--pd-left: {{ pd_item[3] | default: 0 }};--pd-tb-top: {{ pd_item_tb[0] | default: 0 }};--pd-tb-right: {{ pd_item_tb[1] | default: 0 }};--pd-tb-bottom: {{ pd_item_tb[2] | default: 0 }};--pd-tb-left: {{ pd_item_tb[3] | default: 0 }};--pd-mb-top: {{ pd_item_mb[0] | default: 0 }};--pd-mb-right: {{ pd_item_mb[1] | default: 0 }};--pd-mb-bottom: {{ pd_item_mb[2] | default: 0 }};--pd-mb-left: {{ pd_item_mb[3] | default: 0 }}; --bg-cl:  {{ block.settings.bg_cl }}" >
            <div class="t4s-w-100">
              {%- for block in section.blocks offset: index2 -%}
                {%- liquid
                  assign index2 = index2 | plus: 1
                  
                  if block.type != "bl_col" 
                  else 
                    break  
                  endif -%}  
                  {%- case block.type -%}
                    {%- when 'custom_text' -%}
                      {% if block.settings.text != blank %}
                        <{{ block.settings.tag }} data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" class="t4s-bl-item t4s-animation-none t4s-pe-none t4s-text-bl t4s-text-bl-verticle t4s-fnt-fm-{{ block.settings.fontf }} t4s-font-italic-{{ block.settings.font_italic }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-hidden-br-mb-{{ block.settings.remove_br_tag }} t4s-text-shadow-{{ block.settings.text_shadow }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_text', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</{{ block.settings.tag }}>
                      {% endif %}
                    {%- when 'space_html' -%}
                      <div class="t4s-space-html t4s-bl-item t4s-animation-none t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" {%- render 'bk_cus_style', type: 'space_html', block: block, ani_delay: ani_delay -%}></div>
                    {%- when 'html' -%}
                      {% if block.settings.html_content != blank %}
                          <div class="t4s-bl-item t4s-animation-none t4s-raw-html t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-rte--list" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'html', block: block, ani_delay: ani_delay -%}>{{ block.settings.html_content }}</div>
                        {% endif %}
                    {%- when 'custom_label' -%}
                      {% if block.settings.text != blank %}
                          <div data-lh-lg="{{ block.settings.text_lh }}" class="t4s-bl-item t4s-animation-none t4s-custom-label t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-fnt-fm-{{ block.settings.fontf }} t4s-font-italic-{{ block.settings.font_italic }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-hidden-br-mb-{{ block.settings.remove_br_tag }} t4s-text-shadow-{{ block.settings.text_shadow }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_label', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</div>
                        {% endif %}
                    {%- when 'image' -%}
                      {%- assign image = block.settings.image_child -%}
                      {%- if block.settings.img_link != blank -%}
                        {%- assign image_parent = 'a,' | split: ',' -%}
                      {%- else -%}
                        {%- assign image_parent = 'div,data-' | split: ',' -%}
                      {%- endif -%}
                      {%- if image != blank -%}
                          <{{ image_parent[0] }} {{ image_parent[1] }}href="{{ block.settings.img_link }}" class="t4s-bl-item t4s-animation-none t4s-img-child t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', block: block, ani_delay: ani_delay -%}>
                              <img class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                          </{{- image_parent[0] -}}>
                      {%- else -%}
                        {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                      {%- endif -%}
                    {%- endcase -%}
                {%- endfor -%}
            </div>
          </div>
        </div>
        {% endfor %}
        <div class="t4s-blog-col t4s-col-item t4s-col-lg-6 t4s-col-md-12 t4s-col-12 ">
          {%- render 'section_tophead', get_height: true, sid: sid, slider_btns: slider_btns -%}
              {%- liquid
                assign icon_slider = section.settings.icon_slider 
                case icon_slider
                  when '1'
                  assign view_box = "0 0 22 22"
                  when '2'
                  assign view_box = "0 0 22 22"
                  when '3'
                  assign view_box = "0 0 28 18"
                endcase
              -%}
              <div class="t4s-section-content t4s-flickity-slider t4s-blog-list t4s-blog-layout__slider t4s-article-design{{ article_des }} t4s_{{ section.settings.image_ratio }} t4s_position_{{ section.settings.image_position }} t4s_{{ section.settings.image_size }} {{ section.settings.content_align }} t4s-base-slider t4s-pr t4s_box_slider t4s-row t4s-row-cols-lg-{{ section.settings.col_dk }} t4s-row-cols-md-{{ section.settings.col_tb }} t4s-row-cols-{{ section.settings.col_mb }} t4s-gx-lg-{{ section.settings.space_h_item }} t4s-gy-lg-{{ section.settings.space_v_item }} t4s-gx-md-{{ section.settings.space_h_item_tb }} t4s-gy-md-{{ section.settings.space_v_item_tb }} t4s-gx-{{ section.settings.space_h_item_mb }} t4s-gy-{{ section.settings.space_v_item_mb }} {% if slider_btns %} t4s-slider-btn__{{ section.settings.btn_pos }}-content t4s-slider-btn-{{ show_btn }} t4s-slider-btn-style-{{ section.settings.btn_owl }} t4s-slider-btn-{{ section.settings.btn_shape }} t4s-slider-btn-{{ section.settings.btn_size }} t4s-slider-btn-cl-{{ section.settings.btn_cl }} t4s-slider-btn-vi-{{ section.settings.btn_vi }} {% endif %}  
                {% if slider_dots %} t4s-dots-style-{{ section.settings.dot_owl }} t4s-slider-dots-{{ show_dots }} t4s-dots-cl-{{ section.settings.dots_cl }} t4s-dots-round-{{ section.settings.dots_round }} {% endif %} t4s-base-dots-{{ section.settings.dots_pos }} t4s-flickity-slider flickityt4s" data-flickityt4s-js='{"customIcon":{{ icon_slider }}, "viewBox":"{{ view_box }}", "setPrevNextButtons": true,"arrowIcon": "{{ arrow_icon }}","imagesLoaded": 0,"adaptiveHeight": 1, "contain": 1,"groupCells":1,"initialIndex":1, "dragThreshold" : 5,  {% if article_des == "7" %}"cellAlign": "center" {% else %}"cellAlign": "left" {% endif %},"wrapAround": {{ section.settings.loop }},"prevNextButtons": {{ slider_btns }},"percentPosition": 1,"pageDots": {{ slider_dots }}, "autoPlay" : {{ section.settings.au_time | times: 1000 }}, "pauseAutoPlayOnHover" : {{ section.settings.au_hover }}, "t4sid": "{{ sid }}","navUI": 1,"cellSelector": "{{ item-cellSelector }}" }' style="--btn-distance: {{ section.settings.btn_distance }}px;--space-dots: {{ section.settings.dots_space }}px;--dots-bottom-pos: {{ section.settings.dots_bottom_pos }}px;">
              {%- assign  count = 0 -%}
                {%- if blog != blank -%} 
                  {% for article in blog.articles limit: limit %}
                    {%- if count == 0 -%}
                        <div class="t4s-article-item9 t4s-col-item" data-cacl-slide>
                    {%- endif -%}
                    {%-liquid
                    assign aid = article.id
                    assign image = article.image
                    assign article_url = article.url
                    -%}
                    <article class="t4s-col-item t4s-article-item t4s-article-style9 t4s-article-{{ aid }}">
                      <div class="t4s-article-inner t4s-pr t4s-oh t4s-row ">
                        {%- if image != blank -%}
                          <div class=" t4s-col-item t4s-article-thumb t4s-pr t4s-col-lg-4 t4s-col-md-4 t4s-col-12">
                            <a class="t4s-article-img t4s-d-block t4s-eff t4s-eff-{{ b_effect }} t4s-eff-img-{{ img_effect }}" href="{{ article_url }}">
                              <div
                                class="t4s_ratio t4s-bg-11"
                                style="background: url({{ image | image_url: width: 1 }});--aspect-ratioapt: {{ image.aspect_ratio | default: 2 }}"
                                >
                                <img class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                              </div>
                            </a>
                          </div>
                        {%- endif -%}
                        <div class=" t4s-col-item t4s-article-info t4s-col-lg-8 t4s-col-md-8 t4s-col-12">
                          {%- if show_tags -%}
                            {%- assign article_tags = article.tags | join: ';' | split: ';' -%}
                            <div class="t4s-article-tags">
                              {%- for tag in article_tags -%}
                                {%- if tag contains 'blog_' -%}
                                  {%- continue -%}{%- endif -%}
                                <a href="{{ blog_url }}/tagged/{{ tag | handle }}">{{ tag }}</a>
                                {%- unless forloop.last -%}
                                  <span class="t4s-seperate"></span>
                                {% endunless -%}
                              {%- endfor -%}
                            </div>
                          {%- endif -%}
                          <h3 class="t4s-article-title">
                            <a href="{{ article_url }}">{{ article.title }}</a>
                          </h3>
                          {%- if show_content -%}
                            <div class="t4s-article-content t4s-pr t4s-rte">
                              {%- if article.excerpt.size > 0 -%}
                                {{ article.excerpt }}
                              {%- else -%}
                                {{ article.content | strip_html | truncate: 118 }}{%- endif -%}
                            </div>
                          {%- endif -%}
                          {%- if show_author or show_date or show_comment -%}
                            <div class="t4s-article-metas">
                              {%- if show_author -%}
                                <div class="t4s-article-author">
                                  <span class="t4s-article-author__by">{{ by_txt }}
                                  </span>
                                  <span class="t4s-article-author__name">
                                    {{ article.author | replace_first: 'ad clnt', 'admin' }}</span>
                                </div>
                              {%- endif -%}
                              {%- if show_date -%}
                                <div class="t4s-article-time">
                                  <time class="t4s-entry-date">
                                    {%- if date_format contains "%" -%}
                                      {{ article.published_at | time_tag: date_format }}
                                    {%- else -%}
                                      {{ article.published_at | time_tag: format: date_format }}{%- endif -%}
                                  </time>
                                </div>
                              {%- endif -%}
                              {%- if show_comment -%}
                                {%- if article.comments_count > 0 %}
                                  {% assign cm_link = '#comments' %}
                                {% else %}
                                  {% assign cm_link = '#CommentForm' %}{% endif -%}
                                <div class="t4s-article-comment">
                                  <a href="{{ article_url }}{{ cm_link }}" class="t4s-d-inline-block">
                                    <span class="t4s-article-comment__icon"></span>
                                    <span class="t4s-article-comment__count">{{ 'blogs.comments.with_count_html' | t: count: article.comments_count }}</span>
                                  </a>
                                </div>
                              {%- endif -%}
                            </div>
                          {%- endif -%}
                          {%- if show_readmore -%}
                            <div class="t4s-article-btn">
                              <a href="{{ article_url }}" class="t4s-article-readmore">{{ readmore_txt }}</a>
                              {% if show_ic_readmore %}
                                <svg class="t4s-icon-readmore" viewBox="0 0 32 32"><path d="M 21.1875 9.28125 L 19.78125 10.71875 L 24.0625 15 L 4 15 L 4 17 L 24.0625 17 L 19.78125 21.28125 L 21.1875 22.71875 L 27.90625 16 Z" /></svg>
                              {% endif %}
                            </div>
                          {%- endif -%}
                        </div>
                      </div>
                    </article>
                    {%- if count < 3 %}
                      {%- assign  count = count | plus : 1 -%}
                    {%- endif -%}
                    {%- if count == 3 or forloop.last -%}
                      </div>
                      {%- assign  count = 0 -%}
                    {%- endif -%}
                  {%- endfor -%}
                {%- else -%}
                  {%- for i in (1..6) limit: limit -%}
                    {%- if count == 0 -%}
                      <div class="t4s-article-item9 t4s-col-item" data-cacl-slide>
                    {%- endif -%}
                    <article class="t4s-col-item t4s-article-item t4s-article-style1">
                      <div class="t4s-article-inner t4s-pr t4s-oh t4s-row t4s-row-cols-2">
                        <div class="t4s-article-thumb t4s-pr t4s-col-item">
                          <a class="t4s-eff t4s-eff-{{ b_effect }} t4s-eff-img-{{ img_effect }} t4s-d-block" href="/admin/blogs">
                            {% capture current %}{% cycle 1, 2 %}{% endcapture %}
                            <div class="t4s_ratio" style="--aspect-ratioapt: {{ image.aspect_ratio | default: 2 }}">
                              {{ 'lifestyle-' | append: current | placeholder_svg_tag: 't4s-placeholder-svg t4s-obj-eff' }}  
                            </div> 
                          </a>
                        </div>
                        <div class="t4s-article-info t4s-col-item">
                          {%- if show_tags -%}
                            <div class="t4s-article-tags">
                              <a href="/admin/blogs">Fashion</a>,  <a href="/admin/blogs">Lifestyle</a>
                            </div>
                          {%- endif -%}
                          <h3 class="t4s-article-title"><a href="/admin/blogs">{{ 'onboarding.blog_title' | t }}</a></h3>
                          {%- if show_content -%}
                            <div class="t4s-article-content t4s-rte">{{ 'onboarding.blog_excerpt' | t }}</div>
                          {%- endif -%}
                          <div class="t4s-article-metas">
                            {%- if show_author -%}
                              <div class="t4s-article-author">
                                <span class="t4s-article-author__by">{{ by_txt }} </span>
                                <span class="t4s-author-name">{{ 'onboarding.blog_author' | t }}</span>
                              </div>
                            {%- endif -%}
                            {%- if show_date -%}
                              <div class="t4s-article-time">
                                <span class="t4s-article-time__top">28 </span>
                                <span class="t4s-article-time__bottom">May</span>
                              </div>    
                            {%- endif -%}
                            {%- if show_comment -%}
                              <div class="t4s-article-comment">
                                <span class="t4s-article-comment__count">{{ 'blogs.comments.with_count' | t: count: 19 }}</span>
                              </div>
                            {%- endif -%}
                          </div>
                          {%- if show_readmore %}
                            <div class="t4s-article-btn">
                              <a href="/admin/blogs" class="t4s-article-btn__readmore">{{ 'blogs.article.read_more' | t }}</a>
                            </div>
                          {% endif -%}
                        </div>
                      </div>
                    </article>
                    {%- if count < 3 %}
                      {%- assign  count = count | plus : 1 -%}
                    {%- endif -%}
                    {%- if count == 3 or forloop.last -%}
                      </div>
                    {%- assign  count = 0 -%}
                    {%- endif -%}
                  {%- endfor -%}
                {%- endif -%}
              </div>
            </div>
        {{- html_layout[1] -}}
</div>
{% style %}
  .t4s-blog-left{
    flex-direction: row-reverse;
  }
  .t4s-blog-post-3 .t4s-article-title a{
  font-size: 20px;
  line-height: 30px;
  margin-bottom: 10px;
}
.t4s-article-item .t4s-article-info {
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding-top: 0px;
    padding-bottom: 0px;
		max-width:590px;

  }
	.t4s-article-item9 {
		padding:0
	}
  
	.t4s-blog-post-3 .t4s_des_title_9 .t4s-bg-inline-t{
		flex: 1 0 auto;
	}
	.t4s-blog-post-3 .t4s_des_title_9 .t4s-bg-inline-t .t4s-dg-button{
		justify-content:flex-end;
	}
	.t4s-blog-post-3 .t4s_des_title_9 .t4s-heading-reverse{
		gap:10px;
	}
	@media(min-width:1025px){
		.t4s-show-border-true.t4s-blog-left .t4s-blog-col{
    border-inline-end: 1px solid #eee;
  }
  .t4s-show-border-true.t4s-blog-right .t4s-custom-col{
    border-inline-end: 1px solid #eee;
  }
	}
	@media(max-width:1919px){
		.t4s-blog-post-3 .t4s-article-content {
			display:-webkit-box;
			-webkit-line-clamp:2;
			-webkit-box-orient: vertical;
			text-overflow: ellipsis;
    	overflow: hidden;
		}
	}
  @media (max-width: 1024px){
    .t4s-article-item .t4s-article-info {
      padding-top: 0px;
      padding-bottom: 0px;
      padding-inline-end: 15px;
    }
  }
  @media (max-width:576px){
    .t4s-blog-post-3 .t4s-blog-col{
      padding: 0;
    }    
    .t4s-blog-post-3 .t4s-article-item9 {
      padding-inline-start: 15px;
      padding-inline-end: 15px;
    }
     .t4s-article-style9 .t4s-article-thumb {
    margin-bottom: 15px;
  }
  }
{% endstyle %}
{% schema %}
{
  "name": "Blog post 3",
  "tag": "section",
  "class": "t4s-section t4s-section-all t4s_bk_flickity t4s_tp_cdt t4s-blog-post-3 t4s_tp_cd",
  "disabled_on": {
    "groups": [
      "*"
    ]
  },
  "settings": [
    {
      "type": "checkbox",
      "id": "show_bd_se",
      "label": "Show middle border",
      "default": true
    },
    {
      "type": "header",
      "content": "1. Heading options"
    },
    {
      "type": "select",
      "id": "design_heading",
      "label": "Heading design",
      "default": "1",
      "options": [
        {
          "value": "1",
