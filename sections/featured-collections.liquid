<!-- sections/featured-colections.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-collection-products.css' | asset_url | stylesheet_tag }}
{{ 't4s-slider-settings.css' | asset_url | stylesheet_tag }}
{{ 'pre_flickityt4s.min.css' | asset_url | stylesheet_tag }}
{{ 'pre_flickity_custom.css' | asset_url | stylesheet_tag }}
{{ 't4s-featured-colections.css' | asset_url | stylesheet_tag }}
<link href="{{ 't4s-loading.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
  assign sid = section.id
  
  assign stt_layout = section.settings.layout
  assign stt_image_bg = section.settings.image_bg
 -%}

{%- liquid
  if stt_layout == 't4s-se-container' 
   assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
  elsif stt_layout == 't4s-container-wrap'
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif
  
  assign use_link_vendor = settings.use_link_vendor
  assign enable_rating = settings.enable_rating

  assign show_img = settings.show_img
  assign isGrowaveWishlist = false
  if settings.wishlist_mode == "3" and shop.customer_accounts_enabled
    assign isGrowaveWishlist = true
  endif
  assign placeholder_img = settings.placeholder_img
  assign enable_pr_size = settings.enable_pr_size
  assign show_size_type = settings.show_size_type
  assign size_ck = settings.size_ck | append: ',size,sizes,Größe' 
  assign get_size = size_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

  assign enable_pr_color = settings.enable_pr_color
  assign show_cl_type = settings.show_color_type
  assign color_ck = settings.color_ck | append: ',color,colors,couleur,colour'
  assign get_color = color_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

  assign price_varies_style = settings.price_varies_style
  assign app_review = settings.app_review
 -%}

{% render 'section_style', sid: sid %}
<div class="t4s-section-inner t4s_se_{{ sid }} {{ stt_layout }} {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%} t4s-has-imgbg lazyloadt4s {%- endif -%}"  {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%}>
  {{- html_layout[0] -}}
  {%- if stt_layout == 't4s-se-container' -%}
  <div class="t4s-container-inner {%- if stt_image_bg != blank -%} t4s-has-imgbg lazyloadt4s {%- endif -%} "  {%- if stt_image_bg != blank -%}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%} > {%- endif -%}
  
  {%- liquid
    assign icon_slider = '10'
    assign view_box = "0 0 22 17"
  -%}
  {% comment %} flickityt4s {% endcomment %}
  <div class="t4s-sc-f-colls--click">
    <div data-t4s-resizeobserver class="t4s-section-content t4s_box_pr_slider t4s-imgs-preview t4s-row t4s-row-cols-lg-{{ section.settings.col_dk }} t4s-row-cols-md-{{ section.settings.col_tb }} t4s-row-cols-{{ section.settings.col_mb }} t4s-flickity-slider flickityt4s" data-flickityt4s-js='{"customIcon":{{ icon_slider }}, "viewBox":"{{ view_box }}", "setPrevNextButtons": true,"arrowIcon": "simple","imagesLoaded": 0,"adaptiveHeight": 1, "contain": 1, "groupCells": 1, "dragThreshold" : 5, "cellAlign": "left","wrapAround": true,"prevNextButtons": 1,"percentPosition": 0,"pageDots": 0, "autoPlay" : {{ section.settings.au_time | times: 1000 }}, "pauseAutoPlayOnHover" : {{ section.settings.au_hover }}, "t4sid": "{{ sid }}","navUI": 1,"cellSelector": ".t4s-fea-img"}' style="--ts-gutter-x:0px">
    
    {% comment %} {%- assign limit = section.settings.product_list.count -%}
    {%- if limit == 0 -%}{%- assign limit = 6 -%}{%- endif -%} {% endcomment %}
    {%- assign click-on = "" -%}
    {%- for collection in section.settings.collection_list -%}
      <div class="t4s-col-item t4s-fea-img">
        <div class="t4s-overflow-hidden t4s-f-colls--item-f">
          {%- assign info = section.blocks[forloop.index0] -%}
          <div data-connect="#item-f-coll{{ forloop.index }}--click-on" data-t4s-click-on class="t4s-img__banner banner-h-100" style="background-image: url({{ info.settings.image | image_url: width: 1 }});background-size: cover;">
            {%- if info.settings.image -%}
              {{- info.settings.image | image_url: width: info.settings.image.width | image_tag: class: 't4s-img-fit-cover', alt: info.settings.image.alt, size: '100vw', widths: '385, 550, 750, 1100, 1500', loading: 'lazy' -}}
            {%- elsif collection.featured_image -%}
              {{- collection.featured_image | image_url: width: collection.featured_image.width | image_tag: class: 't4s-img-fit-cover', alt: collection.featured_image.alt, size: '100vw', widths: '385, 550, 750, 1100, 1500', loading: 'lazy' -}}
            {%- else -%}
            {%- capture current -%}{%- cycle 1, 2, 3, 4, 5, 6 -%}{%- endcapture -%} 
              {{- 'collection-' | append: current | placeholder_svg_tag: 't4s-placeholder-svg t4s-img-fit-cover' -}}
            {%- endif -%}
          </div>
          <div class="t4s-f-colls--content t4s-pa">
            <h2 class="t4s-f-colls--title t4s-pr"{%- if info -%} style="color: {{ info.settings.title_color }};font-family: var(--font-family-{{ info.settings.fontf }});--font-size: {{ info.settings.text_fs }}px;--line-height: {{ info.settings.text_lh }};--font-weight: {{ info.settings.text_fw }};--letter-spacing: {{ info.settings.text_ls }};{{ info.settings.mobile_css | replace: '--', '--mb-' }}"{% endif %}>{{ info.settings.title | default: collection.title }}</h2>
            {%- if info.settings.desc != blank -%}            
              <p class="t4s-f-colls--desc" style="color: {{ info.settings.desc_color }};font-family: var(--font-family-{{ info.settings.fontf_desc }});--font-size: {{ info.settings.text_fs_desc }}px;--line-height: {{ info.settings.text_lh_desc }};--font-weight: {{ info.settings.text_fw_desc }};--letter-spacing: {{ info.settings.text_ls_desc }};{{ info.settings.mobile_css_desc | replace: '--', '--mb-' }}">{{ info.settings.desc }}</p>
            {%- endif -%}
          </div>
        </div>
      </div>
      {%- capture click-on-item -%}
      

        {%- liquid
          assign image_ratio = info.settings.image_ratio
          if image_ratio == "ratioadapt"
            assign imgatt = ''
           else 
            assign imgatt = 'data-'
          endif
          assign show_vendor = info.settings.show_vendor
          assign limit = info.settings.limit
          assign product_des = info.settings.product_des
          assign use_countdown = info.settings.use_cdt
        -%}
        <div id="item-f-coll{{ forloop.index }}--click-on" class="t4s-row item--click-on">
          <div class="t4s-col-item t4s-col-12 t4s-pr">
            <div class="prd-head t4s-d-flex t4s-justify-content-between t4s-align-items-center">
              <a href="{{ collection.url }}">
                <h3 class="t4s-f-colls--title"{%- if info -%} style="font-family: var(--font-family-{{ info.settings.fontf }});--font-size: {{ info.settings.text_fs }}px;--line-height: {{ info.settings.text_lh }};--font-weight: {{ info.settings.text_fw }};--letter-spacing: {{ info.settings.text_ls }};{{ info.settings.mobile_css | replace: '--', '--mb-' }}"{% endif %}>
                  {{- info.settings.title | default: collection.title -}}
                </h3>
              </a>
              <div class="t4s-f-colls--close-wrapper t4s-pa">
                <div class="t4s-pr t4s-h-100">
                  <button class="t4s-f-colls--close"><svg class="t4s-modal-icon-close" role="presentation" viewBox="0 0 16 14" width="22" height="22"><path d="M15 0L1 14m14 0L1 0" stroke="currentColor" fill="none" fill-rule="evenodd"></path></svg></button>
                </div>
              </div>
            </div>
            {%- if info.settings.product_list != blank -%}
