                {%- for media in product.media limit: 2 -%}
                  {%- if media.media_type == 'image' -%}
                  <div class="t4s-col-item prd-img">
                    {%- assign width = media.preview_image.width | default: 426.00 -%}{%- assign height = media.preview_image.height | default: 600.00 | plus: 0.00 -%}
                    {%- assign width = 426.00 -%}{%- assign height = 600.00 | plus: 0.00 -%}
                    <a href="{{ product.url }}" class="t4s-img__banner" style="padding-bottom: {{ height | divided_by: width | times: 100.00000000 }}%;">
                      {{- media.preview_image | image_url: width: media.preview_image.width | image_tag: class: 't4s-img-fit-cover', alt: media.preview_image.alt, size: '100vw', widths: '385, 550, 750, 1100, 1500', loading: 'lazy' -}}
                    </a>
                  </div>
                  {%- endif -%}
                {%- endfor -%}
                {%- endfor -%}
              </div>
            </div>
            {%- endif -%}
            <div class="prd-short-desc">
              {{- info.settings.desc_f -}}
            </div>
          </div>
          <div class="t4s-col-item t4s-col-12">
            <div class="prd-collection">
