<!-- collection-with-banner.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-collection-products.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
{{ 't4s-banner.css' | asset_url | stylesheet_tag }}
<link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
<link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
<link href="{{ 't4s-loading.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
  assign sid = section.id
  assign stt_layout = section.settings.layout
  assign stt_image_bg = section.settings.image_bg

  if stt_layout == 't4s-se-container' 
   assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
  elsif stt_layout == 't4s-container-wrap'
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif

  assign arr_item = section.blocks | where: "type", 'b_item'
  assign image_ratio = section.settings.image_ratio
  if image_ratio == "ratioadapt"
    assign imgatt = ''
   else 
    assign imgatt = 'data-'
  endif
  assign collection = collections[section.settings.collection]
  assign show_vendor = section.settings.show_vendor
  assign use_link_vendor = settings.use_link_vendor

  assign enable_rating = settings.enable_rating
  assign limit = section.settings.limit
  assign product_des = section.settings.product_des

  assign show_img = settings.show_img
  assign isGrowaveWishlist = false
  if settings.wishlist_mode == "3" and shop.customer_accounts_enabled
    assign isGrowaveWishlist = true
  endif
  assign placeholder_img = settings.placeholder_img
  assign enable_pr_size = settings.enable_pr_size
  assign show_size_type = settings.show_size_type
  assign size_ck = settings.size_ck | append: ',size,sizes,Größe' 
  assign get_size = size_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

  assign enable_pr_color = settings.enable_pr_color
  assign show_cl_type = settings.show_color_type
  assign color_ck = settings.color_ck | append: ',color,colors,couleur,colour'
  assign get_color = color_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

  assign price_varies_style = settings.price_varies_style
  assign app_review = settings.app_review
  assign use_countdown = section.settings.use_cdt
  
  if section.settings.btn_owl == "simple"
    assign arrow_icon = 1
  else
    assign arrow_icon = 2
  endif
  assign show_btn = section.settings.show_btn
  if show_btn != "hidden"
    assign slider_btns = true
  else
    assign slider_btns = false
  endif
  assign show_dots = section.settings.show_dots
  if show_dots != "hidden"
    assign slider_dots = true
  else
    assign slider_dots = false
  endif
  assign countdown = false
  assign custom_button = false
  assign newsletter = false
  assign general_block = false
  assign no_image_parent = true
 -%}
{% render 'section_style', sid: sid %}
{%- # theme-check-disable DeprecateBgsizes -%}
<div class="t4s-section-inner t4s_se_{{ sid }} {{ stt_layout }} {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%} t4s-has-imgbg lazyloadt4s {%- endif -%}"  {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%}  >
  {{- html_layout[0] -}}
  {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner {%- if stt_image_bg != blank -%} t4s-has-imgbg lazyloadt4s {%- endif -%} "  {%- if stt_image_bg != blank -%}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%} > {%- endif -%}
    <div data-collection-url="{{ collection.url }}" class="t4s-collec-{{ section.settings.coll_pos }} t4s-row {% if arr_item.size > 0 %} t4s-row-cols-lg-2 t4s-row-cols-md-2 t4s-row-cols-1 {% else %} t4s-row-cols-1 {% endif %} t4s-gx-lg-{{ section.settings.space_h_item }} t4s-gy-lg-{{ section.settings.space_v_item }} t4s-gx-md-{{ section.settings.space_h_item_tb }} t4s-gy-md-{{ section.settings.space_v_item_tb }} t4s-gx-{{ section.settings.space_h_item_mb }} t4s-gy-{{ section.settings.space_v_item_mb }}">
      {%- for block in section.blocks %}
        {% assign block_type = block.type -%}
        {%- if block_type != 'b_item' and no_image_parent %}{% continue -%}
        {%- elsif block_type == 'b_item' -%} 
          {%- liquid
            if is_prev_block_image
                echo '</div></div></div></div>'
            endif
            assign no_image_parent = false
            assign is_prev_block_image = true
            assign image_mb = block.settings.image_mb
            assign image = block.settings.image | default: image_mb
            assign image_mb = image_mb | default: image
            assign b_link = block.settings.b_link
            assign bg_content_op = block.settings.bg_content_op | divided_by: 100.0 
            assign bg_content = block.settings.bg_content_cl | color_modify: 'alpha', bg_content_op 
            assign bg_opacity = block.settings.bg_opacity | divided_by: 100.0
            assign bg_overlay = block.settings.bg_overlay | color_modify: 'alpha', bg_opacity
            assign br_opacity = block.settings.br_opacity | divided_by: 100.0
            assign br_bg = block.settings.br_bg | color_modify: 'alpha', br_opacity   
            assign ani_delay = 0   
            assign percent_delay = block.settings.animation_delay | divided_by: 100.0
            assign time_ani_delay = block.settings.time_animation | times: percent_delay  
         -%}
          <div class="t4s-isotopet4s-item t4s-col-item t4s-banner-wrap t4s_{{ block.settings.image_size }} t4s-col-lg-{{ block.settings.col_dk }} t4s-col-md-{{ block.settings.col_tb }} t4s-col-{{ block.settings.col_mb }} bl-{{ block.id }}" {% if block.settings.col_dk == 'custom' or block.settings.col_tb == 'custom' %} style="--t4s-cus-col-lg:{{ block.settings.col_dk_w | remove: ' ' }}%;--t4s-cus-col-tb:{{ block.settings.col_dk_w_tb | remove: ' ' }}%;"{% endif %}>
            <div data-t4s-animate class="t4s-banner-item t4s-eff t4s-eff-{{ block.settings.b_effect }} t4s-eff-img-{{ block.settings.img_effect }} t4s_ratio_mix t4s_ratio_cuspx_mb_{{ block.settings.custom_mb }} t4s_ratio_cuspx_tb_{{ block.settings.custom_tb }} t4s_ratio_cuspx_{{ block.settings.custom_dk }}" style="--ratioapt: {{ image.aspect_ratio | default: 1.5 }};--ratioaptmb: {{ image_mb.aspect_ratio | default: 1.5 }};--aspect-ratio-cus : {{ block.settings.height_dk }}px;--aspect-ratio-custb : {{ block.settings.height_tb }}px;--aspect-ratio-cusmb : {{ block.settings.height_mb }}px;">
              <div class="t4s-banner-inner" style="--bg-overlay: {{ bg_overlay }};">
                {%- liquid 
                  if b_link != blank 
                    assign ARRhtml = 'a,,' | split: ','
                  else
                    assign ARRhtml = 'div,data-,data-' | split: ','
                  endif
               -%}
                {%- if block.settings.bg_content_bl -%}
                  {%- capture append_bg_content_style -%}
                    --bg-content: {{ bg_content }};--content-pd: {{ block.settings.content_pd_tb }}px {{ block.settings.content_pd_lr }}px;--content-pd-mb: {{ block.settings.content_pd_tb_mb }}px {{ block.settings.content_pd_lr_mb }}px;
                  {%- endcapture -%}
                {%- endif -%}
                {%- if block.settings.border_bl -%}
                  {%- capture append_bg_border_style -%}
                    --br-color: {{ block.settings.br_color }};--br-style: {{ block.settings.br_style }};--br-pd: {{ block.settings.br_pd }}px;--br-pd-mb: {{ block.settings.br_pd_mb }}px;--border-bg: {{ br_bg }};
                  {%- endcapture -%}
                {%- endif -%}
                <{{ ARRhtml[0] }} {{ ARRhtml[1] }}href="{{ block.settings.b_link }}"  {{ ARRhtml[2] }}target="{{ block.settings.open_link }}" class="t4s-d-block t4s_ratio t4s_ratio_hasmb t4s-bg-pos-{{ block.settings.image_position }}">
                  {%- if image != blank -%}
                    <img {% if image_mb.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{image_mb.presentation.focal_point}}"{% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff t4s-d-md-none" data-src="{{ image_mb | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image_mb.width }}" height="{{ image_mb.height }}" alt="{{ image_mb.alt | escape }}" loading="lazy">
                    <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none" style="background: url({{ image_mb | image_url: width: 1 }})"></span>
                    <img {% if image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{image.presentation.focal_point}}"{% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff t4s-d-none t4s-d-md-block" data-src="{{ image | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                    <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: url({{ image | image_url: width: 1 }})"></span>
                  {%- else -%}
                    {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                  {%- endif -%}
                </{{ ARRhtml[0] }}>
                <div class="t4s-banner-content t4s-content-position t4s-pa {{ block.settings.content_width }} t4s-text-lg-{{ block.settings.content_align }} t4s-text-md-{{ block.settings.content_align_tb }} t4s-text-{{ block.settings.content_align_mb }} t4s-bg-content-{{ block.settings.bg_content_bl }} t4s-br-content-{{ block.settings.border_bl }} t4s-br-style-{{ block.settings.br_style }}" style="--time-animation: {{ block.settings.time_animation }}s; {%- render 'content_position', ch_pos: block.settings.ch_pos, cv_pos: block.settings.cv_pos, ch_pos_tb: block.settings.ch_pos_tb, cv_pos_tb: block.settings.cv_pos_tb, ch_pos_mb: block.settings.ch_pos_mb, cv_pos_mb: block.settings.cv_pos_mb, append_bg_content_style: append_bg_content_style, append_bg_border_style: append_bg_border_style -%}">
                {%- liquid
                if forloop.last
                  echo '</div></div></div></div>'
                endif -%}
        {%- else -%} 
                {%- liquid
                assign next_block = section.blocks[forloop.index]
                assign is_prev_block_image = false -%}
                {%- case block_type -%}
                  {%- when 'custom_text' -%}
                    {% if block.settings.text != blank %}
                      {%- assign general_block = true -%}
                      <{{ block.settings.tag }} data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" class="t4s-bl-item t4s-pe-none t4s-animation-{{ block.settings.animation }} t4s-text-bl t4s-fnt-fm-{{ block.settings.fontf }} t4s-font-italic-{{ block.settings.font_italic }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-hidden-br-mb-{{ block.settings.remove_br_tag }} t4s-text-shadow-{{ block.settings.text_shadow }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_text', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</{{ block.settings.tag }}>
                    {% endif %}
                  {%- when 'space_html' -%}
                    {%- assign general_block = true -%}
                    <div class="t4s-space-html t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" {%- render 'bk_cus_style', type: 'space_html', block: block, ani_delay: ani_delay -%}></div>
                  {%- when 'html' -%}
                    {% if block.settings.html_content != blank %}
                      {%- assign general_block = true -%}
                        <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-raw-html t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-rte--list" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'html', block: block, ani_delay: ani_delay -%}>{{ block.settings.html_content }}</div>
                      {% endif %}
                  {%- when 'custom_label' -%}
                    {% if block.settings.text != blank %}
                      {%- assign general_block = true -%}
                        <div data-lh-lg="{{ block.settings.text_lh }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-custom-label t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_label', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</div>
                      {% endif %}
                  {%- when 'image' -%}
                    {%- assign image = block.settings.image_child -%}
                    {%- if block.settings.img_link != blank -%}
                      {%- assign image_parent = 'a,' | split: ',' -%}
                    {%- else -%}
                      {%- assign image_parent = 'div,data-' | split: ',' -%}
                    {%- endif -%}
                    {%- if image != blank -%}
                        {%- assign general_block = true -%}        
                        <{{ image_parent[0] }} {{ image_parent[1] }}href="{{ block.settings.img_link }}" class="t4s-bl-item t4s-img-child t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', block: block, ani_delay: ani_delay -%}>
                            <img class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                        </{{- image_parent[0] -}}>
                    {%- else -%}
                      {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                    {%- endif -%}
                  {%- when "countdown" -%}
                    {%- if block.settings.date != blank -%}
                    {%- assign countdown = true -%}
                      <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-countdown sepr_coun_dt_wrap t4s-countdown-des-{{ block.settings.cdt_des }} t4s-countdown-size-{{ block.settings.cdt_size }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'countdown', block: block, ani_delay: ani_delay -%}>
                        <div class="t4s-time" data-countdown-t4s data-date='{{ block.settings.date }}' data-keyid='#countdown-{{ sid }}'></div>
                      </div>
                    {% endif %}
                  {%- when 'custom_button' -%}
                    {%- if block.settings.button_link != blank and block.settings.button_text != blank -%}
                      {%- assign custom_button = true -%}
                      {%- assign  button_style = block.settings.button_style -%}
                      <a href="{{ block.settings.button_link }}" target="{{ block.settings.target_link }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-btn t4s-btn-custom  t4s-pe-auto t4s-fnt-fm-{{ block.settings.fontf }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' %}t4s-btn-effect-{{ block.settings.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_button', block: block, ani_delay: ani_delay -%}>{{ block.settings.button_text }}
                      {%- if block.settings.button_icon_w > 0 -%}
                        {%- if block.settings.icon_style == "1" -%}
                          {% comment %} <svg class="t4s-btn-icon"  width="16" height="11" viewBox="0 0 16 11" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <rect y="4" width="12" height="3" />
                            <path d="M15.0434 5.50985L11.3245 9.44864L11.2626 1.63039L15.0434 5.50985Z"/>
                          </svg> {% endcomment %}
                          <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="18" height="18"><path d="M 21.1875 9.28125 L 19.78125 10.71875 L 24.0625 15 L 4 15 L 4 17 L 24.0625 17 L 19.78125 21.28125 L 21.1875 22.71875 L 27.90625 16 Z"/></svg>
                        {%- elsif block.settings.icon_style == "2" -%}
                            <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>   
                        {% else %}
                          <svg class="t4s-btn-icon" version="1.1" x="0px" y="0px" width="22px" height="22px" viewBox="0 0 22 22" style="enable-background:new 0 0 22 22;" xml:space="preserve"><polygon points="6,20 6,2 17,11 "></polygon></svg>  
                        {%- endif -%}
                    {%- endif -%}
                      </a>
                    {%- endif -%}
                  {%- when 'newsletter' -%}
                      {%- assign newsletter = true -%}
                      {%- assign custom_button = true -%}
                      <div id="b_{{ block.id }}" class="t4s-newsletter-parent t4s-pe-auto t4s-newsletter-design-{{ block.settings.newsletter_design }} t4s-custom-width-{{ block.settings.custom_width }} t4s_newsletter_se t4s-has-btn-{{ block.settings.source_btn }}" {%- render 'bk_cus_style', type: 'newsletter', block: block -%}>
                        {%- render 'newsletter', form_id: block.id, source_btn: block.settings.source_btn, buttonIcon: block.settings.news_icon, btn_content: block.settings.button_content -%}
                      </div>
                {%- endcase -%}
                {%- if block.settings.animation != 'none' -%}
                  {% assign ani_delay = ani_delay | plus: time_ani_delay -%}
                {%- endif -%}
              {%- if forloop.last or next_block and next_block.type == 'b_item' -%}
                </div></div></div></div>
              {%- endif -%}
        {%- endif -%}
      {%- endfor -%}
        <div class="t4s-collection-col t4s-col-item">
          {%- render 'section_tophead', get_height: true, sid: sid, slider_btns: slider_btns -%}
          {%- if section.settings.layout_des == "1" -%} 
            {{ 't4s-button-style.css' | asset_url | stylesheet_tag }}
            <link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
            <div data-collection-url="{{ collection.url }}" data-contentlm-replace class="t4s_box_pr_grid t4s-products t4s-justify-content-center {% if section.settings.enable_featured_product %}t4s-has-featured-pr {% endif %} t4s-border-pr-{{ section.settings.enable_border_pr_item }} t4s-text-{{ section.settings.content_align }} t4s_{{ image_ratio }} t4s_position_{{ section.settings.image_position }} t4s_{{ section.settings.image_size }} t4s-row t4s-row-cols-lg-{{ section.settings.col_dk }} t4s-row-cols-md-{{ section.settings.col_tb }} t4s-row-cols-{{ section.settings.col_mb }} t4s-gx-lg-{{ section.settings.space_h_item }} t4s-gy-lg-{{ section.settings.space_v_item }} t4s-gx-md-{{ section.settings.space_h_item_tb }} t4s-gy-md-{{ section.settings.space_v_item_tb }} t4s-gx-{{ section.settings.space_h_item_mb }} t4s-gy-{{ section.settings.space_v_item_mb }}">
           {%- else -%} 
            {{ 't4s-slider-settings.css' | asset_url | stylesheet_tag }}
            {{ 'pre_flickityt4s.min.css' | asset_url | stylesheet_tag }}
            {{ 'pre_flickity_custom.css' | asset_url | stylesheet_tag }}
            {%- liquid
              assign icon_slider = section.settings.icon_slider 
              case icon_slider
                when '1'
                  assign view_box = "0 0 22 22"
                when '2'
                  assign view_box = "0 0 22 22"
                when '3'
                  assign view_box = "0 0 28 18"
              endcase
           -%}
            <div data-collection-url="{{ collection.url }}" data-t4s-resizeobserver class="t4s_box_pr_slider t4s-products has-product-style{{ product_des }} t4s-box-border-{{ section.settings.enable_box_border }} t4s-text-{{ section.settings.content_align }} t4s_{{ image_ratio }} t4s_position_{{ section.settings.image_position }} t4s_{{ section.settings.image_size }} t4s-row t4s-row-cols-lg-{{ section.settings.col_dk }} t4s-row-cols-md-{{ section.settings.col_tb }} t4s-row-cols-{{ section.settings.col_mb }} t4s-gx-lg-{{ section.settings.space_h_item }} t4s-gy-lg-{{ section.settings.space_v_item }} t4s-gx-md-{{ section.settings.space_h_item_tb }} t4s-gy-md-{{ section.settings.space_v_item_tb }} t4s-gx-{{ section.settings.space_h_item_mb }} t4s-gy-{{ section.settings.space_v_item_mb }} {% if slider_btns %} t4s-slider-btn__{{ section.settings.btn_pos }}-content t4s-slider-btn-{{ show_btn }} t4s-slider-btn-style-{{ section.settings.btn_owl }} t4s-slider-btn-{{ section.settings.btn_shape }} t4s-slider-btn-{{ section.settings.btn_size }} t4s-slider-btn-cl-{{ section.settings.btn_cl }} t4s-slider-btn-vi-{{ section.settings.btn_vi }} {% endif %}
            {% if slider_dots %} t4s-dots-style-{{ section.settings.dot_owl }} t4s-slider-dots-{{ show_dots }} t4s-base-dots-{{ section.settings.dots_pos }} t4s-dots-cl-{{ section.settings.dots_cl }} t4s-dots-round-{{ section.settings.dots_round }} {% endif %} t4s-flickity-slider flickityt4s" data-flickityt4s-js='{"customIcon":{{ icon_slider }}, "viewBox":"{{ view_box }}", "setPrevNextButtons": true,"arrowIcon": "{{ arrow_icon }}","imagesLoaded": 0,"adaptiveHeight": 1, "contain": 1, "groupCells": "100%", "dragThreshold" : 5, "cellAlign": "left","wrapAround": {{ section.settings.loop }},"prevNextButtons": {{ slider_btns }},"percentPosition": 1,"pageDots": {{ slider_dots }}, "autoPlay" : {{ section.settings.au_time | times: 1000 }}, "pauseAutoPlayOnHover" : {{ section.settings.au_hover }}, "t4sid": "{{ sid }}","navUI": 1,"cellSelector": ".t4s-product"}' style="--btn-distance: {{ section.settings.btn_distance }}px;--space-dots: {{ section.settings.dots_space }}px;--dots-bottom-pos: {{ section.settings.dots_bottom_pos }}px;">
         {%- endif -%} 
            {%- if collection != blank -%}
              {%- case product_des -%}
