{%- liquid 
  assign sid       = section.id
  assign design_mode = request.design_mode
  assign loadContent = false
  if content_for_header contains '\u0026section_id=' or content_for_header contains '?section_id='
      assign loadContent = true
  endif
  assign sidebar_size     = section.settings.size
  assign isSidebarDrawer  = section.settings.enable_drawer 

 -%}  
{%- if loadContent or design_mode -%}
  <template class="t4s-d-none">
  [t4splitlz]
      {%- if section.blocks.size > 0 -%}
        <link rel="stylesheet" href="{{ 't4s-widget.css' | asset_url }}" media="all">
        <div class="t4s-sidebar-inner t4s-sidebar-collection t4s-current-scrollbar t4s-drawer-{{ isSidebarDrawer }} t4s-heading-transform-{{ section.settings.heading_uppercase }}"> 
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'category' -%}
                  <div id="t4s-sidebar-{{ block.id }}" class="t4s-widget t4s-widget-category">
                    {%- if block.settings.heading != blank -%}
                        <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                    {%- endif -%}  
                    <ul id="b_{{ block.id }}" class="t4s-product-categories t4s-cat-count-{{ block.settings.count }} t4s-current-scrollbar">
                        {%- case block.settings.cat_source -%}
                           {%- when '1' -%}
                              {%- if collections.size > 0 -%}
                                 {%- for category in collections -%}
                                    {%- if category.products.size > 0 -%}
                                      <li class="t4s-cat-item{%- if collection.handle == category.handle %} t4s-current-cat{%- endif -%}"><a href="{{ category.url }}">{{ category.title }}
                                      {% if block.settings.count %}  
                                        <span class="t4s-value-count">({{ category.all_products_count }})</span>
                                      {%- endif -%}</a> 
                                      </li>
                                    {%- endif -%}
                                 {%- endfor -%}
                              {%- endif -%}
                           {%- else -%}
                              {%- assign top_link_list = block.settings.cat_link_list -%}
                              {%- for link in linklists[top_link_list].links -%}
                                  {%- if link.type == 'collection_link' or link.type == 'catalog_link' -%}
                                    {%- if link.links != blank -%}
                                         <li class="t4s-pr menu_nested t4s-cat-item{%- if link.active or link.child_active %} t4s-current-cat{%- endif -%}">
                                            <a class="has_cats_toggle" href="{{ link.url }}">{{ link.title }} {%- if block.settings.count == true -%} <span class="t4s-value-count">({{ link.object.all_products_count | default: shop.products_count }})</span>{%- endif -%}</a>
                                            <ul class="t4s-sub-menu t4s-children">
                                               {%- for child_link in link.links -%}
                                                   {%- if child_link.type == 'collection_link' or child_link.type == 'catalog_link' -%}
                                                   
                                                     {%- if child_link.links != blank -%}
                                                     <li class="t4s-pr menu_nested2 t4s-cat-item{%- if child_link.active or child_link.child_active %} t4s-current-cat{%- endif -%}">
                                                        <a class="t4s-cat-count" href="{{ child_link.url }}">{{ child_link.title }} 
                                                          {%- if block.settings.count == true -%}  
                                                            <span class="t4s-value-count">({{ child_link.object.all_products_count | default: shop.products_count }})</span>
                                                          {%- endif -%}
                                                        </a>
                                                        <ul class="t4s-sub-menu t4s-children">
                                                          {%- for third_link in child_link.links -%}
                                                           {%- if third_link.type == 'collection_link' or third_link.type == 'catalog_link' -%}
                                                           <li class="t4s-cat-item{%- if third_link.active %} t4s-current-cat{%- endif -%}">
                                                              <a href="{{ third_link.url }}">{{ third_link.title }} {%- if block.settings.count == true -%} <span class="t4s-value-count">({{ third_link.object.all_products_count | default: shop.products_count }})</span> {%- endif -%}</a>
                                                           </li>
                                                           {%- endif -%}
                                                          {%- endfor -%}
                                                        </ul>
                                                     </li>
                                                     {%- else -%}
                                                     <li class="t4s-cat-item{%- if child_link.active %} t4s-current-cat{%- endif -%}">
                                                        <a href="{{ child_link.url }}">{{ child_link.title }} {%- if block.settings.count == true -%} <span class="t4s-value-count">({{ child_link.object.all_products_count | default: shop.products_count }})</span> {%- endif -%}</a>
                                                     </li>
                                                     {%- endif -%}

                                                   {%- endif -%}
                                                {%- endfor -%}
                                            </ul>
                                          </li>
                                    {%- else -%} 
                                      <li class="t4s-cat-item{%- if link.active %} t4s-current-cat{%- endif -%}"><a href="{{ link.url }}">{{ link.title }} {%- if block.settings.count == true -%} <span class="t4s-value-count">({{ link.object.all_products_count | default: shop.products_count }})</span>{%- endif -%}</a></li>
                                    {%- endif -%}
                                  {%- endif -%}
                              {%- endfor -%}
                        {%- endcase -%}
                    </ul>
                  </div>
                {%- when 'instagram' -%}
                   {%- liquid
                     assign limit = block.settings.limit
                     if block.settings.mode == '1' 
                       assign acc = block.settings.acc | default: settings.global_acc
                     else
                       assign acc = 'ins_19041994'
                     endif -%}

                   <div id="t4s-sidebar-{{ block.id }}" class="t4s-widget t4s-widget-instagram">
                      {%- if block.settings.heading != blank -%}
                        <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                      {%- endif -%}   
                      <div id="b_{{ block.id }}" class="t4s-row t4s_ratio1_1 t4s_position_8 t4s_cover t4s-row-cols-{{ block.settings.col_dk }} t4s-row-cols-md-{{ block.settings.col_dk }} t4s-row-cols-lg-{{ block.settings.col_dk }} t4s-gx-{{ block.settings.space_item }} t4s-gy-{{ block.settings.space_item }}" data-inst4s-options='{ "id": "{{ block.settings.col_dk }}{{ block.settings.col_tb }}{{ block.settings.col_mb }}{{ block.settings.limit }}{{ block.settings.open_link }}","limit": {{ limit }}, "acc": {{ acc | base64_encode | json }}, "target": "{{ block.settings.open_link }}" }'><div class="t4s-loading--bg"></div></div>
                      <template class="t4s-icons-ins-svg"><svg class="t4s-svg-ins-image" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="36" height="36" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"><path d="M22,15.5,18.55,11a2,2,0,0,0-3.14,0L12,15.27a2,2,0,0,1-3,.18l-1.1-1.07a2,2,0,0,0-2.81,0L2,17.5V20a2,2,0,0,0,2,2H20a2,2,0,0,0,2-2Z"></path><rect x="2" y="2" width="20" height="20" rx="2"></rect><line x1="6.99" y1="7" x2="7" y2="7" stroke-linecap="round" stroke-width="2.5"></line></svg>[t4split]<svg class="t4s-svg-ins-video" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="36" height="36" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"><rect x="2" y="5" width="14" height="14" rx="2"></rect><path d="M16,10l4.55-2.28a1,1,0,0,1,1.45.9v6.76a1,1,0,0,1-1.45.9L16,14" stroke-linecap="round"></path></svg>[t4split]<svg class="t4s-svg-ins-album" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="36" height="36" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"><rect x="2" y="2" width="14" height="14" rx="2"></rect><path d="M20,22H10a2,2,0,0,1-2-2V16h6a2,2,0,0,0,2-2V8h4a2,2,0,0,1,2,2V20A2,2,0,0,1,20,22Z"></path></svg></template>
                      {%- style -%}
                        #t4s-sidebar-{{ block.id }} .t4s-col-ins a{border-radius: {{ block.settings.round }} %} 
                      {%- endstyle -%}
                   </div>
                {%- when 'text' -%}
                  <div class="t4s-widget t4s-widget-content-page">
                      {%- if block.settings.heading != blank -%}
                        <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                      {%- endif -%}  
                      <div id="b_{{ block.id }}" class="t4s-widget-inner">
                          {%- if block.settings.text_html != blank -%}{{ block.settings.text_html }}{%- endif -%}
                      </div>
                  </div>
                {%- when 'html' -%}
                  {%- if block.settings.html_content != blank -%}
                      <div class="t4s-widget t4s-widget-html t4s-rte--list">
                        {%- if block.settings.heading != blank -%}
                          <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                        {%- endif -%}  
                        <div id="b_{{ block.id }}" class="t4s-raw-html" {%- render 'bk_cus_style', type: 'html', block: block -%}>{{ block.settings.html_content }}</div>
                        </div>
                    {%- endif -%}
                {%- when 'list_products' -%}
                  {%- liquid 

                  assign product_list = block.settings.product_list
                  assign placeholder_img = settings.placeholder_img
                  if block.settings.image_ratio == "t4s_ratioadapt"
                    assign imgatt = ''
                  else 
                    assign imgatt = 'data-'
                  endif 
                  assign limit = block.settings.limit
                  
                  -%}
                  <div id="t4s-sidebar-{{ block.id }}" class="t4s-widget t4s-sidebar-product-feature t4s-sidebar-{{ sidebar_size }}">
                    {%- if block.settings.heading != blank -%}
                      <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                    {%- endif -%}    
                    {%- if product_list == blank -%}
                      <p class="t4s-pr-no-content">{{ 'collections.general.no_matches' | t }}</p>
                    {%- else -%}
                      {{ 't4s-collection-group.css' | asset_url | stylesheet_tag }}
                      <div class="t4s_product_list_widget {{ block.settings.image_ratio }} {{ block.settings.image_position }} {{ block.settings.image_size }} t4s-pr-group">
                        {%- for product in product_list limit: block.settings.limit_pr -%}
                            {%- render 'product_group_item', product: product, imgatt: imgatt, show_list_btns: false, placeholder_img: placeholder_img -%}  
                        {%- endfor -%}
                      </div>
                    {%- endif -%}
                  </div>
                {%- when 'shipping' -%}
                  <div id="t4s-sidebar-{{ block.id }}" class="t4s-widget t4s-widget-shipping t4s-border-shipping-{{ block.settings.border_shipping }} t4s-align-content_shipping-{{ block.settings.align_shipping }}">
                    {%- if block.settings.heading != blank -%}
                      <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                    {%- endif -%}  
                    <div id="b_{{ block.id }}" class="t4s-shipping-list">
                      {%- for i in (1..6) -%}
                        {%- liquid
                          assign shipping = 'shipping_' | append: i
                          assign shipping_id = block.settings[shipping] -%}
                        {%- if shipping_id != blank -%}
                            {%- assign shipping = shipping_id | replace: ' |', '|' | replace: '| ', '|' | split: '|' -%}
                            <div class="t4s-row t4s-gx-0 t4s-gy-0 t4s-text-left t4s-space-item-inner" >
                                {%- if shipping[0] != blank -%}
                                    <div class="t4s-widget-shipping-icon t4s-col-item icon t4s-col-auto">
                                        <i class="{{ shipping[0] }}"></i>
                                    </div>
                                {%- endif -%}
                                <div class="t4s-col-item t4s-col">
                                    <h4 class="t4s-widget-shipping-title">{{ shipping[1] }}</h4>
                                    <p class="t4s-widget-shipping-desc">{{ shipping[2] }}</p>
                                </div>
                            </div>
                        {%- endif -%}
                      {%- endfor -%}
                    </div>
                  </div>
                {%- when 'image' -%}
                  <div id="t4s-sidebar-{{ block.id }}" class="t4s-col-item t4s-widget t4s-widget-image t4s-sidebar-{{ sidebar_size }}">
                    {%- if block.settings.heading != blank -%}
                      <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                    {%- endif -%}    
                    {%- assign image = block.settings.image_sidebar -%}
                    {%- if image -%}
                      <div id="b_{{ block.id }}"  class="t4s-image t4s-pr t4s-oh t4s-pos-{{ block.settings.content_pos }} t4s-eff-img-{{ block.settings.img_effect }}">
                        {%- if block.settings.b_link != blank -%}<a href="{{ block.settings.b_link }}" target="{{ block.settings.open_link }}">{%- endif -%}
                          <img class="lazyloadt4s t4s-obj-eff" data-src="{{ image | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                          {%- if block.settings.text_content != blank or block.settings.btn_content != blank -%}
                              {% comment %}  them btn + content {% endcomment %}
                          {%- endif -%}
                        {%- if block.settings.b_link != blank -%}</a>{%- endif -%}
                      </div>
                    {%- endif -%}
                  </div>
                {%- when 'gallery' -%}
                  {%- liquid
                      assign open_link = block.settings.open_link
                      assign ARRhtml1 = 'a,,' | split: ','
                      assign ARRhtml2 = 'div,data-' | split: ',' -%}
                  <div id="t4s-sidebar-{{ block.id }}" class="t4s-widget t4s-widget-gallery">
                      {%- if block.settings.heading != blank -%}
                        <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                      {%- endif -%}    
                      <div id="b_{{ block.id }}"  class=" t4s_position_8 t4s_cover ">
                        <div class="t4s-row t4s_ratio1_1 t4s-row-cols-{{ block.settings.col_dk }} t4s-row-cols-md-{{ block.settings.col_dk }} t4s-row-cols-lg-{{ block.settings.col_dk }} t4s-gx-{{ block.settings.space_item }} t4s-gy-{{ block.settings.space_item }}">
                          {%- for i in (1..9) -%}
                              {%- liquid
                                  assign image_list = 'img' | append: i 
                                  assign image_url_list = 'link' | append: i 
                                  assign url = block.settings[image_url_list]
                                  assign image = block.settings[image_list] 
                                  if url == blank 
                                      assign ARRhtml = ARRhtml2
                                  else 
                                      assign ARRhtml = ARRhtml1  
                                  endif  
                              -%}                                               
                              {%- if image != blank -%}
                                  <div class="t4s-col-item t4s-gallery-item">
                                      <{{ ARRhtml[0] }} {{ ARRhtml[1] }}href="{{ url }}" {{ ARRhtml[2] }}target="{{ open_link }}" class="t4s-effect t4s-pr t4s-oh t4s-d-block">
                                          <div class="t4s_ratio" style="background: url({{ image | image_url: width: 1 }});">
                                            <img class="lazyloadt4s" data-src="{{ image | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                                          </div>
                                      </{{ ARRhtml[0] }}>
                                  </div>  
                              {%- endif -%}                                            
                          {%- endfor -%}
                        </div>
                      </div>
                      {%- style -%}
                          #t4s-sidebar-{{ block.id }} .t4s-gallery-item .t4s_ratio{border-radius: {{ block.settings.round }}%} 
                      {%- endstyle -%}
                  </div>
                {%- when 'cus_socials' -%}
                  {{ 't4s-icon-social.css' | asset_url | stylesheet_tag }}
                  {%- liquid
                            assign pri_cl_lightness       = block.settings.pri_cl | color_extract: 'lightness'
                            assign pri_hover_cl_lightness       = block.settings.pri_hover_cl | color_extract: 'lightness'
                            if pri_cl_lightness < 85
                            assign pri_cl2 = "#fff"
                            else 
                            assign pri_cl2 = "#000"
                            endif
                            if pri_hover_cl_lightness < 85
                            assign pri_hover_cl2 = "#fff"
                            else 
                            assign pri_hover_cl2 = "#000"
                            endif
                        -%} 
                  <div id="b_{{ block.id }}" class="t4s-socials-block t4s-widget t4s-widget-socials social-{{ block.id }}" style="--pri-cl: {{ block.settings.pri_cl }};--pri-cl2: {{ pri_cl2 }};--pri-hover-cl: {{ block.settings.pri_hover_cl }};--pri-hover-cl2: {{ pri_hover_cl2 }};--bd-radius: {{ block.settings.bd_radius }}px;">
                      {%- if block.settings.heading != blank -%}
                        <h5 class="t4s-widget-title">{{ block.settings.heading }}</h5>
                      {%- endif -%}     
                      <div id="b_{{ block.id }}" class="t4s-socials-sidebar">
                      {%- if block.settings.social_mode == '1' -%} 
                          {%- assign follow_social = true -%} 
                      {%- else -%} 
                          {%- assign share_image = settings.share_image | default: page_image | default: settings.logo -%} 
                      {%- endif -%} 
                      {%- render 'social_sharing', style: block.settings.social_style, use_color_set: block.settings.use_color_set, size: block.settings.social_size, space_h_item: block.settings.space_h_item, space_h_item_mb: block.settings.space_h_item_mb, space_v_item: block.settings.space_v_item, space_v_item_mb: block.settings.space_v_item_mb, share_permalink: shop.url, share_title: shop.name, share_image: share_image, follow_social: follow_social, block: block -%} 
                      </div>
                  </div>
                {%- when 'filter' -%}
                  <link rel="stylesheet" href="{{ 't4s-facets.css' | asset_url }}" media="all">
                  <div data-replace-filter class="t4s-widget"></div>
              {%- endcase -%}
            {%- endfor -%}
        </div>
      {%- else -%}
        <span class="t4s-onboarding-info">{{ 'onboarding.no_content' | t }}</span>
      {%- endif -%}
  [t4splitlz]
     <link rel="stylesheet" href="{{ 't4s-drawer.min.css' | asset_url }}" media="all">
     <link rel="stylesheet" href="{{ 't4s-facets.css' | asset_url }}" media="all">
     <div id="drawer-{{ sid }}" class="t4s-drawer t4s-drawer__left t4s-sidebar t4s-sidebar-drawer" aria-hidden="true">
        <div class="t4s-drawer__header"><button class="t4s-drawer__close" data-drawer-close aria-label="{{ 'general.sidebar.close' | t }}">
         <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M310.6 150.6c12.5-12.5 12.5-32.8 0-45.3s-32.8-12.5-45.3 0L160 210.7 54.6 105.4c-12.5-12.5-32.8-12.5-45.3 0s-12.5 32.8 0 45.3L114.7 256 9.4 361.4c-12.5 12.5-12.5 32.8 0 45.3s32.8 12.5 45.3 0L160 301.3 265.4 406.6c12.5 12.5 32.8 12.5 45.3 0s12.5-32.8 0-45.3L205.3 256 310.6 150.6z"/></svg>
          <span class="t4s-close-text">{{ 'general.sidebar.close' | t }}</span></button></div>
        <div class="t4s-drawer__content">
           <div class="t4s-drawer__main t4s-current-scrollbar">[t4splitlz2]</div>
           <div class="t4s-drawer__footer"></div>
        </div>
     </div>     
  [t4splitlz]
  </template>
{%- endif -%}
  <button data-sidebar-id='{{ sid }}' data-sidebar-{{ isSidebarDrawer }} data-drawer-options='{ "id": "#drawer-{{ sid }}" }' class="t4s-btn-sidebar"><span class="t4s-btn-sidebar-icon"><svg viewBox="0 0 24 24" width="22" height="22" stroke="currentColor" stroke-width="1.5" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect><line x1="9" y1="3" x2="9" y2="21"></line></svg></span> <span class="t4s-btn-sidebar-text">{{ 'general.sidebar.open' | t }}</span></button>
  <style>
    button.t4s-btn-sidebar {
        position: fixed;
        top: 50%;
        z-index: 100;
        background-color: var(--t4s-light-color);
        color:  var(--t4s-dark-color);
        box-shadow: 0 0 3px rgb(0 0 0 / 15%);
        transition: all .6s cubic-bezier(.19,1,.22,1);
        display: flex;
        align-items: center;
        flex-direction: row;
        flex-wrap: nowrap;
        overflow: hidden;
        min-width: 50px;
        height: 50px;
        padding: 0;
    }
    .t4s-btn-sidebar .t4s-btn-sidebar-icon {
        width: 50px;
        height: 50px;
        display: inline-flex;
        justify-content: center;
        align-items: center;
    }
    .t4s-btn-sidebar .t4s-btn-sidebar-text {
        padding: 0;
        max-width:0;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        font-weight: 500;
        will-change: padding, max-width;
        transition: padding 0.4s cubic-bezier(.175,.885,.32,1.15),max-width 0.4s cubic-bezier(.175,.885,.32,1.15);
    }
    .t4s-btn-sidebar:hover .t4s-btn-sidebar-text {
        padding-inline-end: 25px;
        max-width:280px;
    }

    #drawer-{{ sid }} .t4s-drawer__main{overflow-x: hidden;padding: 15px;}
    @media (min-width: 1025px) {
      #drawer-{{ sid }} .t4s-drawer__main{padding: 20px 30px;}
    }
    {%- unless isSidebarDrawer %}
    @media screen and (min-width: 1025px) {
        .t4s-sidebar { display: block }
        button.t4s-btn-sidebar,.t4s-section-sidebar{display: none;}
        {%- if sidebar_size == 'small' -%}
        .t4s-main-area {flex: 0 0 auto;width: 83.33333333%; }.t4s-sidebar {flex: 0 0 auto;width: 16.66666667%; }
        {%- else if sidebar_size == 'medium' -%}
        .t4s-main-area {flex: 0 0 auto;width: 70%; }.t4s-sidebar {flex: 0 0 auto;width: 30%; }
        {%- else -%}
        .t4s-main-area {flex: 0 0 auto;width: 66.66666667%; }.t4s-sidebar {flex: 0 0 auto;width: 33.33333333%; }
        {%- endif -%}
    }
    .t4s-section-sidebar ~ .t4s-section-main .t4s-main-area{order: 10}
    .t4s-section-sidebar ~ .t4s-section-main .t4s-sidebar{order: 1}
    .t4s-section-main .t4s-collection-header button[data-col="5"],
    .t4s-section-main .t4s-collection-header button[data-col="6"] {
       display: none;
    }
    .t4s-layout-switch-wrapper .t4s-layout__switch.is--desktop button[data-col="2"] {
      display: inline-flex !important;
    }
    {% endunless -%}
    {% if isSidebarDrawer == false -%}
    /* css border conffig */
    .t4s-sidebar-inner {
      background-color: {{ section.settings.sidebar_bg_color }};
    }
    {% if section.settings.sidebar_pd >= 15 %}
      .t4s-sidebar-inner {
        padding: 15px;
      }
    {% endif %}
    {% if section.settings.sidebar_pd >= 30 %}
