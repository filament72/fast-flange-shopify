{%- liquid
  assign checkFirst        = true
  assign sid = section.id
  if sid == 'mega-menu'
   assign show_code = true
  elsif sid != 'mega-menu' and request.design_mode or request.page_type == 'index' 
   assign show_code = true
  else
   assign show_code = false
  endif
-%}

<div data-section-id="{{ sid }}"></div>
{%- if show_code -%}
  [nt_mega_split1]
  {%- for block in section.blocks -%}
    {%- assign bk_type = block.type -%}
    {%- if checkFirst and bk_type == 'mega' -%}
      {%- assign checkFirst = false -%}
      <div id="t4s-mega-contents{{ block.settings.id }}" data-id="{{ block.settings.id }}" data-blockid="{{ block.id }}" {{ block.shopify_attributes }}>
        <link rel="stylesheet" href="{{ 'mega-menu.css' | asset_url }}" media="all">
    {%- elsif bk_type == 'mega' -%}
      </div>
      <div id="t4s-mega-contents{{ block.settings.id }}" data-id="{{ block.settings.id }}" data-blockid="{{ block.id }}" {{ block.shopify_attributes }}>
        <link rel="stylesheet" href="{{ 'mega-menu.css' | asset_url }}" media="all">
      {%- endif -%}
      {%- case bk_type -%}
        {%- when 'link' -%}
          {%- assign llists = linklists[block.settings.menu].links -%}
