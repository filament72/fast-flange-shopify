                  {%- when '2' -%}
                    {%- render 'article_loop_2' for blog.articles as article, show_blog_title: show_blog_title, block.settings: block.settings, blog_url: blog_url, blog_title: blog_title, by_txt: by_txt, on_txt: on_txt, b_effect: b_effect, img_effect: img_effect, show_date: show_date, show_author: show_author, show_comment: show_comment, show_tags: show_tags, show_content: show_content, show_readmore: show_readmore, show_ic_readmore: show_ic_readmore, date_format: date_format, readmore_txt: readmore_txt, col_dk: col_dk, col_tb: col_tb -%}
