{%- unless section.settings.hidden_footer -%}
  <!-- sections/footer.liquid -->
  {{ 't4s-section.css' | asset_url | stylesheet_tag }}
  {{ 't4s-footer.css' | asset_url | stylesheet_tag }}
  {%- liquid 
    assign stt_layout = section.settings.layout
    assign stt_image_bg = section.settings.image_bg 
    if stt_layout == 't4s-se-container' 
      assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
    elsif stt_layout == 't4s-container-wrap'
      assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
    else
      assign html_layout = '__' | split: '__'
    endif
    assign sticky   = section.settings.sticky
    assign collapse = section.settings.collapse
    assign no_image_parent = true
  -%} 
  {% render 'section_style', sid: section.id %}
  <div class="t4s-section-inner t4s_se_{{ section.id }} t4s_se_{{ section.id }} {{ stt_layout }} t4s-footer-hidden-{{ section.settings.hidden_footer }} {%- if  section.settings.border_top == true -%} t4s-footer-has-border t4s-footer-border-{{ section.settings.border_pos }} {%- endif -%} {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%}  t4s-has-imgbg lazyloadt4s {%- endif -%} "  {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto" data-optimumx="1.5" {% endif %}>
    {{- html_layout[0] -}}
    {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner {% if stt_image_bg != blank %} t4s-has-imgbg lazyloadt4s {% endif %} "  {% if stt_image_bg != blank %}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {% endif %}> {%- endif -%}
    <div class="t4s-footer-wrap t4s-row{% if section.settings.space_between %} t4s-justify-content-lg-between{% endif %} is--footer-sticky-{{ sticky }} is--footer-collapse-{{ collapse }} t4s-ver-center-{{ section.settings.align_vertical }} t4s-heading-design{{ section.settings.heading_des }} t4s-gx-lg-{{ section.settings.space_h_item }} t4s-gx-md-{{ section.settings.space_h_item_tb }} t4s-gx-{{ section.settings.space_h_item_mb }} t4s-gy-lg-{{ section.settings.space_v_item }} t4s-gy-md-{{ section.settings.space_v_item_tb }} t4s-gy-{{ section.settings.space_v_item_mb }}" style="--heading-fs:{{ section.settings.heading_fs }}px;--heading-fw:{{ section.settings.heading_fw }};--heading-lh:{{ section.settings.heading_lh }}px;--heading-ls:{{ section.settings.heading_ls }}px;--heading-cl:{{ section.settings.heading_cl }};--text-fs:{{ section.settings.text_fs }}px;--text-fw:{{ section.settings.text_fw }};--text-ls:{{ section.settings.content_ls }}px;--text-cl:{{ section.settings.text_cl }};--link-cl:{{ section.settings.link_cl }};--link-hover-cl:{{ section.settings.link_hover_cl }};--border-cl:{{ section.settings.border_cl }};--heading-mgb:{{ section.settings.mgb }}px;--heading-mgb-tb:{{ section.settings.mgb_tb }}px;--heading-mgb-mb:{{ section.settings.mgb_mb }}px;">   
      {%- for block in section.blocks %}
        {% assign block_type = block.type -%}
        {%- if block_type != 'bl_col' and no_image_parent %}{% continue -%}
        {%- elsif block_type == 'bl_col' -%} 
          {%- liquid
            if is_prev_block_image
              echo '</div></div></div>'
            endif
            assign no_image_parent = false
            assign is_prev_block_image = true         
            
            assign pd_item = block.settings.padding_inner | remove: ' ' | split: ','
            assign pd_item_tb = block.settings.padding_inner_tb | remove: ' ' | split: ','
            assign pd_item_mb = block.settings.padding_inner_mb | remove: ' ' | split: ','
          -%}
          <div class="t4s-custom-col t4s-col-item t4s-columns-border-{{ block.settings.col_border }} t4s-col-lg-{{ block.settings.col_dk }} t4s-col-md-{{ block.settings.col_tb }} t4s-col-{{ block.settings.col_mb }} bl-{{ block.id }}"{% if block.settings.col_dk == 'custom' or block.settings.col_tb == 'custom' %} style="--t4s-cus-col-lg:{{ block.settings.col_dk_w | remove: ' ' }}%;--t4s-cus-col-md:{{ block.settings.col_dk_w_tb | remove: ' ' }}%;"{% endif %}>
            <div class="t4s-col-inner {{ block.settings.content_align }} {{ block.settings.content_align_tb }} {{ block.settings.content_align_mb }} {%- if block.settings.bg_image != blank -%}  lazyloadt4s t4s-has-imgbg  {%- endif -%} "  {%- if block.settings.bg_image != blank -%}  data-bgset="{{ block.settings.bg_image | image_url: width: 1 }}" data-sizes="auto" data-optimumx="1.5" {%- endif -%} style="--pd-top: {{ pd_item[0] | default: 0 }};--pd-right: {{ pd_item[1] | default: 0 }};--pd-bottom: {{ pd_item[2] | default: 0 }};--pd-left: {{ pd_item[3] | default: 0 }};--pd-tb-top: {{ pd_item_tb[0] | default: 0 }};--pd-tb-right: {{ pd_item_tb[1] | default: 0 }};--pd-tb-bottom: {{ pd_item_tb[2] | default: 0 }};--pd-tb-left: {{ pd_item_tb[3] | default: 0 }};--pd-mb-top: {{ pd_item_mb[0] | default: 0 }};--pd-mb-right: {{ pd_item_mb[1] | default: 0 }};--pd-mb-bottom: {{ pd_item_mb[2] | default: 0 }};--pd-mb-left: {{ pd_item_mb[3] | default: 0 }};" timeline hdt-reveal="slide-in">
              {% if block.settings.col_heading != blank %}  
                <div class="t4s-footer-heading t4s-d-flex t4s-align-items-center t4s-d-md-block t4s-d-none">
                  <h3 class="t4s-col-heading t4s-fnt-fm-{{ section.settings.heading_fontf }}">{{ block.settings.col_heading }}</h3>
                </div>
              {%- endif -%} 
              {%- if block.settings.col_heading_mobile != blank or block.settings.col_heading != blank -%}   
                <div data-footer-open class="t4s-footer-heading t4s-footer-heading-mobile t4s-d-flex t4s-align-items-center t4s-d-md-none">
                  {% if block.settings.col_heading_mobile != blank %}  
                    <h3 class="t4s-col-heading t4s-fnt-fm-{{ section.settings.heading_fontf }}">{{ block.settings.col_heading_mobile }}</h3> 
                  {%- else -%}     
                    <h3 class="t4s-col-heading t4s-fnt-fm-{{ section.settings.heading_fontf }}">{{ block.settings.col_heading }}</h3> 
                  {%- endif -%}     
                    <span class="t4s-footer-collapse-icon"></span>
                </div>
              {%- endif -%} 
              <div {% if block.settings.col_heading != blank or block.settings.col_heading_mobile != blank %}data-footer-content{% endif %} class="t4s-footer-content">
                {%- if block.settings.menu != blank -%} 
                  <div class="t4s-footer-menu t4s-menu-style-{{ block.settings.menu_style | default: "default" }}" style="--space-item: {{ block.settings.space_item }}px;--space-item-tb: {{ block.settings.space_item_tb }}px;--space-item-mb: {{ block.settings.space_item_mb }}px;--mgb: {{ block.settings.mgb }}px;--mgb-tb: {{ block.settings.mgb_tb }}px;--mgb-mb: {{ block.settings.mgb_mb }}px;"> 
                    <ul class="t4s-footer-linklist">
                      {%- for link in block.settings.menu.links -%}
                      <li>
                        <a href="{{ link.url }}" class="t4s-footer-link {% if link.current %}t4s-footer-link-active{% endif %}" {% if link.current %}  aria-current="page" {% endif %}>
                          {% if block.settings.menu_style == "icon" %}
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32"><path d="M 12.96875 4.28125 L 11.53125 5.71875 L 21.8125 16 L 11.53125 26.28125 L 12.96875 27.71875 L 23.96875 16.71875 L 24.65625 16 L 23.96875 15.28125 Z"/></svg>
                          {% endif %}
                          {%- render 'title_menu2', title: link.title -%}
                        </a>
                      </li>
                      {%- endfor -%} 
                    </ul>
                  </div> 
                {%- endif -%}   
          {%- else -%}
            {%- liquid
              assign next_block = section.blocks[forloop.index]
              assign is_prev_block_image = false 
            -%}
            {%- render 'footer_content', block: block, block: block -%}  
            {%- if forloop.last or next_block and next_block.type == 'bl_col' -%}
              </div></div></div>
            {%- endif -%}
          {%- endif -%}
        {%- endfor -%}
      </div>
      {{- html_layout[1] -}}
    </div>
    {%- if sticky %}<style>@media (min-width: 1025px) { #MainContent,.t4s-announcement-bar-section,.t4s-top-bar-section,#t4s-header,.t4s-prefooter,.t4s-section-header{position:relative;z-index:4}.t4s-top-bar-section {z-index: 466;} #MainContent { z-index: 2;background-color: {%- if settings.body_bg != blank and settings.body_bg != 'rgba(0,0,0,0)' %}{{ settings.body_bg }}{% else %}#fff{% endif -%};}  .t4s-prefooter{z-index:1}#t4s-footer{position:sticky;position:-webkit-sticky;bottom:0;left:0;right:0;width:100%} }</style>{% endif -%}
    
{%- endunless -%}
<style>
.t4s-footer-content .t4s-cur button,
.t4s-footer-content .t4s-cur  button {
  text-transform: capitalize;
  color: var(--text-cl);
  display: flex;
  padding: 5px;
  align-items: center;
  line-height: 20px;
  font-size: 100%;
  background-color: transparent;
}
.t4s-footer-content .t4s-cur button:hover,
.t4s-footer-content button:hover {
  background: 0 0;
  color: var(--accent-color);
}
.t4s-footer-content .t4s-cur button svg,
.t4s-footer-content button svg {
  width: 8px;
  margin-inline-start: 5px;
}
.t4s-footer .t4s-newsletter-design-1 .t4s-newsletter__inner .t4s-newsletter__submit{
  min-width: unset;
  padding: 0 15px;
}
.t4s-footer-content .t4s-cur .t4s-dropdown__wrapper,
.t4s-footer-content .t4s-dropdown__wrapper {
  background-color: var(--t4s-body-background);
  padding: 15px;
}
.t4s-footer-content .t4s-cur .t4s-drop-arrow,
.t4s-footer-content .t4s-drop-arrow {
  background-color: var(--t4s-body-background);
}
.t4s-footer-content .t4s-cur button.is--selected,
.t4s-footer-content button.is--selected {
  color: var(--text-cl);;
}
.t4s-footer-content .t4s-cur .t4s-dropdown__wrapper button.is--selected,
.t4s-footer-content .t4s-dropdown__wrapper button.is--selected {
  color: var(--text-cl);
}
.t4s-footer-content .t4s-cur .t4s-currency-item,
.t4s-footer-content .t4s-cur .t4s-lang-item {
  color: var(--text-cl);
}
.t4s-footer-content .t4s-cur .t4s-header__currencies,
.t4s-footer-content .t4s-cur .t4s-header__languages {
  margin: 0 7px;
}
@media (max-width: 767px) {
  .t4s-footer-content .t4s-cur .t4s-dropdown__wrapper.is-style-mb--false,
  .t4s-footer-content .t4s-dropdown__wrapper.is-style-mb--false {
    min-width: 100px;
    max-width: 300px;
    width: auto;
  }
}
@media (min-width: 768px) {
  .t4sp-hover ..t4s-footer-content .t4s-cur .t4s-dropdown__wrapper button:hover,
  .t4sp-hover .t4s-footer-content .t4s-dropdown__wrapper button:hover {
    color: var(--link-hover-cl);
  }
  .t4s-footer-content .t4s-cur .t4s-dropdown__wrapper,
  .t4s-footer-content .t4s-dropdown__wrapper {
    min-width: 100px;
    max-width: 300px;
    width: auto;
  }
}
</style>

