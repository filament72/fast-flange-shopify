<!-- sections/bgvideo-cms.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-hero.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
{%-liquid
  assign sid = section.id
  assign stt_layout = section.settings.layout
  assign image = section.settings.image
  if section.settings.source == "2" and image == blank
    assign image = section.settings.video.preview_image
  endif
  assign se_height = section.settings.se_height

  assign bg_opacity = section.settings.bg_opacity | divided_by: 100.0 
  assign bg_overlay = section.settings.bg_overlay | color_modify: 'alpha', bg_opacity 

  assign ani_delay = 0   
  assign percent_delay = section.settings.animation_delay | divided_by: 100.0
  assign time_ani_delay = section.settings.time_animation | times: percent_delay

  if stt_layout == 't4s-container-wrap' 
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif

  assign countdown = false
  assign custom_button = false
  assign use_animation = false
  assign general_block = false
-%}
{%- capture append_style -%}
  --aspect-ratio-cus:{{ section.settings.height_dk }}px;--aspect-ratio-custb:{{ section.settings.height_tb }}px;--aspect-ratio-cusmb:{{ section.settings.height_mb }}px;
{%- endcapture -%}
{%- render 'section_style', sid: sid, append_style: append_style -%}
<div class="t4s-section-inner t4s_se_{{ sid }} {{ stt_layout }}">
  {{- html_layout[0] -}}
  <div data-t4s-animate class="t4s-bg-video t4s-row t4s-row-cols-1 t4s-gx-0 t4s_ratio_mix {{ se_height }} t4s_ratio_cuspx_mb_{{ section.settings.custom_mb }} t4s_ratio_cuspx_tb_{{ section.settings.custom_tb }} t4s_ratio_cuspx_{{ section.settings.custom_dk }}">
    <div class="t4s-col-item t4s-bg-video-item">
      <div class="t4s-hero-inner t4s-pr t4s-oh t4s_cover" data-video-background data-options='{ "type": "{% if section.settings.source == "1" %}{{ section.settings.video_url.type }}{% else %}html5{% endif %}","vid": "{{ section.settings.video_url.id }}","requestHost": "{{ request.host }}","srcDefault": "https://cdn.shopify.com/s/files/1/0610/5209/2628/files/Share_your_brand_story_by_adding_a_video_to_your_store.mp4?v=1641630446" ,"mute": "true", "id":"#tmp-video-{{ sid }}" }' style="--ratioapt: {{ image.aspect_ratio | default: 1.7777 }};">
        <div class="t4s_ratio t4s_position_0" {% if image != blank %} style="background: url({{ image | image_url: width: 1 }})" {%- endif -%}>
            
          {%- if image != blank -%}
              <img class="t4s-img-video t4s-pe-none lazyloadt4s" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
          {%- else -%}
              <div class="t4s-video-bg-placeholder t4s-img-video"></div>
          {%- endif -%}
          <div data-bgvideo-insert></div>

          {%- if section.settings.source == "2" and section.settings.video != blank -%}
          <template id="tmp-video-{{ sid }}" class="t4s-d-none">{{ section.settings.video | video_tag: image_size: '1x1', autoplay: true, loop: true, muted: true, controls: false, preload: 'auto', class: 't4s_bg_vid_html5' }}</template>
          {%- endif -%}

        </div>
        <div class="t4s-content-wrap t4s-pe-none t4s-full-width-link t4s-z-100" hdt-reveal="slide-in">
          <div class="t4s-content-position t4s-container t4s-pa" style="--time-animation:{{ section.settings.time_animation }}s;{%- render 'content_position', ch_pos: 50, cv_pos: 50, ch_pos_tb: 50, cv_pos_tb: 50, ch_pos_mb: 50, cv_pos_mb: 50 -%}">
            <div class="t4s-text-lg-{{ section.settings.text_align }} t4s-text-md-{{ section.settings.text_align_tb }} t4s-text-{{ section.settings.text_align_mb }}">
              {%- for block in section.blocks -%}
                
                {%- case block.type -%}
                  {%- when 'custom_text' -%}
                    {%- assign general_block = true -%} 
                    {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                    <{{ block.settings.tag }} data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-text-bl t4s-fnt-fm-{{ block.settings.fontf }} t4s-font-italic-{{ block.settings.font_italic }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-hidden-br-mb-{{ block.settings.remove_br_tag }} t4s-text-shadow-{{ block.settings.text_shadow }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_text', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</{{ block.settings.tag }}>
                  {%- when 'space_html' -%}
                    {%- assign general_block = true -%}
                    {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                    <div class="t4s-bl-item t4s-space-html t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" {%- render 'bk_cus_style', type: 'space_html', block: block, ani_delay: ani_delay -%}></div>
                  {%- when 'html' -%}
                    {%- assign general_block = true -%}
                    {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                    <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-raw-html t4s-rte--list t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" {%- render 'bk_cus_style', type: 'html', block: block, ani_delay: ani_delay -%}>{{ block.settings.html_content }}</div>
                  {%- when 'break' -%}
                  </div><div class="t4s-text-lg-{{ section.settings.text_align }} t4s-text-md-{{ section.settings.text_align_tb }} t4s-text-{{ section.settings.text_align_mb }}">
                  {%- when 'image' -%}
                    {%- liquid
                      assign image = block.settings.image_child 
                      if block.settings.img_link != blank 
                        assign image_parent = 'a,' | split: ','
                      else 
                        assign image_parent = 'div,data-' | split: ','
                      endif 
                    -%}
                    {%- if image != blank -%}
                      {%- assign general_block = true -%}
                      {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                      <{{ image_parent[0] }} {{ image_parent[1] }}href="{{ block.settings.img_link }}" class="t4s-bl-item t4s-img-child t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-pe-auto" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', block: block, ani_delay: ani_delay -%}>
                          <img class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                      </{{- image_parent[0] -}}>
                    {%- else -%}
                      {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                    {%- endif -%}
                  {%- when 'countdown' -%}
                      {%- if block.settings.date != blank -%}
                        {%- assign countdown = true -%}
                          <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-countdown t4s-countdown-size-{{ block.settings.cdt_size }} t4s-countdown-text-{{ block.settings.cd_txt_ts }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'countdown', block: block, ani_delay: ani_delay -%}>
                            <div class="t4s-time" data-countdown-t4s data-date='{{ block.settings.date }}' data-keyid='#countdown-{{ sid }}'></div>
                          </div>
                      {%- endif -%}
                  {%- when 'custom_button' -%}
                    {%- if block.settings.button_link != blank and block.settings.button_text != blank -%}
                      {%- assign custom_button = true -%}
                      {%- assign  button_style = block.settings.button_style -%}
                      <a href="{{ block.settings.button_link }}" target="{{ block.settings.target_link }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-btn t4s-btn-custom  t4s-pe-auto t4s-fnt-fm-{{ block.settings.fontf }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' %}t4s-btn-effect-{{ block.settings.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_button', block: block, ani_delay: ani_delay -%}>{{ block.settings.button_text }}
                      {%- if block.settings.button_icon_w > 0 -%}
                        {%- if block.settings.icon_style == "1" -%}
                          {% comment %} <svg class="t4s-btn-icon"  width="16" height="11" viewBox="0 0 16 11" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <rect y="4" width="12" height="3" />
                            <path d="M15.0434 5.50985L11.3245 9.44864L11.2626 1.63039L15.0434 5.50985Z"/>
                          </svg> {% endcomment %}
                          <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="18" height="18"><path d="M 21.1875 9.28125 L 19.78125 10.71875 L 24.0625 15 L 4 15 L 4 17 L 24.0625 17 L 19.78125 21.28125 L 21.1875 22.71875 L 27.90625 16 Z"/></svg>
                            
                        {%- elsif block.settings.icon_style == "2" -%}
                            <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>   
                        {% else %}
                          <svg class="t4s-btn-icon" version="1.1" x="0px" y="0px" width="22px" height="22px" viewBox="0 0 22 22" style="enable-background:new 0 0 22 22;" xml:space="preserve"><polygon points="6,20 6,2 17,11 "></polygon></svg>  
                        {%- endif -%}
                    {%- endif -%}
                     </a>
                    {%- endif -%}
                {%- endcase -%}
                {%- if block.settings.animation != 'none' -%}{%- assign ani_delay = ani_delay | plus: time_ani_delay -%}{%- endif -%}
              {%- endfor -%}
            </div>
          </div>
        </div>
        <div class="t4s-hero-overlay t4s-full-width-link t4s-pe-none" style="--bg-overlay:{{ bg_overlay }};"></div>
      </div>
    </div>
  </div>
  {{- html_layout[1] -}}
</div>
<style>
  /* hero background video */
  .t4s-cms-video .t4s-video-bg-placeholder {
    background-color: #f5f5f5;
  }
  .t4s-cms-video .t4s-container {padding: 0 15px;}
  @media (min-width:1025px){
    .t4s-cms-video .t4s-content-position{
      display:flex;
      width:100%;
      align-items:center;
      justify-content:space-between;
      gap:15px;
    }
  }
  @media (min-width:1119px){
    .t4s-cms-video .t4s-container {
      max-width: 1500px!important;
    }
  }
</style>
{%- if general_block -%}
  {{ 't4s-general-block.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- if custom_button -%}
  {{ 't4s-button-style.css' | asset_url | stylesheet_tag }}
  <link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- endif -%}
{%- if use_animation -%}
  <link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
  <link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- endif -%}
{%- if countdown -%}
  {{ 't4s-countdown.css' | asset_url | stylesheet_tag }}
  <template id="countdown-{{ sid }}">
    <span class="t4s-countdown-days">
      <span class="cd_timet4s t4s-cd-number">%-D</span>
      <span class="cd_txtt4s t4s-cd-text">%!D:{{ "sections.countdown_text.day" | t }},{{ "sections.countdown_text.day_plural" | t }};</span>
    </span>
    <span class="t4s-countdown-hours">
      <span class="cd_timet4s t4s-cd-number">%H</span> 
      <span class="cd_txtt4s t4s-cd-text">%!H:{{ "sections.countdown_text.hr" | t }},{{ "sections.countdown_text.hr_plural" | t }};</span>
    </span>
    <span class="t4s-countdown-min">
      <span class="cd_timet4s t4s-cd-number">%M</span> 
      <span class="cd_txtt4s t4s-cd-text">%!M:{{ "sections.countdown_text.min" | t }},{{ "sections.countdown_text.min_plural" | t }};</span>
    </span>
    <span class="t4s-countdown-sec">
      <span class="cd_timet4s t4s-cd-number">%S</span> 
      <span class="cd_txtt4s t4s-cd-text">%!S:{{ "sections.countdown_text.sec" | t }},{{ "sections.countdown_text.sec_plural" | t }};</span>
    </span> 
  </template>
{%- endif -%}

{% schema %}
{
  "name": "CMS video",
  "tag": "section",
  "class": "t4s-section t4s-section-all t4s_tp_cd t4s-cms-video t4s_tp_bgvideo",
  "enabled_on": {
    "groups": [
      "custom.password"
    ]
  },
  "settings": [
    {
      "type": "paragraph",
      "content": "Background videos are automatically muted to allow autoplay. If you require audio, use the \"Video Popup & Inline\" or \"Video Featured\" section."
    },
    {
      "type": "header",
      "content": "1. General options"
    },
    {
      "type": "select",
      "id": "se_height",
      "label": "Section height",
      "default": "t4s_ratioadapt_mix16_9",
      "options": [
        {
          "value": "t4s_ratio_fh",
          "label": "Full screen height"
        },
        {
          "value": "t4s_ratioadapt_mix",
          "label": "Adapt to image"
        },
        {
          "value": "t4s_ratioadapt_mix16_9",
          "label": "16:9"
        },
        {
          "value": "t4s_ratioadapt_mix4_3",
          "label": "4:3"
        }
      ]
    },
    {
      "type": "checkbox",
      "id": "custom_dk",
      "label": "Use custom height (Desktop)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_dk",
      "label": "Section height (Desktop)",
      "default": 600
    },
    {
      "type": "checkbox",
      "id": "custom_tb",
      "label": "Use custom height (Tablet)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_tb",
      "label": "Section height (Tablet)",
      "default": 400
    },
    {
      "type": "checkbox",
      "id": "custom_mb",
      "label": "Use custom height (Mobile)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_mb",
      "label": "Section height (Mobile)",
      "default": 250
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Cover image",
      "info": "Fallback when autoplaying video is not supported on mobile devices"
    },
    {
      "type": "select",
      "id": "source",
      "label": "+ Source video",
      "default": "2",
      "options": [
        {
          "value": "1",
          "label": "Youtube or Vimeo"
        },
        {
          "value": "2",
          "label": "Shopify-hosted"
        }
      ]
    },
    {
      "id": "video_url",
      "type": "video_url",
      "label": "A video from Youtube or Vimeo",
      "accept": [
        "youtube",
        "vimeo"
      ],
      "default": "https://www.youtube.com/watch?v=_9VUPq3SxOc"
    },
    {
      "type": "video",
      "id": "video",
      "label": "A Shopify-hosted video",
      "info": "File video link from uploaded files. File size is smaller 4 mb recommended"
    },
    {
      "type": "paragraph",
      "content": "————————————————————"
    },
    {
      "type": "select",
      "id": "text_align",
      "label": "Content align",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "select",
      "id": "text_align_tb",
      "label": "Content align (Tablet)",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "select",
      "id": "text_align_mb",
      "label": "Content align (Mobile)",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "header",
      "content": "+ Background, color options"
    },
    {
      "type": "color",
      "id": "bg_overlay",
      "label": "Overlay",
      "default": "#000"
    },
    {
      "type": "range",
      "id": "bg_opacity",
      "label": "Overlay opacity",
      "default": 0,
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "%"
    },
    {
      "type": "header",
      "content": "--Animation Options--"
    },
    {
      "type": "range",
      "id": "time_animation",
      "label": "Duration animation each block",
      "max": 5,
      "min": 1,
      "default": 1,
      "unit": "s",
      "step": 0.5
    },
    {
      "type": "range",
      "id": "animation_delay",
      "label": "Time animation delay",
      "max": 110,
      "min": 10,
      "step": 10,
      "unit": "%",
      "default": 40,
      "info": "Defines the number of time to wait when the animation previous end, before the animation next will start."
    },
    {
      "type": "header",
      "content": "2. Design options"
    },
    {
      "type": "select",
      "id": "layout",
      "default": "t4s-container-fluid",
      "label": "Layout",
      "options": [
        {
          "value": "t4s-container-wrap",
          "label": "Wrapped container"
        },
        {
          "value": "t4s-container-fluid",
          "label": "Full width"
        }
      ]
    },
    {
      "type": "color",
      "id": "cl_bg",
      "label": "Background"
    },
    {
      "type": "color_background",
      "id": "cl_bg_gradient",
      "label": "Background gradient"
    },
    {
      "type": "text",
      "id": "mg",
      "label": "Margin",
      "info": "Margin top, margin right, margin bottom, margin left. If you do not use it please blank.",
      "default": ",,50px,",
      "placeholder": ",,50px,"
    },
    {
      "type": "text",
      "id": "pd",
      "label": "Padding",
      "info": "Padding top, padding right, padding bottom, padding left. If you do not use it please blank.",
      "placeholder": "50px,,50px,"
    },
    {
      "type": "header",
      "content": "+ Design tablet options"
    },
    {
      "type": "text",
      "id": "mg_tb",
      "label": "Margin",
      "default": ",,50px,",
      "placeholder": ",,50px,"
    },
    {
      "type": "text",
      "id": "pd_tb",
      "label": "Padding",
      "placeholder": "50px,,50px,"
    },
    {
      "type": "header",
      "content": "+ Design Mobile Options"
    },
    {
      "type": "text",
      "id": "mg_mb",
      "label": "Margin",
      "default": ",,30px,",
      "placeholder": ",,50px,"
    },
    {
      "type": "text",
      "id": "pd_mb",
      "label": "Padding",
      "placeholder": ",,50px,"
    }
  ],
  "blocks": [
    {
      "type": "custom_text",
      "name": "Text",
      "settings": [
        {
          "type": "textarea",
          "id": "text",
          "label": "Input text",
          "default": "Text",
          "info": "If you want to line break, please add a <br> tag in the text"
        },
        {
          "type": "checkbox",
          "id": "remove_br_tag",
          "label": "Remove <br> tag on tablet & mobile",
          "default": false
        },
        {
          "type": "select",
          "id": "tag",
          "label": "Html tag",
          "default": "p",
          "options": [
            {
              "value": "h2",
              "label": "H2"
            },
            {
              "value": "h3",
              "label": "H3"
            },
            {
              "value": "h4",
              "label": "H4"
            },
            {
              "value": "h5",
              "label": "H5"
            },
            {
              "value": "h6",
              "label": "H6"
            },
            {
              "value": "p",
              "label": "P"
            },
            {
              "value": "div",
              "label": "DIV"
            }
          ]
        },
        {
          "type": "select",
          "id": "fontf",
          "default": "inherit",
          "label": "Font family",
          "options": [
            {
              "label": "Inherit",
              "value": "inherit"
            },
            {
              "label": "Font Family #1",
              "value": "1"
            },
            {
              "label": "Font Family #2",
              "value": "2"
            },
            {
              "label": "Font Family #3",
              "value": "3"
            }
          ]
        },
        {
          "type": "color",
          "id": "text_cl",
          "label": "Color text",
          "default": "#fff"
        },
        {
          "type": "range",
          "id": "text_fs",
