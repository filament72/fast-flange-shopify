                              <img {% if image2.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{image2.presentation.focal_point}}" {% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff t4s-d-none t4s-d-md-block" data-src="{{ image2 | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image2.width }}" height="{{ image2.height }}" alt="{{ image2.alt | escape }}" loading="lazy">
