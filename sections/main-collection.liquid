                              <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none" style="background: url({{ image_mb1 | image_url: width: 1 }})"></span>
                              <img {% if image1.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{ image1.presentation.focal_point }};" {% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff t4s-d-none t4s-d-md-block" data-src="{{ image1 | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image1.width }}" height="{{ image1.height }}" alt="{{ image1.alt | escape }}" loading="lazy">
                              <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: url({{ image1 | image_url: width: 1 }})"></span>
                            {%- else -%}
                              {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                            {%- endif -%}
                          </{{ ARRhtml[0] }}>
                          <div class="t4s-banner-content t4s-content-position t4s-pa t4s-bg-content-true ">
                            {% if block.settings.title1 != blank %}
                              <h5 class="title">
                                {{ block.settings.title1 }}
                              </h5>
                            {% endif %}
                            {% if block.settings.subtitle1 != blank %}
                              <h3 class="subtitle">
                                {{ block.settings.subtitle1 }}
                              </h5>
                            {% endif %}
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="t4s-isotopet4s-item t4s-banner-collection2 t4s-col-item t4s-banner-wrap t4s_cover bl-{{ block.id }}" style="--text-cl: {{ block.settings.text-color2 }};{{ block.settings.quick_options2 }}">
                      <div data-t4s-animate class="t4s-banner-item t4s-eff t4s-eff-img-zoom t4s_ratio_mix t4s_ratio_cuspx_mb_true t4s_ratio_cuspx_tb_true t4s_ratio_cuspx_true" style="--ratioapt: {{ image2.aspect_ratio | default: 1.5 }};--ratioaptmb: {{ image_mb2.aspect_ratio | default: 1.5 }};--aspect-ratio-cus : {{ block.settings.height_dk2 }}px;--aspect-ratio-custb : {{ block.settings.height_tb2 }}px;--aspect-ratio-cusmb : {{ block.settings.height_mb2 }}px;">
                        <div class="t4s-banner-inner">
                          <{{ ARRhtml2[0] }} {{ ARRhtml2[1] }}href="{{ block.settings.b_link2 }}"{{ ARRhtml2[2] }}target="_blank"class="t4s-d-block t4s_ratio t4s_ratio_hasmb t4s-bg-pos-cener">
