{%- style -%}
  {%- liquid 
		if section.settings.lh_pr == 0
			assign lh_pr = 1
			assign lh_pr_f = 1
		else
			assign lh_pr = section.settings.lh_pr | append: 'px'
			assign lh_pr_f = section.settings.lh_pr | plus: 5 | append: 'px'
		endif 
		if section.settings.lh_pr_mb == 0
			assign lh_pr_mb = 1
			assign lh_pr_f_mb = 1
		else
			assign lh_pr_mb = section.settings.lh_pr_mb | append: 'px'
			assign lh_pr_f_mb = section.settings.lh_pr_mb | plus: 5 | append: 'px'
		endif 
		if section.settings.lh_price_pr == 0
			assign lh_price_pr = 1
		else
			assign lh_price_pr = section.settings.lh_price_pr | append: 'px'
		endif 
		if section.settings.lh_price_pr_mb == 0
			assign lh_price_pr_mb = 1
		else
			assign lh_price_pr_mb = section.settings.lh_price_pr_mb | append: 'px'
		endif -%}
  .t4s-section-config-product .t4s_box_pr_grid {
    margin-bottom: 100px;
  }
  .t4s-section-config-product .t4s-top-heading {
    margin-bottom: 30px;
  }
  .t4s-product,
  .t4s-product-quick-view .t4s-product__media-wrapper {
    --pr-btn-radius-size: {{ section.settings.pr_btn_round }}px;
  }

  {%- assign countdown_color_lightness       = section.settings.countdown_color | color_extract: 'lightness' -%}
  {% if settings.enable_compe == false and settings.wishlist_mode == "0" and settings.enable_quickview == false and settings.enable_quickshop == false  %}
  	.t4s-product .t4s-product-thumb .t4s-product-btns {
  		display: none !important;
    }
  {% endif %}
  .t4s-product {
    --pr-title-family: var(--font-family-{{ section.settings.fnt_df_pr }});
    --pr-title-style: {{ section.settings.txt_tr_pr }};
    --pr-title-size: {{ section.settings.size_pr }}px;
    --pr-title-size-mb: {{ section.settings.size_pr_mb }}px;
    --pr-title-weight: {{ section.settings.fw_pr }};
    --pr-title-lh: {{ lh_pr }};
    --pr-title-lh-mb: {{ lh_pr_mb }};
    --pr-title-ls: {{ section.settings.ls_pr }}px;
    --pr-title-ls-mb: {{ section.settings.ls_pr_mb }}px;

    --pr-price-size: {{ section.settings.size_price_pr }}px;
    --pr-price-size-mb: {{ section.settings.size_price_pr_mb }}px;
    --pr-price-lh: {{ lh_price_pr }};
    --pr-price-lh-mb: {{ lh_price_pr_mb }};
    --pr-price-weight: {{ section.settings.fw_price_pr }};
    --pr-space-img-txt: {{ section.settings.space_img_content }}px;
    --pr-space-img-txt-mb: {{ section.settings.space_img_content_mb }}px;
    --pr-space-elements: {{ section.settings.space_elements }}px;
    --pr-space-elements-mb: {{ section.settings.space_elements_mb }}px;

    --pr-countdown-color: {% if countdown_color_lightness < 85 %}
      #fff{% else %}
      #23232c{% endif %};
    --pr-countdown-bg-color: {{ section.settings.countdown_color | color_modify: 'alpha', countdown_overlay }};

    --swatch-color-size: {{ section.settings.swatch_color_size }}px;
    --swatch-color-size-mb: {{ section.settings.swatch_color_size_mb }}px;
    --pr-background-overlay: {{ section.settings.pr_bg_overlay | color_modify: 'alpha', pr_overlay }};
  }

  .t4s-product-quick-view,
  .t4s-product-quick-shop {
    --swatch-color-size: {{ section.settings.swatch_color_size }}px;
    --swatch-color-size-mb: {{ section.settings.swatch_color_size_mb }}px;
  }

  .t4s-product,
  .t4s-container .t4s-row-cols-lg-5 > .t4s-product,
  .t4s-container .t4s-row-cols-lg-6 > .t4s-product {
    --pr-title-color: {{ section.settings.pr_title_color }};
    --pr-title-color-hover: {{ section.settings.pr_title_color_hover }};
    --pr-price-color: {{ section.settings.price_color }};
    --pr-secondary-price-color: {{ section.settings.price_secondary }};
    --pr-price-sale-color: {{ section.settings.price_sale_color }};
    --pr-vendors-color: {{ section.settings.vendors_color }};
    --pr-vendors-color-hover: {{ section.settings.vendors_color_hover }};
    --pr-size-list-color: {{ section.settings.size_list_color }};
  }
  .t4s-products > .t4s-pr-featured {
    --pr-title-size: calc({{ section.settings.size_pr }}px + 2px);
    --pr-title-size-mb: calc({{ section.settings.size_pr_mb }}px + 2px);
    --pr-title-lh: {{ lh_pr_f }};
    --pr-title-lh-mb: {{ lh_pr_f_mb }};
  }
{%- endstyle -%}
{%- for block in section.blocks -%}
  {%- liquid
		
		assign hidden_atc_mb = block.settings.hidden_atc_mb
		assign hidden_qv_mb = block.settings.hidden_qv_mb
		assign hidden_wishlist_mb = block.settings.hidden_wishlist_mb
