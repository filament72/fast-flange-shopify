    {%- endif -%}
    {%- if section.settings.enable_gift_wrap and gift_pr.available == true -%}<div data-toogle-gift data-cart-tool_action data-id="gift" class="mini_cart_tool_btn is--gift t4s-pr t4s-truncate"{% if arr_gift_id.size > 0 %} style=" display: none"{% endif %}>{{ 'cart.tool.add_gift_wrap' | t }}</div>{%- endif -%}
    {%- if section.settings.enable_rates -%}<div data-cart-tool_action data-id="rates" class="mini_cart_tool_btn is--rates t4s-pr t4s-truncate">{{ 'cart.shipping_estimator.estimate' | t }}</div>{%- endif -%}
