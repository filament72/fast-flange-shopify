<!-- sections/tabs-collection.liquid -->
{%- # theme-check-disable NestedSnippet, DeprecateBgsizes -%}
{%- liquid
  assign sid = section.id
  assign stt_layout = section.settings.layout
  assign stt_image_bg = section.settings.image_bg
  if stt_layout == 't4s-se-container'
    assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
  elsif stt_layout == 't4s-container-wrap'
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif

  assign search_url = routes.all_products_collection_url
  if section.settings.btn_owl == "simple"
    assign arrow_icon = 1
  else
    assign arrow_icon = 2
  endif

  assign title_inline = section.settings.title_inline
  assign tabs_des = section.settings.tabs_des
  assign tabs_pos = section.settings.tabs_pos

  assign use_link_vendor = settings.use_link_vendor
-%} 
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-top-head.css' | asset_url | stylesheet_tag }}
{{ 't4s-tabs-collection.css' | asset_url | stylesheet_tag }}
{{ 't4s-collection-products.css' | asset_url | stylesheet_tag }}
{{ 't4s-slider-settings.css' | asset_url | stylesheet_tag }}
{{ 'pre_flickity_custom.css' | asset_url | stylesheet_tag }}
{{ 'pre_flickityt4s.min.css' | asset_url | stylesheet_tag }}

<link rel="stylesheet" href="{{ 'base_drop.min.css' | asset_url }}" media="all">
<link href="{{ 't4s-loading.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- assign search_terms = search.terms | split: ' ' -%}
{%- if search.terms and search_terms contains 'ntt4tag' -%}
  {%- liquid
    assign num_i = search_terms[0] | remove: '_bid' | plus: 0
    assign block = section.blocks[num_i]
    assign collection = collections[block.settings.collection]
    render 'inc_tab', ck_q: false, sid: sid, collection: collection, block: block, arrow_icon: arrow_icon
  -%}
{%- else -%}
{% render 'section_style', sid: sid -%} 
<div class="t4s-section-inner t4s_se_{{ sid }} {{ stt_layout }}{% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} t4s-has-imgbg lazyloadt4s{% endif %}" {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="2"{% endif %} >
  {{- html_layout[0] -}}
  {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner{% if stt_image_bg != blank %} t4s-has-imgbg lazyloadt4s{% endif %}" {% if stt_image_bg != blank %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="2"{% endif %}>{% endif -%}
  <div class=" t4s-tabs-se tabs-layout-{{ tabs_des }} {% if title_inline %}t4s-tabs-inline{% endif %} t4s-tabs-{{ tabs_des }}" style="--pri-cl:{{ section.settings.pri_cl }};--secon-cl:{{ section.settings.secon_cl }};--pri-active-cl:{{ section.settings.pri_active_cl }};--secon-active-cl:{{ section.settings.secon_active_cl }};--item-fs:{{ section.settings.item_fs }}px;--item-fw:{{ section.settings.item_fw }};--item-lh:{{ section.settings.item_lh }}px;--item-ls:{{ section.settings.item_ls }}px;--icon-width:{{ section.settings.icon_width }}px;--space-between:{{ section.settings.space_between }}px;--space-between-tb:{{ section.settings.space_between_tb }}px;--space-between-mb:{{ section.settings.space_between_mb }}px;--tablist-mgb:{{ section.settings.tabslist_mb }}px;--tablist-mgb-tb:{{ section.settings.tabslist_mb_tb }}px;--tablist-mgb-mb:{{ section.settings.tabslist_mb_mb }}px;--item-fs-mb:{{ section.settings.item_fs_mb }}px;--item-lh-mb:{{ section.settings.item_lh_mb }}px;--item-ls-mb:{{ section.settings.item_ls_mb }}px;">
    {%- if title_inline == false -%}
      {%- render 'section_tophead' -%}
    {%- endif -%}
    <div class="t4s-tabs t4s-type-tabs" data-t4s-tabs2>
      {%- if title_inline -%}
        <div class="t4s-tabs-head"{% if settings.animations_reveal_on_scroll %} hdt-reveal="slide-in"{% endif %}>
        {%- render 'section_tophead' -%}
      {%- endif -%}
        <ul data-t4s-tab-ul2 class="t4s-tabs-ul t4s-align-items-center t4s-justify-content-{{ tabs_pos }} t4s-tabs-icon_hide-{{ section.settings.hide_ico }} t4s-flickity-slider t4s-slider-btn__in-content t4s-slider-btn-show_all t4s-slider-btn-style-simple t4s-slider-btn-none t4s-slider-btn-small t4s-slider-btn-cl-{{ section.settings.btn_cl }} t4s-slider-btn-vi-always flickityt4s t4s-dots-round-{{ section.settings.dots_round }}" data-flickityt4s-js='{"isSimple": true, "freeScroll": true, "setPrevNextButtons": true, "arrowIcon": "1", "imagesLoaded": 0,"adaptiveHeight": 0, "contain": 1, "groupCells": "100%", "dragThreshold" : 5, "minWidthLG": 768, "cellAlignLG":"{{ tabs_pos }}", "cellAlign":"center", "wrapAround": false,"prevNextButtons": false,"percentPosition": 0,"pageDots": true, "pauseAutoPlayOnHover" : true }'>  
        {%- for block in section.blocks -%}
          {%- liquid
            assign blockid    = block.id
            assign title      = block.settings.title
            assign image      = block.settings.image_title
          -%}
          <li class="t4s-tab-item t4s-d-inline-flex">
            <a id="b_{{ block.id }}" href="#t4s-{{ blockid }}" rel="nofollow" data-t4s-tab-item data-no-instant{% if forloop.first %} class="t4s-active"{% endif %} {{ block.shopify_attributes }}>
              <span>
                {%- if block.settings.icon_title != blank -%}
                  <div class="t4s-icon-title"><i class="{{ block.settings.icon_title }} t4s-obj-eff"></i></div>
                {%- elsif block.settings.image_title != blank -%}
                  <span class="t4s-image-title">
                    <img class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                    <span class="lazyloadt4s-loader is-bg-img" style="background: url({{ image | image_url: width: 1 }});"></span>
                  </span>
                {%- endif -%}
                <span class="t4s-text-title">{{ title }}</span>
              </span>
            </a>
          </li>
        {%- endfor -%}
      </ul>
      {%- if title_inline -%}
        </div>
      {%- endif -%}
      <div class="t4s-pr t4s-tab-contents2"{% if settings.animations_reveal_on_scroll %} hdt-reveal="slide-in"{% endif %}>
        {%- if request.design_mode -%}
          {%- for block in section.blocks -%}
            {%- assign collection = collections[block.settings.collection] -%}
            <div id="t4s-{{ block.id }}" class="t4s-tab-content2 {% if forloop.first == true %} t4s-active {% endif %}" data-t4s-tab-content data-render-lazy-component >
              {%- render 'inc_tab', ck_q: true, sid: sid, collection: collection, block: block, arrow_icon: arrow_icon, use_link_vendor: use_link_vendor -%}
            </div>
          {%- endfor -%}
        {%- else -%}
          {%- assign block = section.blocks[0] -%}
          {%- assign collection = collections[block.settings.collection] -%}
          <div id="t4s-{{ block.id }}" class="t4s-tab-content2 t4s-active" data-t4s-tab-content data-render-lazy-component >
            {%- render 'inc_tab', ck_q: true, sid: sid, collection: collection, block: block, arrow_icon: arrow_icon, use_link_vendor: use_link_vendor -%}
          </div>
          {%- for block in section.blocks offset: 1 -%}
          {%- assign collection = collections[block.settings.collection] -%}
            <div id="t4s-{{ block.id }}" class="t4s-tab-content2 lazyloadt4s" data-t4s-tab-content data-render-lazy-component data-rendert4s="{{ routes.search_url }}?type=article&q={{ forloop.index }}_bid+ntt4tag" data-set4surl='&section_id={{ sid }}' data-t4splitlz><div class="lds_bginfinity t4s-pr"></div></div>
          {%- endfor -%}
        {%- endif -%}
      </div>
    </div>
  </div>
  {{- html_layout[1] -}}
</div>
{%- endif -%}
{%- # theme-check-enable NestedSnippet, DeprecateBgsizes -%}

