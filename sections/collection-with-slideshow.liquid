<!-- collection-with-slideshow.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-collection-products.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
{{ 't4s-slideshow.css' | asset_url | stylesheet_tag }}
{{ 't4s-slider-settings.css' | asset_url | stylesheet_tag }}
{{ 'pre_flickityt4s.min.css' | asset_url | stylesheet_tag }}
{{ 'pre_flickity_custom.css' | asset_url | stylesheet_tag }}
<link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
<link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
<link href="{{ 't4s-loading.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
  assign sid = section.id
  assign stt_layout = section.settings.layout
  assign stt_image_bg = section.settings.image_bg

  if stt_layout == 't4s-se-container' 
   assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
  elsif stt_layout == 't4s-container-wrap'
    assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif
  assign arr_item = section.blocks | where: "type", 'image_parent'
  assign image_ratio = section.settings.image_ratio
  if image_ratio == "ratioadapt"
    assign imgatt = ''
   else 
    assign imgatt = 'data-'
  endif
  assign collection = collections[section.settings.collection]
  assign show_vendor = section.settings.show_vendor
  assign use_link_vendor = settings.use_link_vendor

  assign enable_rating = settings.enable_rating
  assign limit = section.settings.limit
  assign product_des = section.settings.product_des

  assign show_img = settings.show_img
  assign isGrowaveWishlist = false
  if settings.wishlist_mode == "3" and shop.customer_accounts_enabled
    assign isGrowaveWishlist = true
  endif
  assign placeholder_img = settings.placeholder_img
  assign enable_pr_size = settings.enable_pr_size
  assign show_size_type = settings.show_size_type
  assign size_ck = settings.size_ck | append: ',size,sizes,Größe' 
  assign get_size = size_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

  assign enable_pr_color = settings.enable_pr_color
  assign show_cl_type = settings.show_color_type
  assign color_ck = settings.color_ck | append: ',color,colors,couleur,colour'
  assign get_color = color_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

  assign price_varies_style = settings.price_varies_style
  assign app_review = settings.app_review
  assign use_countdown = section.settings.use_cdt
  
  if section.settings.btn_owl_ss == "simple"
    assign arrow_icon_ss = 1
  else
    assign arrow_icon_ss = 2
  endif
  assign show_btn_ss = section.settings.show_btn_ss
  if show_btn_ss != "hidden"
    assign slider_btns_ss = true
  else
    assign slider_btns_ss = false
  endif
  assign show_dots_ss = section.settings.show_dots_ss
  if show_dots_ss != "hidden"
    assign slider_dots_ss = true
  else
    assign slider_dots_ss = false
  endif

  if section.settings.btn_owl == "simple"
    assign arrow_icon = 1
  else
    assign arrow_icon = 2
  endif
  assign show_btn = section.settings.show_btn
  if show_btn != "hidden"
    assign slider_btns = true
  else
    assign slider_btns = false
  endif
  assign show_dots = section.settings.show_dots
  if show_dots != "hidden"
    assign slider_dots = true
  else
    assign slider_dots = false
  endif
  assign nav_btn_icon = section.settings.nav_btn_icon
  assign navUI = false
  if show_btn != "hidden" and nav_btn_icon != "default" 
    assign navUI = true
  endif 
  if show_btn != "hidden"
    assign navUI = true
  endif 
  assign countdown = false
  assign general_block = false
  assign no_block_parent = true

 -%}

{%- capture data_flickityt4s -%}
{ "cellAlign": "center","freeScroll": 0, "contain" : 1,"dragThreshold" : 15,"percentPosition": 1,"imagesLoaded": 0,"lazyload": 0, "arrowIcon": "{{ arrow_icon_ss }}", "t4sid": "{{ sid }}", "prevNextButtons": {{ slider_btns_ss }},"navUI": {{ navUI }}, "pageDots": {{ slider_dots_ss }},"pauseAutoPlayOnHover": {{ section.settings.au_hover_ss }},"wrapAround": {{ section.settings.loop_ss }},"autoPlay": {{ section.settings.au_time_ss | times: 1000 }},"adaptiveHeight": {% if section.settings.se_height != 't4s_ratio_fh' or section.settings.se_height != 't4s_ratioadapt_f_mix' %}true{% else %}false{% endif %} }
{%- endcapture -%}
{%- capture append_style -%}
  --aspect-ratio-cus : {{ section.settings.height_dk }}px;--aspect-ratio-custb : {{ section.settings.height_tb }}px;--aspect-ratio-cusmb : {{ section.settings.height_mb }}px;
{%- endcapture -%}
{%- capture slideshow_html -%}
  {%- if section.blocks.size > 0 -%}
    {%- for block in section.blocks %}{% assign block_type = block.type -%}
      {%- if block_type == 'image_parent' -%}
        {%- liquid
          unless no_block_parent
            echo '</div></div></div></div>'
          endunless
          assign no_block_parent = false
          assign isRunLoop = true
          if section.settings.se_height == 't4s_ratioadapt_mix'
            assign attr_img = ''
          elsif section.settings.se_height == 't4s_ratioadapt_f_mix'
            assign attr_img = ''
          else 
            assign attr_img = 'data-'
          endif 
          assign image_mb = block.settings.image_mb
          assign image = block.settings.image | default: image_mb
          assign mb_image = image_mb | default: image
          if isRunLoop
            assign ratio = image.aspect_ratio 
            assign ratiomb = mb_image.aspect_ratio 
          endif 
          assign url = block.settings.link_img1 
          assign open_link = block.settings.open_link
          assign bg_content_op = block.settings.bg_content_op | divided_by: 100.0 
          assign bg_content = block.settings.bg_content_cl | color_modify: 'alpha', bg_content_op 
          assign bg_opacity = block.settings.bg_opacity | divided_by: 100.0
          assign bg_overlay = block.settings.bg_overlay | color_modify: 'alpha', bg_opacity
          assign br_opacity = block.settings.br_opacity | divided_by: 100.0
          assign br_bg = block.settings.br_bg | color_modify: 'alpha', br_opacity  
          assign ani_delay = 0   
          assign percent_delay = block.settings.animation_delay | divided_by: 100.0
          assign time_ani_delay = block.settings.time_animation | times: percent_delay     
          assign cl_img = block.settings.cl_img
          if forloop.first
            assign img_url_w     = image | image_url: width: 1904 | split: '1904'
            assign img_url_w0    = img_url_w[0]
            assign img_url_w1    = img_url_w[1]
            assign img_url_w_mb  = mb_image | image_url: width: 1904 | split: '1904'
            assign img_url_w_mb0 = img_url_w_mb[0]
            assign img_url_w_mb1 = img_url_w_mb[1]
            assign img_1         = img_url_w0 | append: 1 | append: img_url_w1
            assign img_1_mb      = img_url_w_mb0 | append: 1 | append: img_url_w_mb1
          else
            assign img_1    = image | image_url: width: 1
            assign img_1_mb = mb_image | image_url: width: 1
          endif
          if cl_img != blank and cl_img != 'rgba(0,0,0,0)'
            assign url_img = cl_img
            assign url_img_mb = cl_img
          else
            assign url_img = 'url(' | append: img_1 | append: ')'
            assign url_img_mb = 'url(' | append: img_1_mb | append: ')'
          endif
        -%}
        <div class="t4s-slideshow-item t4s-col-item t4s-slide t4s_position_{{ block.settings.image_position }}" id="b_{{ block.id }}" data-select-flickity {{ block.shopify_attributes }}>
          <div class="t4s-slideshow-inner t4s-pr t4s-oh t4s_ratio t4s_ratio_hasmb" {{ attr_img }}style="--ratioapt: {{ ratio | default: 1.7777 }};--ratioaptmb: {{ ratiomb | default: 1.7777 }};">
            <a href="{{ url }}" target="{{ open_link }}" class="t4s-full-width-link{% if url == blank %} t4s-pe-none {% else %} t4s-pe-auto{% endif %}" style="--bg-overlay: {{ bg_overlay }};"></a>
            {%- if image != blank -%}   
              {%- if forloop.first -%}
                <img {% if mb_image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{mb_image.presentation.focal_point}}"{% endif %} class="t4s-img-as-bg t4s-d-md-none t4s-slide__{{ block.settings.animate_slide }} lazyloadt4sNative t4s-lz--fadeIn"
                srcset="{{ img_url_w_mb0 }}375{{ img_url_w_mb1 }} 375w, {{ img_url_w_mb0 }}550{{ img_url_w_mb1 }} 550w, {{ img_url_w_mb0 }}750{{ img_url_w_mb1 }} 750w, {{ img_url_w_mb0 }}1100{{ img_url_w_mb1 }} 1100w, {{ img_url_w_mb0 }}1500{{ img_url_w_mb1 }} 1500w, {{ img_url_w_mb0 }}1780{{ img_url_w_mb1 }} 1780w, {{ img_url_w_mb0 }}2000{{ img_url_w_mb1 }} 2000w, {{ img_url_w_mb0 }}3000{{ img_url_w_mb1 }} 3000w,{{ img_url_w_mb0 }}3840{{ img_url_w_mb1 }} 3840w, {{ mb_image | image_url }} {{ mb_image.width }}w"
                sizes="(min-width: 768px) 768px,100vw" src="{{ img_url_w_mb0 }}1500{{ img_url_w_mb1 }}"
                loading="lazy" onload="loadImageT4s(this)" alt="{{ mb_image.alt | escape }}" width="{{ mb_image.width }}" height="{{ mb_image.height }}">
                <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none" style="background: {{ url_img_mb }}"></span>
                <img {% if image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{image.presentation.focal_point}}"{% endif %} class="t4s-img-as-bg t4s-d-none t4s-d-md-block t4s-slide__{{ block.settings.animate_slide }} lazyloadt4sNative t4s-lz--fadeIn"
                srcset="{{ img_url_w0 }}375{{ img_url_w1 }} 375w, {{ img_url_w0 }}550{{ img_url_w1 }} 550w, {{ img_url_w0 }}750{{ img_url_w1 }} 750w, {{ img_url_w0 }}1100{{ img_url_w1 }} 1100w, {{ img_url_w0 }}1500{{ img_url_w1 }} 1500w, {{ img_url_w0 }}1780{{ img_url_w1 }} 1780w, {{ img_url_w0 }}2000{{ img_url_w1 }} 2000w, {{ img_url_w0 }}3000{{ img_url_w1 }} 3000w,{{ img_url_w0 }}3840{{ img_url_w1 }} 3840w, {{ image | image_url }} {{ image.width }}w" src="{{ img_url_w0 }}1500{{ img_url_w1 }}" loading="lazy" onload="loadImageT4s(this)" alt="{{ image.alt | escape }}" width="{{ image.width }}" height="{{ image.height }}">
                <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: {{ url_img }}"></span>
              {%- else -%}
                <img {% if mb_image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{mb_image.presentation.focal_point}}"{% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-img-as-bg t4s-d-md-none t4s-slide__{{ block.settings.animate_slide }}" data-src="{{ img_1_mb }}" data-widths="[375, 575, 750]" data-optimumx="1.5" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ mb_image.width }}" height="{{ mb_image.height }}" alt="{{ mb_image.alt | escape }}" loading="lazy">
                <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none" style="background: {{ url_img_mb }}"></span>
                <img {% if image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{image.presentation.focal_point}}"{% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-img-as-bg t4s-d-none t4s-d-md-block t4s-slide__{{ block.settings.animate_slide }}" data-src="{{ img_1 }}" data-widths="[800, 1000, 1200, 1400, 1600, 1800, 2000, 2200, 2500, 3000, 3400, 3800, 4100]" data-optimumx="1.5" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: {{ url_img }}"></span>
              {%- endif -%}
            {%- else -%} 
              {%- capture current -%}{% cycle 1, 2 %}{%- endcapture -%}
                {{ 'lifestyle-' | append: current | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1' }}  
              {%- endif -%}
              {%- capture append_bg_content_style -%}--bg-content: {{ bg_content }};--content-pd: {{ block.settings.content_pd_tb }}px {{ block.settings.content_pd_lr }}px;--content-pd-mb: {{ block.settings.content_pd_tb_mb }}px {{ block.settings.content_pd_lr_mb }}px;{%- endcapture -%}
              {%- if block.settings.border_bl -%}
                {%- capture append_bg_border_style -%}--br-color: {{ block.settings.br_color }};--br-style: {{ block.settings.br_style }};--br-pd: {{ block.settings.br_pd }}px;--br-pd-mb: {{ block.settings.br_pd_mb }}px;--border-bg: {{ br_bg }};{%- endcapture -%}
              {%- endif -%}
              <div class="t4s-content-wrap t4s-pe-none t4s-full-width-link t4s-z-100">
                <div class="t4s-content-position t4s-{{ block.settings.content_width }} t4s-pa t4s-text-lg-{{ block.settings.text_align }} t4s-text-md-{{ block.settings.text_align_tb }} t4s-text-{{ block.settings.text_align_mb }} t4s-bg-content-true t4s-br-content-{{ block.settings.border_bl }} t4s-br-style-{{ block.settings.br_style }}" style="--time-animation: {{ block.settings.time_animation }}s;{%- render 'content_position', ch_pos: block.settings.ch_pos, cv_pos: block.settings.cv_pos, ch_pos_tb: block.settings.ch_pos_tb, cv_pos_tb: block.settings.cv_pos_tb, ch_pos_mb: block.settings.ch_pos_mb, cv_pos_mb: block.settings.cv_pos_mb, append_bg_content_style: append_bg_content_style, append_bg_border_style: append_bg_border_style -%};">  
            
        {%- else -%}

            {%- liquid
              if no_block_parent
                continue
              endif
            -%}

            {%- case block_type -%}
              {%- when 'custom_text' -%}
                {%- assign general_block = true -%}
                <{{ block.settings.tag }} data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-text-bl t4s-fnt-fm-{{ block.settings.fontf }} t4s-font-italic-{{ block.settings.font_italic }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-hidden-br-mb-{{ block.settings.remove_br_tag }} t4s-text-shadow-{{ block.settings.text_shadow }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_text', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</{{ block.settings.tag }}>
              {%- when 'space_html' -%}
                {%- assign general_block = true -%}
                <div class="t4s-bl-item t4s-space-html t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'space_html', block: block, ani_delay: ani_delay -%}></div>
              {%- when 'html' -%}
                {%- assign general_block = true -%}
                <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-raw-html t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-rte--list" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'html', block: block, ani_delay: ani_delay -%}>{{ block.settings.html_content }}</div>
              {%- when 'image' -%}
                {%- liquid
                  assign image = block.settings.image_child
                  if block.settings.img_link != blank
                    assign image_parent = 'a,' | split: ','
                  else
                    assign image_parent = 'div,data-' | split: ','
                  endif
                -%}
                {%- if image != blank -%}
                  {%- assign general_block = true -%}
                  <{{ image_parent[0] }} {{ image_parent[1] }}href="{{ block.settings.img_link }}" class="t4s-bl-item t4s-img-child t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-pe-auto" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', block: block, ani_delay: ani_delay -%}>
                    <img class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                  </{{- image_parent[0] -}}>
                {%- else -%}
                {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                {%- endif -%}
              {%- when "countdown" -%}
                {%- if block.settings.date != blank -%}
                  {%- assign countdown = true -%}
                  <div class="t4s-bl-item t4s-countdown sepr_coun_dt_wrap t4s-countdown-des-{{ block.settings.cdt_des }} t4s-countdown-size-{{ block.settings.cdt_size }} t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'countdown', block: block, ani_delay: ani_delay -%}>
                    <div class="t4s-time" data-countdown-t4s data-date='{{ block.settings.date }}' data-keyid='#countdown-{{ sid }}'></div>
                  </div>
                {% endif %}
              {%- when 'custom_button' -%}
                {%- if block.settings.product != blank and block.settings.button_text != blank -%}
                  {%- assign button_style = block.settings.button_style -%}
                  <a href="{{ block.settings.product.url }}" target="{{ block.settings.target_link }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-btn t4s-btn-custom t4s-pe-auto t4s-fnt-fm-{{ block.settings.fontf }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline'  or button_style == 'ovic' %}t4s-btn-effect-{{ block.settings.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_button', block: block, ani_delay: ani_delay -%}>  {%- render 'product-price', product: block.settings.product, price_varies_style: price_varies_style, type: 'card' -%}<span>{{ block.settings.button_text }}</span>
                  {%- if block.settings.button_icon_w > 0 -%}
                    {%- if block.settings.icon_style == "1" -%}
                      <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="18" height="18"><path d="M 21.1875 9.28125 L 19.78125 10.71875 L 24.0625 15 L 4 15 L 4 17 L 24.0625 17 L 19.78125 21.28125 L 21.1875 22.71875 L 27.90625 16 Z"/></svg>                                
                    {%- elsif block.settings.icon_style == "2" -%}
                      <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>   
                    {% else %}
                      <svg class="t4s-btn-icon" version="1.1" x="0px" y="0px" width="22px" height="22px" viewBox="0 0 22 22" style="enable-background:new 0 0 22 22;" xml:space="preserve"><polygon points="6,20 6,2 17,11 "></polygon></svg>  
                    {%- endif -%}
                  {%- endif -%}
                  </a>        
                {%- elsif block.settings.button_link != blank and block.settings.button_text != blank -%}
                  {%- assign button_style = block.settings.button_style -%}
                  <a href="{{ block.settings.button_link }}" target="{{ block.settings.target_link }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-btn t4s-btn-custom t4s-pe-auto t4s-fnt-fm-{{ block.settings.fontf }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline'  or button_style == 'ovic' %}t4s-btn-effect-{{ block.settings.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_button', block: block, ani_delay: ani_delay -%}>{{ block.settings.button_text }}
                    {%- if block.settings.button_icon_w > 0 -%}
                      {%- if block.settings.icon_style == "1" -%}
                        <svg  class="t4s-btn-icon" viewBox="0 0 22 22"><use xlink:href="#t4s-icon-btn"></use></svg>
                      {%- elsif block.settings.icon_style == "2" -%}
                        <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>   
                      {% else %}
                        <svg class="t4s-btn-icon" version="1.1" x="0px" y="0px" width="22px" height="22px" viewBox="0 0 22 22" style="enable-background:new 0 0 22 22;" xml:space="preserve"><polygon points="6,20 6,2 17,11 "></polygon></svg>  
                      {%- endif -%}
                    {%- endif -%}
                  </a>
                {%- endif -%}
              {%- when 'custom_label' -%}
                {% if block.settings.text != blank %}
                {%- assign general_block = true -%}
                  <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-custom-label t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_label', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</div>
                {% endif %}
              {%- when 'button_video' -%}
                {%- capture data_options -%}
                  {"type": "{%- if block.settings.source == "1" -%}{{ block.settings.video_url.type }}{%-else -%}html5{%- endif -%}", "vid": "{{ block.settings.video_url.id }}", "autoplay": {{ block.settings.au_video }}, "loop": {{ block.settings.loop_video }}, "accent_color": "{{ settings.accent_color | remove_first: '#' }}","srcDefault": "https://cdn.shopify.com/s/files/1/0610/5209/2628/files/Share_your_brand_story_by_adding_a_video_to_your_store.mp4?v=1641630446", "id":"#tmp-video-{{ block.id }}" }
                {%- endcapture -%}
