<!-- sections/hero-image.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
{{ 't4s-hero.css' | asset_url | stylesheet_tag }}
{%-liquid
	assign sid = section.id
  assign stt_layout = section.settings.layout
  assign image_mb = section.settings.image_mb
  assign image = section.settings.image | default: image_mb
  assign mb_image = image_mb | default: image 
  assign IsParallax = section.settings.parallax
  if image == blank
      assign IsParallax = false
  endif

	assign url = section.settings.link_img1 
  assign open_link = section.settings.open_link
  assign bg_content_op = section.settings.bg_content_op | divided_by: 100.0 
  assign bg_content = section.settings.bg_content_cl | color_modify: 'alpha', bg_content_op 

  assign bg_opacity = section.settings.bg_opacity | divided_by: 100.0 
  assign bg_overlay = section.settings.bg_overlay | color_modify: 'alpha', bg_opacity 
  
  assign br_opacity = section.settings.br_opacity | divided_by: 100.0
  assign br_bg = section.settings.br_bg | color_modify: 'alpha', br_opacity 

  assign ani_delay = 0   
  assign percent_delay = section.settings.animation_delay | divided_by: 100.0
  assign time_ani_delay = section.settings.time_animation | times: percent_delay

  if stt_layout == 't4s-container-wrap' 
      assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
      assign size_img = '(min-width: 1800px) 1800px, 100vw'
  else
      assign html_layout = '__' | split: '__'
      assign size_img = '100vw'
  endif 

  assign countdown = false
  assign custom_button = false
  assign use_animation = false
  assign general_block = false
-%}
{%- capture append_style -%}
  --aspect-ratio-cus:{{ section.settings.height_dk }}px;--aspect-ratio-custb:{{ section.settings.height_tb }}px;--aspect-ratio-cusmb:{{ section.settings.height_mb }}px;
{%- endcapture -%}
{%- capture append_bg_content_style -%}--bg-content:{{ bg_content }};--content-pd:{{ section.settings.content_pd_tb }}px {{ section.settings.content_pd_lr }}px;--content-pd-tb:{{ section.settings.content_pd_tb_tb }}px {{ section.settings.content_pd_lr_tb }}px;--content-pd-mb:{{ section.settings.content_pd_tb_mb }}px {{ section.settings.content_pd_lr_mb }}px;{%- endcapture -%}
{%- if section.settings.border_bl -%}
  {%- capture append_bg_border_style -%}--br-color:{{ section.settings.br_color }};--br-style:{{ section.settings.br_style }};--br-pd:{{ section.settings.br_pd }}px;--br-pd-tb:{{ section.settings.br_pd_tb }}px;--br-pd-mb:{{ section.settings.br_pd_mb }}px;--border-bg:{{ br_bg }};{%- endcapture -%}
{%- endif -%}
{% render 'section_style', sid: sid, append_style: append_style -%}
<div class="t4s-section-inner t4s-pr t4s_se_{{ sid }} {{ stt_layout }}">
  {{- html_layout[0] -}}
  <div class="t4s-pr t4s-oh">
    <div data-t4s-animate class="t4s-hero-image t4s-row t4s-row-cols-1 t4s-gx-0 t4s_ratio_mix {{ section.settings.se_height }} t4s_ratio_cuspx_mb_{{ section.settings.custom_mb }} t4s_ratio_cuspx_tb_{{ section.settings.custom_tb }} t4s_ratio_cuspx_{{ section.settings.custom_dk }}">
      <div class="t4s-col-item t4s-hero-image-item">
        <div data-parallax-t4s{{ IsParallax }} class="{% if IsParallax %}t4s-parallax {% endif %}t4s-hero-inner t4s-pr t4s-oh t4s_cover t4s_ratio" style="--ratioapt: {{ image.aspect_ratio | default: 1.7777 }};--ratioaptmb: {{ mb_image.aspect_ratio | default: 1.7777 }};">
          {%- if image != blank -%}
            {%- liquid
              assign img_url_w     = image | image_url: width: 1904 | split: '1904'
              assign img_url_w0    = img_url_w[0]
              assign img_url_w1    = img_url_w[1]
              assign img_url_w_mb  = mb_image | image_url: width: 1904 | split: '1904'
              assign img_url_w_mb0 = img_url_w_mb[0]
              assign img_url_w_mb1 = img_url_w_mb[1] 
              assign img_1         = img_url_w0 | append: 1 | append: img_url_w1
              assign img_1_mb      = img_url_w_mb0 | append: 1 | append: img_url_w_mb1
              assign url_img       = 'url(' | append: img_1 | append: ')'
              assign url_img_mb    = 'url(' | append: img_1_mb | append: ')'
            -%}
            <img {% if mb_image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{ mb_image.presentation.focal_point }};"{% endif %} class="t4s-img-as-bg t4s-d-md-none lazyloadt4sNative t4s-lz--fadeIn t4s-parallax-img" srcset="{{ img_url_w_mb0 }}375{{ img_url_w_mb1 }} 375w, {{ img_url_w_mb0 }}550{{ img_url_w_mb1 }} 550w, {{ img_url_w_mb0 }}750{{ img_url_w_mb1 }} 750w, {{ img_url_w_mb0 }}1100{{ img_url_w_mb1 }} 1100w, {{ img_url_w_mb0 }}1500{{ img_url_w_mb1 }} 1500w, {{ img_url_w_mb0 }}1780{{ img_url_w_mb1 }} 1780w, {{ img_url_w_mb0 }}2000{{ img_url_w_mb1 }} 2000w, {{ img_url_w_mb0 }}3000{{ img_url_w_mb1 }} 3000w,{{ img_url_w_mb0 }}3840{{ img_url_w_mb1 }} 3840w, {{ mb_image | image_url }} {{ mb_image.width }}w" sizes="(min-width: 768pxpx) 768px,100vw" src="{{ img_url_w_mb0 }}1500{{ img_url_w_mb1 }}" loading="lazy" onload="loadImageT4s(this)" alt="{{ mb_image.alt | escape }}" width="{{ mb_image.width }}" height="{{ mb_image.height }}">
            <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none"style="background: {{ url_img_mb }}"></span>
            <img {% if image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{ image.presentation.focal_point }};"{% endif %} class="t4s-img-as-bg t4s-d-none t4s-d-md-block lazyloadt4sNative t4s-lz--fadeIn t4s-parallax-img" srcset="{{ img_url_w0 }}375{{ img_url_w1 }} 375w, {{ img_url_w0 }}550{{ img_url_w1 }} 550w, {{ img_url_w0 }}750{{ img_url_w1 }} 750w, {{ img_url_w0 }}1100{{ img_url_w1 }} 1100w, {{ img_url_w0 }}1500{{ img_url_w1 }} 1500w, {{ img_url_w0 }}1780{{ img_url_w1 }} 1780w, {{ img_url_w0 }}2000{{ img_url_w1 }} 2000w, {{ img_url_w0 }}3000{{ img_url_w1 }} 3000w,{{ img_url_w0 }}3840{{ img_url_w1 }} 3840w, {{ image | image_url }} {{ image.width }}w" sizes="{{ size_img }}" src="{{ img_url_w0 }}1500{{ img_url_w1 }}" loading="lazy" onload="loadImageT4s(this)" alt="{{ image.alt | escape }}" width="{{ image.width }}" height="{{ image.height }}">
            <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: {{ url_img }}"></span>
          {%- else -%}
            {{ 'lifestyle-1' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1' }}
          {%- endif -%}
          <div class="t4s-content-wrap t4s-bg-cl-wrap t4s-pe-none t4s-full-width-link t4s-z-100">
            <div class="t4s-content-position t4s-{{ section.settings.content_width }} t4s-pa t4s-bg-content-true t4s-br-content-{{ section.settings.border_bl }} t4s-br-style-{{ section.settings.br_style }} t4s-text-lg-{{ section.settings.text_align }} t4s-text-md-{{ section.settings.text_align_tb }} t4s-text-{{ section.settings.text_align_mb }}" style="--time-animation: {{ section.settings.time_animation }}s;{%- render 'content_position', ch_pos: section.settings.ch_pos, cv_pos: section.settings.cv_pos, ch_pos_tb: section.settings.ch_pos_tb, cv_pos_tb: section.settings.cv_pos_tb, ch_pos_mb: section.settings.ch_pos_mb, cv_pos_mb: section.settings.cv_pos_mb, append_bg_content_style: append_bg_content_style, append_bg_border_style: append_bg_border_style -%}">
              {%- for block in section.blocks -%}
                {%- case block.type -%}
                  {%- when 'custom_text' -%}
                    {%- assign general_block = true -%}
                    {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%}{%- endif -%}
                    <{{ block.settings.tag }} class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-text-bl t4s-fnt-fm-{{ block.settings.fontf }} t4s-font-italic-{{ block.settings.font_italic }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-hidden-br-mb-{{ block.settings.remove_br_tag }} t4s-text-shadow-{{ block.settings.text_shadow }}" data-lh="{{ block.settings.text_lh_mb }}" data-lh-md="{{ block.settings.text_lh_tb }}" data-lh-lg="{{ block.settings.text_lh }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_text', block: block, ani_delay: ani_delay -%}>{{ block.settings.text }}</{{ block.settings.tag }}>
                  {%- when 'space_html' -%}
                    {%- assign general_block = true -%}
                    {%- if block.settings.animation != 'none' -%}{%- assign use_animation = true -%}{%- endif -%}
                  <div class="t4s-bl-item t4s-space-html t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'space_html', block: block, ani_delay: ani_delay -%}></div>
                  {%- when 'html' -%}
                    {%- if block.settings.html_content != blank -%}
                      {%- assign general_block = true -%}
                      {%- if block.settings.animation != 'none' -%}
                        {%- assign use_animation = true -%}
                      {%- endif -%}
                      <div class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-raw-html t4s-rte--list t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'html', block: block, ani_delay: ani_delay -%}>{{ block.settings.html_content }}</div>
                    {%- endif -%}
                  {%- when 'image' -%}
                    {%- liquid
                      assign image = block.settings.image_child 
                      if block.settings.img_link != blank 
                        assign image_parent = 'a,' | split: ','
                      else 
                        assign image_parent = 'div,data-' | split: ','
                      endif 
                    -%}
                    {%- if image != blank -%} 
                      {%- assign general_block = true -%} 
                      {%- if block.settings.animation != 'none' -%} {%- assign use_animation = true -%} {%- endif -%} 
                      <div class="t4s-bl-item t4s-img-child t4s-animation-{{ block.settings.animation }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', block: block, ani_delay: ani_delay -%}>
                        <img data-maxw="{{ block.settings.img_width_mb }}" data-maxw-md="{{ block.settings.img_width_tb }}" data-maxw-lg="{{ block.settings.img_width }}" class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                      </div> 
                    {%- endif -%}
                  {%- when 'custom_button' -%}
                    {%- if block.settings.button_link != blank and block.settings.button_text != blank -%}
                      {%- assign custom_button = true -%}
                      {%- assign  button_style = block.settings.button_style -%}
                      <a href="{{ block.settings.button_link }}" target="{{ block.settings.target_link }}" class="t4s-bl-item t4s-animation-{{ block.settings.animation }} t4s-btn t4s-btn-custom  t4s-pe-auto t4s-fnt-fm-{{ block.settings.fontf }} t4s-hidden-mobile-{{ block.settings.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' %}t4s-btn-effect-{{ block.settings.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_button', block: block, ani_delay: ani_delay -%}>{{ block.settings.button_text }}
                      {%- if block.settings.button_icon_w > 0 -%}
                        {%- if block.settings.icon_style == "1" -%}
                          {% comment %} <svg class="t4s-btn-icon"  width="16" height="11" viewBox="0 0 16 11" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <rect y="4" width="12" height="3" />
                            <path d="M15.0434 5.50985L11.3245 9.44864L11.2626 1.63039L15.0434 5.50985Z"/>
                          </svg> {% endcomment %}
                          <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="18" height="18"><path d="M 21.1875 9.28125 L 19.78125 10.71875 L 24.0625 15 L 4 15 L 4 17 L 24.0625 17 L 19.78125 21.28125 L 21.1875 22.71875 L 27.90625 16 Z"/></svg>
                            
                        {%- elsif block.settings.icon_style == "2" -%}
                            <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>   
                        {% else %}
                          <svg class="t4s-btn-icon" version="1.1" x="0px" y="0px" width="22px" height="22px" viewBox="0 0 22 22" style="enable-background:new 0 0 22 22;" xml:space="preserve"><polygon points="6,20 6,2 17,11 "></polygon></svg>  
                        {%- endif -%}
                    {%- endif -%}
                     </a>
                    {%- endif -%}
                {%- endcase -%}
                {%- if block.settings.animation != 'none' %}{% assign ani_delay = ani_delay | plus: time_ani_delay %}{% endif -%}
              {%- endfor -%}
            </div>
          </div>
          <a href="{{ url }}" target="{{ open_link }}" class="t4s-full-width-link{% if url == blank %} t4s-pe-none {% else %} t4s-pe-auto{% endif %}" style="--bg-overlay: {{ bg_overlay }};"></a>
        </div>
      </div>
    </div>
  </div>
  {{- html_layout[1] -}}
</div>
{%- if general_block -%}
  {{ 't4s-general-block.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- if custom_button -%}
  {{ 't4s-button-style.css' | asset_url | stylesheet_tag }}
  <link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- endif -%}
{%- if use_animation -%}
  <link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- endif -%}
{%- if countdown -%}
  {{ 't4s-countdown.css' | asset_url | stylesheet_tag }}
  <template id="countdown-{{ sid }}">
    <span class="t4s-countdown-days">
      <span class="cd_timet4s t4s-cd-number">%-D</span>
      <span class="cd_txtt4s t4s-cd-text">%!D:{{ "sections.countdown_text.day" | t }},{{ "sections.countdown_text.day_plural" | t }};</span>
    </span>
    <span class="t4s-countdown-hours">
      <span class="cd_timet4s t4s-cd-number">%H</span> 
      <span class="cd_txtt4s t4s-cd-text">%!H:{{ "sections.countdown_text.hr" | t }},{{ "sections.countdown_text.hr_plural" | t }};</span>
    </span>
    <span class="t4s-countdown-min">
      <span class="cd_timet4s t4s-cd-number">%M</span> 
      <span class="cd_txtt4s t4s-cd-text">%!M:{{ "sections.countdown_text.min" | t }},{{ "sections.countdown_text.min_plural" | t }};</span>
    </span>
    <span class="t4s-countdown-sec">
      <span class="cd_timet4s t4s-cd-number">%S</span> 
      <span class="cd_txtt4s t4s-cd-text">%!S:{{ "sections.countdown_text.sec" | t }},{{ "sections.countdown_text.sec_plural" | t }};</span>
    </span> 
  </template>
{%- endif -%}
{%- if IsParallax -%}
  {{ 't4s-parallax.min.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{% schema %}
{
  "name": "Hero image",
  "tag": "section",
  "class": "t4s-section t4s-section-all t4s_tp_parallax t4s_tp_cd",
  "disabled_on": {
    "groups": [
      "*"
    ]
  },
  "settings": [
    {
      "type": "header",
      "content": "1. General options"
    },
    {
      "type": "select",
      "id": "se_height",
      "label": "Section height",
      "default": "t4s_ratioadapt_mix",
      "options": [
        {
          "value": "t4s_ratio_fh",
          "label": "Full screen height"
        },
        {
          "value": "t4s_ratioadapt_mix",
          "label": "Adapt to image"
        }
      ]
    },
    {
      "type": "checkbox",
      "id": "custom_dk",
      "label": "Use custom height (Desktop)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_dk",
      "label": "Section height (Desktop)",
      "default": 600
    },
    {
      "type": "checkbox",
      "id": "custom_tb",
      "label": "Use custom height (Tablet)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_tb",
      "label": "Section height (Tablet)",
      "default": 400
    },
    {
      "type": "checkbox",
      "id": "custom_mb",
      "label": "Use custom height (Mobile)",
      "default": true
    },
    {
      "type": "number",
      "id": "height_mb",
      "label": "Section height (Mobile)",
      "default": 250
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Choose Image",
      "info": "Not support focal point when use parallax"
    },
    {
      "type": "image_picker",
      "id": "image_mb",
      "label": "Mobile image (optional)",
      "info": "If none is set, desktop image will be used. Not support focal point when use parallax"
    },
    {
      "type": "checkbox",
      "id": "parallax",
      "label": "Enable parallax scroll",
      "default": false
    },
    {
      "type": "url",
      "id": "link_img1",
      "label": "Link image",
      "info": "The whole image becomes clickable."
    },
    {
      "type": "select",
      "id": "open_link",
      "label": "Open link in",
      "default": "_blank",
      "options": [
        {
          "value": "_self",
          "label": "Current window"
        },
        {
          "value": "_blank",
          "label": "New window"
        }
      ]
    },
    {
      "type": "header",
      "content": "+ Content position"
    },
    {
      "type": "select",
      "id": "text_align",
      "label": "Content align",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "select",
      "id": "text_align_tb",
      "label": "Content align (Tablet)",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "select",
      "id": "text_align_mb",
      "label": "Content align (Mobile)",
      "default": "center",
      "options": [
        {
          "label": "Left",
          "value": "start"
        },
        {
          "label": "Center",
          "value": "center"
        },
        {
          "label": "Right",
          "value": "end"
        }
      ]
    },
    {
      "type": "select",
      "id": "content_width",
      "label": "Content width",
      "default": "container",
      "options": [
        {
          "label": "Auto",
          "value": "auto"
        },
        {
          "label": "Container",
          "value": "container"
        }
      ]
    },
    {
      "type": "header",
      "content": "--Content position options--"
    },
    {
      "type": "paragraph",
      "content": "Warning: \"Content horizontal position\" options doesn't work when using \"Content width\" is 'Container'"
    },
    {
      "type": "range",
      "id": "cv_pos",
      "label": "Content vertical position",
      "info": " <= 50: Top position , > 50 bottom position",
      "max": 100,
      "min": 0,
      "step": 1,
      "unit": "%",
      "default": 50
    },
    {
      "type": "range",
      "id": "ch_pos",
      "label": "Content horizontal position",
      "info": " <= 50: Left position , > 50 right position",
      "max": 100,
      "min": 0,
      "step": 1,
      "unit": "%",
      "default": 50
    },
    {
      "type": "header",
      "content": "--Content position options (Tablet)--"
    },
    {
      "type": "range",
      "id": "cv_pos_tb",
      "label": "Content vertical position",
      "info": " <= 50: Top position , > 50 bottom position",
      "max": 100,
      "min": 0,
      "step": 1,
      "unit": "%",
      "default": 50
    },
    {
      "type": "range",
      "id": "ch_pos_tb",
      "label": "Content horizontal position",
      "info": " <= 50: Left position , > 50 right position",
      "max": 100,
      "min": 0,
      "step": 1,
      "unit": "%",
      "default": 50
    },
    {
      "type": "header",
      "content": "--Content position options (Mobile)--"
    },
    {
      "type": "range",
      "id": "cv_pos_mb",
      "label": "Content vertical position",
      "info": " <= 50: Top position , > 50 bottom position",
      "max": 100,
      "min": 0,
      "step": 1,
      "unit": "%",
      "default": 50
    },
    {
      "type": "range",
      "id": "ch_pos_mb",
      "label": "Content horizontal position",
      "info": " <= 50: Left position , > 50 right position",
      "max": 100,
      "min": 0,
      "step": 1,
      "unit": "%",
      "default": 50
    },
    {
      "type": "header",
      "content": "+ Content background, color options"
    },
    {
      "type": "color",
      "id": "bg_overlay",
      "label": "Overlay",
      "default": "#000"
    },
    {
      "type": "range",
      "id": "bg_opacity",
      "label": "Overlay opacity",
      "default": 0,
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "%"
    },
    {
